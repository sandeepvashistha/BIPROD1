  <DataSets>
    <DataSet Name="InventoryAgeByCompanyIL">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
         b.uni_company_name
       , b.[Location Code]
       , b.[Item No_]
	   ,f.[Description]
	   ,(case f.[OE Transactable] when 0 then 'No' when 1 then 'Yes' end) as [Sellable Item]
	   ,f.[Standard Cost]
       , sum(Quantity) as [Original Quantity]
       , sum([Remaining Quantity]) as [Remaining Quantity]
       , avg(inv_age) as avg_inv_age
       , sum([Remaining Quantity]*inv_age)/sum([Remaining Quantity]) as weighted_inv_age
from
(
       select 
               uni_company_name
             , [Location Code]
             , [Item No_]
             , Quantity
             , [Remaining Quantity]
             , [Creation Date]
             , datediff(day,[Creation Date], getdate()) as inv_age
       from prod_navision.dbo.item_ledger_entry_nc_vw a 
       where uni_company_name in (@CompanyName) and [Remaining Quantity] &gt; 0
) b
inner join prod_navision.dbo.item_nc_vw f on b.uni_company_name = f.uni_company_name and b.[Item No_] = f.No_
group by
         b.uni_company_name
       , b.[Location Code]
       , b.[Item No_]
	   ,f.[Description]
	   ,(case f.[OE Transactable] when 0 then 'No' when 1 then 'Yes' end)
	   ,f.[Standard Cost]
order by 
		b.uni_company_name
		,b.[Item No_]
		,b.[Location Code]</CommandText>
      </Query>
      <Fields>
        <Field Name="uni_company_name">
          <DataField>uni_company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_Code">
          <DataField>Location Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No_">
          <DataField>Item No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sellable_Item">
          <DataField>Sellable Item</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Standard_Cost">
          <DataField>Standard Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Original_Quantity">
          <DataField>Original Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Remaining_Quantity">
          <DataField>Remaining Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="avg_inv_age">
          <DataField>avg_inv_age</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="weighted_inv_age">
          <DataField>weighted_inv_age</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
