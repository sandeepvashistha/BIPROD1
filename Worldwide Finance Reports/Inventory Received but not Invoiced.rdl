  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select uni_company_name,[Document No_],No_,[Location Code],[Description],[Type],(case when [Type]=1 then 'G/L Account' when [Type]=2 then 'Item' when [Type]=3 then 'Fixed Asset' when [Type]=4 then 'Charge (Item)' else '' end) [Type Desc],
cast([Order Date] as date) [Order Date],cast([Posting Date] as date) [Posting Date],[Buy-from Vendor No_],
cast([Direct Unit Cost] as numeric(24,4)) [Direct Unit Cost],
cast([Quantity] as numeric(24,4)) [Total Quantity],
--cast([Line Amount] as numeric(24,4)) [Total Line Amount excl Tax],
cast([Qty_ Rcd_ Not Invoiced] as numeric(24,4)) [Qty Rcd Not Invoiced],
cast(([Qty_ Rcd_ Not Invoiced]*[Direct Unit Cost]) as numeric(24,4)) [amount excl Tax Not Invoiced]
 from prod_navision.[dbo].[Purch_Rcpt_Line_nc_vw] 
where uni_company_name in (@CompanyName) and [Qty_ Rcd_ Not Invoiced]&lt;&gt;0
--and [Posting Date] between @StartPostingDate and @EndPostingDate
order by uni_company_name,[Posting Date],[Document No_]</CommandText>
      </Query>
