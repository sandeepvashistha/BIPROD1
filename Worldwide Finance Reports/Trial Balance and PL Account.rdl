    <DataSet Name="PL_Query">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartPostingDate">
            <Value>=Parameters!StartPostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPostingDate">
            <Value>=Parameters!EndPostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select glacc.uni_company_name,glacc.[No_] [g_l account no_],glacc.[Name],b.[Posting Date],b.gl_amount
from
prod_navision.dbo.g_l_account_nc_vw glacc
left outer join
(
select gla.uni_company_name,gl.[g_l account no_],gla.[Name],
cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, gl.[Posting Date]), 0) as date) [Posting Date],
cast(sum(isnull(gl.[Amount],0)) as numeric(24,4)) gl_amount
from  prod_navision.dbo.g_l_account_nc_vw gla 
inner join prod_navision.dbo.g_l_entry_nc_vw gl 
on gl.uni_company_name=gla.uni_company_name and gl.[g_l account no_]=gla.[No_] 
and gl.[Posting Date] between @StartPostingDate and @EndPostingDate
where gla.uni_company_name=@CompanyName
group by
gla.uni_company_name,gl.[g_l account no_],gla.[Name],cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, gl.[Posting Date]), 0) as date)
)b on glacc.uni_company_name=b.uni_company_name and glacc.[No_]=b.[g_l account no_] 
where  glacc.uni_company_name=@CompanyName and glacc.[Account Type] = 0
order by glacc.[No_],b.[Posting Date]</CommandText>
      </Query>
