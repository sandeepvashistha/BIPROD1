  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PostingGroup">
            <Value>=Parameters!PostingGroup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	uni_company_name,
	[Vendor Posting Group],
	cast([Posting Date] as date) [Posting Date],
	[Document Type],
	[Document No_],
	[External Document No_],
	[Vendor No_],
	REPLACE(REPLACE(REPLACE([Description], CHAR(9), ''), CHAR(10), ''), CHAR(13), '') [Description],
	[Currency Code],
	[Adjusted Currency Factor],
	(case when [Document Type]=0 then cast([1099 Amount] as numeric(24,2)) else cast([Purchase (LCY)]*[Adjusted Currency Factor] as numeric(24,2)) end) [Original Amount],
	(case when [Document Type]=0 then cast([1099 Amount]/[Adjusted Currency Factor] as numeric(24,2)) else cast([Purchase (LCY)] as numeric(24,2)) end) [Original Amt. ($)],
	cast([1099 Amount] as numeric(24,4)) [1099 Amount],
	[User ID]
from 
	prod_navision.dbo.vendor_ledger_entry_nc_vw 
where  
	uni_company_name=@CompanyName 
	and [Vendor Posting Group] in (@PostingGroup)
--and [Vendor No_] in ('22599297','188279566')</CommandText>
      </Query>
