  <DataSets>
    <DataSet Name="Shipping_Data">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ship_To_Country">
            <Value>=Parameters!Ship_To_Country.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Start_Ship_Date">
            <Value>=Parameters!Start_Ship_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Ship_Date">
            <Value>=Parameters!End_Ship_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Warehouse">
            <Value>=Parameters!Warehouse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Item_Code">
            <Value>=Parameters!Item_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.Order_number,
  DW.dbo.ODH.VERIFY_DATE,
  DW.dbo.ODH.SHIP_TO_COUNTRY,
  DW.dbo.ODH.SHIP_TO_ZIP,
  DW.dbo.ODL.Item_Code,
  DW.dbo.ODL.Qty_Shipped,
  DW.dbo.ODL.Weight_Unit,
  Sum(DW.dbo.ODL.Weight_Unit*DW.dbo.ODL.Qty_Shipped) as Item_Total_Weight
FROM
  DW.dbo.ODH,
  DW.dbo.ODL
WHERE
  ( DW.dbo.ODH.Morder = DW.dbo.ODL.Morder )
  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  49
   AND
   DW.dbo.ODH.SHIP_TO_COUNTRY  IN  (@Ship_To_Country)     --@Prompt
   AND
   DW.dbo.ODH.VERIFY_DATE  BETWEEN  @Start_Ship_Date  AND  @End_Ship_Date      --@Prompt
   AND
   DW.dbo.ODH.STATUS  IN  (  5  )
   AND
   DW.dbo.ODH.ODH_TYPE  IN  ( 'I','D'  )
   AND
   (
   DW.dbo.ODL.Flag_2 = ' C ' 
   OR
   DW.dbo.ODL.Flag_2 is Null
   )
   AND
   DW.dbo.ODH.WAREHOUSE  IN  (@Warehouse)  --or (@Warehouse is null)    --@Prompt
   AND
   DW.dbo.ODL.Item_Code IN (@Item_Code)    --@Prompt
   --AND
   --DW.dbo.ODH.Order_number = '9036373'
  )
Group by
  DW.dbo.ODH.Order_number,
  DW.dbo.ODH.VERIFY_DATE,
  DW.dbo.ODH.SHIP_TO_COUNTRY,
  DW.dbo.ODH.SHIP_TO_ZIP,
  DW.dbo.ODL.Item_Code,
  DW.dbo.ODL.Qty_Shipped,
  DW.dbo.ODL.Weight_Unit,
  DW.dbo.ODH.Total_Weight</CommandText>
      </Query>
      <Fields>
        <Field Name="Order_number">
          <DataField>Order_number</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="VERIFY_DATE">
          <DataField>VERIFY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SHIP_TO_COUNTRY">
          <DataField>SHIP_TO_COUNTRY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SHIP_TO_ZIP">
          <DataField>SHIP_TO_ZIP</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Code">
          <DataField>Item_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qty_Shipped">
          <DataField>Qty_Shipped</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Weight_Unit">
          <DataField>Weight_Unit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Item_Total_Weight">
          <DataField>Item_Total_Weight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Warehouse">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select 
   Distinct Warehouse 
from 
   ODH
Where
   Uni_Mkting_Unit = '49'
   And
   Warehouse in ('1','2','3','4','5','6','7','8','10','20','30','90')</CommandText>
      </Query>
      <Fields>
        <Field Name="Warehouse">
          <DataField>Warehouse</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Item_Code">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select 
  Distinct Item_Code
from
  ODL
where
  Mkting_Unit = '49'
  and
  (
  flag_2 &lt;&gt; 'M'
  or
  Flag_2 is Null
  )
Order By
  Item_Code</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_Code">
          <DataField>Item_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Country">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select
  Distinct Ship_to_Country
from
  ODH
Where
  Uni_Mkting_Unit = '49'
Order by
  Ship_to_Country</CommandText>
      </Query>
      <Fields>
        <Field Name="Ship_to_Country">
          <DataField>Ship_to_Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
