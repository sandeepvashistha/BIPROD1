  </DataSources>
  <DataSets>
    <DataSet Name="Query_Single_Item">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@countryCode">
            <Value>=Parameters!countryCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@itemCode">
            <Value>=Parameters!itemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
Required prompts:
- CountryCode (@countryCode)
- Item Codes, either all items or given list of items (@itemCode)
*/

SELECT pl.country_code COUNTRY,
       pl.currency_code_1 CURRENCY,
       pl.item_code 'ITEM CODE',
       pl.item_name_1 'ITEM _NAME',
       Sum(CASE WHEN pl.price_level = 1 THEN PL.price_2 END) AS 'PV',
       Sum(CASE WHEN pl.price_level = 2 THEN pl.price_1 END) AS 'NET RETAIL',
       Sum(CASE WHEN pl.price_level = 2 THEN pl.gross_price END) AS 'GROSS RETAIL',
       Sum(CASE WHEN pl.price_level = 2 THEN pl.price_5 END) AS 'MARGIN RETAIL',
       Sum(CASE WHEN pl.price_level = 11 THEN pl.price_1 END) AS 'NET PC',
       Sum(CASE WHEN pl.price_level = 11 THEN pl.gross_price END) AS 'GROSS PC',
       Sum(CASE WHEN pl.price_level = 11 THEN pl.price_5 END) AS 'MARGIN PC',
       Sum(CASE WHEN pl.price_level = 1 THEN pl.price_1 END) AS 'NET WHLS',
       Sum(CASE WHEN pl.price_level = 1 THEN pl.gross_price END) AS 'GROSS WHLS',
       ISNULL(pl.product_code, '') 'PRODUCT CODE',
       pl.tax_rate 'TAX RATE'
  FROM (SELECT COU.country_code,
       COU.currency_code_1,
       IPL.price_level,
       IPL.item_code,
       ITD.item_name_1,
       ipl.price_1,
       ipl.price_2,
       ipl.price_5,
       IPC.product_code,
       CASE
         WHEN IPC.product_code IS NOT NULL THEN Round(IPL.price_1 * ( 100 + TOX.combined_rate ), 2) / 100
         WHEN IPC.product_code IS NULL THEN Round(IPL.price_1 * ( 100 + deftax.combined_rate ), 2) / 100
       END GROSS_PRICE,
       CASE
         WHEN IPC.product_code IS NOT NULL THEN TOX.combined_rate
         WHEN IPC.product_code IS NULL THEN deftax.combined_rate
       END TAX_RATE
  FROM IPL
       INNER JOIN ITD
               ON IPL.mitem = ITD.mitem
                  AND ITD.uni_mkting_unit = 49
       LEFT OUTER JOIN IPC
               ON IPL.item_code = IPC.item_code
                  AND IPC.geo_code = @countryCode
       LEFT OUTER JOIN TOX
               ON IPC.product_code = TOX.product_code
                  AND IPC.geo_code = TOX.geo_code
       LEFT OUTER JOIN (SELECT combined_rate,
                                       geo_code
                                  FROM TOX
                                 WHERE product_code = '*****'
                                       AND uni_mkting_unit = 49) deftax
                    ON deftax.geo_code = @countryCode
       INNER JOIN COU
               ON COU.country_code = @countryCode
                  AND COU.uni_mkting_unit = IPL.mkting_unit
       INNER JOIN (SELECT TOP 1 country_code,
                              ship_to_country
                         FROM ODH
                        WHERE uni_mkting_unit = 49
                              AND ship_to_country = @countryCode
                        ORDER BY entry_date DESC) cc
               ON cc.ship_to_country = @countryCode
 WHERE IPL.mkting_unit = 49
       AND (IPL.item_code IN ( @itemCode ) OR 'All' IN (@itemCode))
       AND ITD.disallow_datatrax = 'N'
       AND IPL.price_level IN ( 1, 2, 11 )
       AND IPL.country_code = cc.country_code
       AND NOT EXISTS (SELECT *
                          FROM IKL
                         WHERE mkting_unit = 49
                               AND IPL.item_code = ikl.item_code)
 GROUP BY COU.currency_code_1,
          COU.country_code,
          IPL.price_level,
          IPL.item_code,
          ITD.item_name_1,
          IPL.price_1,
          IPL.price_2,
          ipl.price_5,
          IPC.product_code,
          TOX.combined_rate,
          deftax.combined_rate) pl
 GROUP BY pl.country_code,
          pl.currency_code_1,
          pl.item_code,
          pl.item_name_1,
          pl.product_code,
          pl.tax_rate
 ORDER BY pl.item_code</CommandText>
      </Query>
