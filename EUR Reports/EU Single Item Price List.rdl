    <DataSet Name="Country_List">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select country_code,(country_code+' - '+country_name) name from dw.dbo.dm_location_manager 
where backoffice_mkting_unit=49 and len(country_code)&lt;=3 and country_name is not null
order by country_code;</CommandText>
      </Query>
      <Fields>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
