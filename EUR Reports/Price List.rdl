  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PriceLevel">
            <Value>=Parameters!PriceLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH ITEMS_CTE (MARKET, COUNTRY, CUR_INDICATOR, ITEM, MITEM, PRICE_LEVEL, ITEM_CATEGORY)
  AS
     (
     SELECT a.UNI_MKTING_UNIT,
            a.SHIP_TO_COUNTRY,
			a.COUNTRY_CODE,
            a.ITEM_CODE,
            a.MITEM,
            a.PRICE_LEVEL,
            IPC.PRODUCT_CODE
       FROM (
            SELECT ODH.UNI_MKTING_UNIT,
                   ODH.SHIP_TO_COUNTRY,
				   ODH.COUNTRY_CODE,
	               ODL.ITEM_CODE,
			       ODL.MITEM,
			       ODL.PRICE_LEVEL
              FROM ODL
                   INNER JOIN ODH
	                ON ODL.MORDER = ODH.MORDER
             WHERE ODH.UNI_MKTING_UNIT = 49
                   AND ODH.VERIFY_DATE &gt;= (SELECT FORMAT(dateadd(year, -1, getdate()), 'yyyy-MM-dd'))
			       AND (ODL.FLAG_2 = 'C' OR ODL.FLAG_2 IS NULL)
             GROUP BY ODH.UNI_MKTING_UNIT,
                      ODH.SHIP_TO_COUNTRY,
					  ODH.COUNTRY_CODE,
	                  ODL.ITEM_CODE,
				      ODL.MITEM,
				      ODL.PRICE_LEVEL
             ) AS a
	         LEFT OUTER JOIN IPC
	          ON a.MITEM = IPC.MITEM AND a.SHIP_TO_COUNTRY = IPC.GEO_CODE
     ),
	 PRICES_CTE (ITEM, NAME, ITEM_PL, MARKET, CUR_INDICATOR, PRICE_LEVEL, NET_PRICE, PV, CV, RETAIL_MARGIN)
 AS
     (
     SELECT IPL.ITEM_CODE,
            ITD.ITEM_NAME_1,
            IPL.MITEM_PRICE_LEVEL,
	        IPL.MKTING_UNIT,
	        IPL.COUNTRY_CODE,
	        IPL.PRICE_LEVEL,
	        IPL.PRICE_1,
	        IPL.PRICE_2,
	        IPL.PRICE_4,
	        IPL.PRICE_5
       FROM IPL
            INNER JOIN ITD
	         ON IPL.MITEM = ITD.MITEM
      WHERE MKTING_UNIT = 49
	        AND IPL.PRICE_LEVEL IN (1, 2, 11)
     ),
	 RATES_CTE (COUNTRY, ITEM_CATEGORY, VAT_RATE)
  AS 
     (
     SELECT GEO_CODE,
            PRODUCT_CODE,
	        COMBINED_RATE
       FROM TOX
      WHERE UNI_MKTING_UNIT = 49
	 ),
	 DEF_RATES_CTE (COUNTRY, VAT_RATE)
 AS (
     SELECT GEO_CODE,
	        COMBINED_RATE
       FROM TOX
      WHERE UNI_MKTING_UNIT = 49
	        AND PRODUCT_CODE = '*****'
     )

SELECT ITEMS_CTE.COUNTRY,
       ITEMS_CTE.ITEM,
	   PRICES_CTE.NAME,
	   CASE
	    WHEN PRICES_CTE.PRICE_LEVEL = 1 THEN 'Wholesale'
		WHEN PRICES_CTE.PRICE_LEVEL = 2 THEN 'Retail'
		WHEN PRICES_CTE.PRICE_LEVEL = 11 THEN 'Preferred Customer'
	   END AS PRICE_LEVEL,
	   CASE
	    WHEN ITEMS_CTE.ITEM_CATEGORY = 'NUT' THEN 'Nutritional'
		WHEN ITEMS_CTE.ITEM_CATEGORY = '*****' THEN 'Standard'
		ELSE ITEMS_CTE.ITEM_CATEGORY
	   END AS ITEM_CATEGORY,
	   PRICES_CTE.PV,
	   PRICES_CTE.CV,
	   PRICES_CTE.RETAIL_MARGIN,
	   PRICES_CTE.NET_PRICE,
	   CASE
	    WHEN ITEMS_CTE.ITEM_CATEGORY IS NULL THEN ROUND(CAST(DEF_RATES_CTE.VAT_RATE AS numeric(12,2)), 2) ELSE ROUND(CAST(RATES_CTE.VAT_RATE AS numeric(12,2)), 2)
       END AS VAT_RATE,
	   CASE 
	    WHEN ITEMS_CTE.ITEM_CATEGORY IS NULL THEN ROUND(CAST(PRICES_CTE.NET_PRICE * (100 + DEF_RATES_CTE.VAT_RATE) / 100 as numeric(12,2)), 2)
		ELSE ROUND(CAST(PRICES_CTE.NET_PRICE * (100 + RATES_CTE.VAT_RATE) / 100 as numeric(12,2)), 2)
	   END AS GROSS_PRICE
  FROM ITEMS_CTE
       INNER JOIN PRICES_CTE
	    ON ITEMS_CTE.ITEM = PRICES_CTE.ITEM AND ITEMS_CTE.CUR_INDICATOR = PRICES_CTE.CUR_INDICATOR
	   LEFT OUTER JOIN RATES_CTE
	    ON ITEMS_CTE.ITEM_CATEGORY = RATES_CTE.ITEM_CATEGORY AND ITEMS_CTE.COUNTRY = RATES_CTE.COUNTRY
	   LEFT OUTER JOIN DEF_RATES_CTE
	    ON ITEMS_CTE.COUNTRY = DEF_RATES_CTE.COUNTRY
 WHERE (PRICES_CTE.PRICE_LEVEL IN (@PriceLevel) OR 'ALL' in (@PriceLevel))-- &lt; optional filter
       AND (ITEMS_CTE.COUNTRY IN (@CountryCode))-- OR 'ALL' in (@CountryCode))-- &lt; optional filter
       --AND PRICES_CTE.PRICE_LEVEL IN -- &lt; optional filter
	   AND (ITEMS_CTE.ITEM IN (@ItemCode) OR 'ALL' in (@ItemCode))-- &lt; optional filter 
 GROUP BY ITEMS_CTE.COUNTRY,
       ITEMS_CTE.ITEM,
	   PRICES_CTE.NAME,
	   PRICES_CTE.PRICE_LEVEL,
	   ITEMS_CTE.ITEM_CATEGORY,
	   PRICES_CTE.PV,
	   PRICES_CTE.CV,
	   PRICES_CTE.RETAIL_MARGIN,
	   PRICES_CTE.NET_PRICE,
	   RATES_CTE.VAT_RATE,
	   DEF_RATES_CTE.VAT_RATE
 ORDER BY ITEMS_CTE.COUNTRY ASC, ITEMS_CTE.ITEM ASC, PRICES_CTE.PRICE_LEVEL ASC</CommandText>
      </Query>
