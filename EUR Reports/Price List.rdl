    <DataSet Name="Country_Code">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select distinct country_code 
			from dw.dbo.dm_location_manager 
			where backoffice_mkting_unit=49
			order by country_code</CommandText>
      </Query>
      <Fields>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Price_Level">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT PRICE_LEVEL,[DESCRIPTION],CAST(PRICE_LEVEL AS VARCHAR)+' - '+[DESCRIPTION] PRICE_LVL_DESC 
	   FROM PLD 
	   WHERE MKTING_UNIT=49 AND PRICE_LEVEL IN (1, 2, 11)
ORDER BY PRICE_LEVEL</CommandText>
      </Query>
      <Fields>
        <Field Name="PRICE_LEVEL">
          <DataField>PRICE_LEVEL</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRICE_LVL_DESC">
          <DataField>PRICE_LVL_DESC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
