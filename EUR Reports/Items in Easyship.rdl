  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Item_Code">
            <Value>=Parameters!Item_Code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Country_Code">
            <Value>=Parameters!Country_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>(
SELECT
  ASL.ITEM_CODE,
  ASH.ORDER_NUMBER,
  ASH.DIST_ID,
  ASH.AUTO_BATCH,
  DST.EMAIL_ADDR,
  DST.COUNTRY_CODE
FROM
	ASL 
	INNER JOIN ASH ON ASH.MORDER = ASL.MORDER
	LEFT OUTER JOIN DST ON DST.MDIST = ASH.MDIST
WHERE 1=1
AND SUBSTRING(ASH.MDIST,1,2)  =  '49'
   AND ASL.ITEM_CODE  IN (@Item_Code)
   AND ASH.STATUS  =  1
)
EXCEPT  
(
SELECT
  ASL.ITEM_CODE,
  ASH.ORDER_NUMBER,
  ASH.DIST_ID,
  ASH.AUTO_BATCH,
  DST.EMAIL_ADDR,
  DST.COUNTRY_CODE
FROM
	ASL
	INNER JOIN ASH ON ASH.MORDER = ASL.MORDER
	LEFT OUTER JOIN DST ON DST.MDIST = ASH.MDIST
WHERE 1=1
   AND SUBSTRING(ASH.MDIST,1,2) = '49'
   AND ASL.ITEM_CODE IN (@Item_Code)
   AND ASH.STATUS = 1
   AND DST.COUNTRY_CODE IN (@Country_Code) Or @Country_Code is Null
)</CommandText>
      </Query>
