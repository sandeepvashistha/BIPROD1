  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@COUNTRY">
            <Value>=Parameters!COUNTRY.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT *
FROM
  (	SELECT 
	  ROW_NUMBER() OVER(PARTITION BY PV_DATE, COUNTRY_CODE ORDER BY VOL3 desc) AS RowNum, PV_DATE_DATE, DIST_ID, NM1 DIST_NAME, PAID_RANK, STATUS, COUNTRY_CODE, VOL3 OV
	FROM BWT
	WHERE MARKETING_UNIT = 49
	  AND COUNTRY_CODE NOT IN ('BEL','MAR')
	  AND COUNTRY_CODE = @COUNTRY
	  AND PAID_RANK &gt;= 5
	  AND PV_DATE &gt;= CONVERT(VARCHAR(6), DATEADD(MONTH , -12, GETDATE()), 112)
  ) A
WHERE RowNum &lt;= 10
ORDER BY PV_DATE_DATE, COUNTRY_CODE, RowNum
</CommandText>
      </Query>
      <Fields>
        <Field Name="RowNum">
          <DataField>RowNum</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="PV_DATE_DATE">
          <DataField>PV_DATE_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DIST_NAME">
          <DataField>DIST_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PAID_RANK">
          <DataField>PAID_RANK</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="STATUS">
          <DataField>STATUS</DataField>
