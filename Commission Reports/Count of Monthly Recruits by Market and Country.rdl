  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@COMM_PRD">
            <Value>=Parameters!COMM_PRD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    UNI_MKTING_UNIT MARKET, 
    COUNTRY_CODE COUNTRY, 
    ENTRY_PV_DATE_DATE COMM_PRD, 
    COUNT(DISTINCT MDIST) COUNT_OF_RECRUITS
FROM 
    DST
WHERE 
    ENTRY_PV_DATE_DATE &gt;= @COMM_PRD
    AND ENTRY_PV_DATE_DATE &lt; DATEADD(MONTH,1,GETDATE())
    AND DIST_STATUS IN ('A','B')
GROUP BY 
    UNI_MKTING_UNIT, COUNTRY_CODE, ENTRY_PV_DATE_DATE
ORDER BY 
    UNI_MKTING_UNIT, COUNTRY_CODE, ENTRY_PV_DATE_DATE



</CommandText>
      </Query>
      <Fields>
        <Field Name="MARKET">
          <DataField>MARKET</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="COUNTRY">
          <DataField>COUNTRY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="COMM_PRD">
          <DataField>COMM_PRD</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="COUNT_OF_RECRUITS">
