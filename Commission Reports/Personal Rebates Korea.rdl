      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>777aa4a1-4973-4127-89d5-1dab24fec874</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@COMMISSION_PERIOD">
            <Value>=Parameters!COMMISSION_PERIOD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  CONVERT(dateTIME, RTRIM(DW.dbo.BTR.PV_DATE)+'01') COMM_PRD,
  sum(DW.dbo.BTR.CONVERTED_BONUS) SUM_OF_COMMISSION_PAID
FROM
  DW.dbo.BTR
WHERE
  (
   DW.dbo.BTR.MKTING_UNIT  =  82
   AND
   CONVERT(dateTIME, RTRIM(DW.dbo.BTR.PV_DATE)+'01')  &gt;  @COMMISSION_PERIOD
   AND
   DW.dbo.BTR.BONUS_TYPE  =  3
   AND
   DW.dbo.BTR.CONVERTED_BONUS  &gt;  0
  )
GROUP BY
  CONVERT(dateTIME, RTRIM(DW.dbo.BTR.PV_DATE)+'01')</CommandText>
      </Query>
      <Fields>
        <Field Name="COMM_PRD">
          <DataField>COMM_PRD</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SUM_OF_COMMISSION_PAID">
          <DataField>SUM_OF_COMMISSION_PAID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
