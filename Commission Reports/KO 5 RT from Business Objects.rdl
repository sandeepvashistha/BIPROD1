  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DISTRIBUTOR_ID_LIST">
            <Value>=Parameters!DISTRIBUTOR_ID_LIST.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMMISSION_PERIOD">
            <Value>=Parameters!COMMISSION_PERIOD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.BTR.DIST_ID,
  DW.dbo.BTR.BONUS_TYPE,
  DW.dbo.BTR.SOURCE_COUNTRY,
  sum(DW.dbo.BTR.CONVERTED_BONUS) SUM_OF_CONVERTED_BONUS
FROM
  DW.dbo.BTR
WHERE 1=1
   AND DW.dbo.BTR.DIST_ID  IN (@DISTRIBUTOR_ID_LIST)
   --AND   CONVERT(dateTIME, RTRIM(DW.dbo.BTR.PV_DATE)+'01')  =  @COMMISSION_PERIOD
   AND DW.dbo.BTR.PV_DATE_DATE = @COMMISSION_PERIOD
   AND SOURCE_COUNTRY = 'KOR'
GROUP BY
  DW.dbo.BTR.DIST_ID, 
  DW.dbo.BTR.BONUS_TYPE, 
  DW.dbo.BTR.SOURCE_COUNTRY</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BONUS_TYPE">
          <DataField>BONUS_TYPE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SOURCE_COUNTRY">
          <DataField>SOURCE_COUNTRY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SUM_OF_CONVERTED_BONUS">
          <DataField>SUM_OF_CONVERTED_BONUS</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
