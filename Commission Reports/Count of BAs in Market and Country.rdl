  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  SAPDW_Dist_Market.MARKET_NAME,
  DW.dbo.DST.UNI_MKTING_UNIT,
  DW.dbo.DST.COUNTRY_CODE,
  count(distinct(DW.dbo.DST.MDIST)) DIST_COUNT
FROM
  DW.dbo.MARKET  SAPDW_Dist_Market,
  DW.dbo.DST
WHERE
  ( SAPDW_Dist_Market.UNI_MKTING_UNIT=DW.dbo.DST.UNI_MKTING_UNIT  )
  AND  
  DW.dbo.DST.DIST_STATUS  IN  ( 'A','B'  )
GROUP BY
  SAPDW_Dist_Market.MARKET_NAME, 
  DW.dbo.DST.UNI_MKTING_UNIT, 
  DW.dbo.DST.COUNTRY_CODE
</CommandText>
      </Query>
      <Fields>
        <Field Name="MARKET_NAME">
          <DataField>MARKET_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UNI_MKTING_UNIT">
          <DataField>UNI_MKTING_UNIT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="COUNTRY_CODE">
          <DataField>COUNTRY_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DIST_COUNT">
          <DataField>DIST_COUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
