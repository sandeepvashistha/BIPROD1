  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>UnityDb</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BatchId">
            <Value>=Parameters!BatchId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UnicityId">
            <Value>=Parameters!UnicityId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  BatchId
, UnicityId
, CommissionProfitCenterID
, SourceCountryCode
, PaidLevel
, SUM(Amount1) as Pv 
, SUM(BonusVolume) as BonusVolume
, [Percentage]
, SUM(BonusValue) BonusValue 
, SUM(ConvertedBonusValue) as ConvertedBonusValue

FROM UNCBonusResults.dbo.BonusDetail
WHERE 1=1
AND BatchId = @BatchId
AND UnicityId = @UnicityId--22309182 
AND CommissionMonth = '202004'

GROUP BY UnicityId
, SourceCountryCode
, PaidLevel
, [Percentage]
, CommissionProfitCenterID
, BatchId


</CommandText>
      </Query>
