  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>UnityDB</DataSourceName>
        <CommandText>select dist_id as DistId, 
d_name as CheckName, 
COUNTRY_CODE,
pv_date_date as PvDate, 
SUM(EARNINGS_AMOUNT) as GrossAmount
from [SQLPRODA\SQLA].DW.dbo.ERN
where mkting_unit = 1
and COUNTRY_CODE = 'USA'
and DATEDIFF(m,PV_DATE_DATE,GETDATE()) &lt; = 6 
GROUP BY dist_id, 
d_name, 
pv_date_date, 
COUNTRY_CODE 


UNION ALL 

SELECT UnicityId
, Name
, CountryCode
, CONVERT(DATE,RTRIM(CommissionMonth) + '01')
, ([RetailEarnings] 
 + [PersonalRebatesUnityEarnings] 
 + [BusinessDevelopmentEarnings] 
 + [FastStartUnityEarnings] 
 + [Phase1Earnings] 
 + [GenerationPayEarnings] 
 + [CarBonusEarnings] 
 + [LifestylePointsEarnings] 
 + [PersonalRebatesGlobalEarnings] 
 + [RoyaltiesEarnings] 
 + [FastStartGlobalEarnings] 
 + [DirectorLifestyleEarnings] 
 + [ManualAdjustmentEarnings] 
 + [ItalyEarnings] ) as TotalEarnings
FROM UNCBonusResults.dbo.CustomerResults cr
INNER JOIN UnityDB.dbo.Customer c ON c.CustomerId_Unicity = cr.UnicityId
WHERE 1=1
AND BatchId = UnityDB.dbo.fnGetBatchId(1,DATEADD(m,-1,DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0)))
AND ([RetailEarnings] 
 + [PersonalRebatesUnityEarnings] 
 + [BusinessDevelopmentEarnings] 
 + [FastStartUnityEarnings] 
 + [Phase1Earnings] 
 + [GenerationPayEarnings] 
 + [CarBonusEarnings] 
 + [LifestylePointsEarnings] 
 + [PersonalRebatesGlobalEarnings] 
 + [RoyaltiesEarnings] 
 + [FastStartGlobalEarnings] 
 + [DirectorLifestyleEarnings] 
 + [ManualAdjustmentEarnings] 
 + [ItalyEarnings] ) &gt; 0 
</CommandText>
      </Query>
