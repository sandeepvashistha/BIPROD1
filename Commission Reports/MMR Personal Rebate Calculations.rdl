  <DataSets>
    <DataSet Name="CurrentSales">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CommPrd">
            <Value>=Parameters!CommPrd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DIST_ID, 
  SUM(CASE WHEN CURRENCY_CODE = 'MMK' THEN CURRENT_SALES ELSE CURRENT_SALES * ExchangeToUSD * USDtoMMK END) CURRENT_SALES_MMK
FROM 
(	
SELECT 
  DIST_ID,
  CURRENCY_CODE,
  SUM(CASE ODH_TYPE WHEN 'I' THEN PRICE_1 WHEN 'C' THEN -PRICE_1 END) CURRENT_SALES,
  exchange_rate_amount ExchangeToUSD,
  (  SELECT exchange_rate_amount 
     FROM VW_NAV_CURRENCY_ALL_DSR 
	 WHERE from_currency = 'USD' 
	   AND to_currency = 'MMK' 
	   AND EOMONTH(@CommPrd) BETWEEN starting_date AND end_date) USDtoMMK
FROM
  ODH LEFT JOIN VW_NAV_CURRENCY_ALL_DSR
    ON ODH.CURRENCY_CODE = VW_NAV_CURRENCY_ALL_DSR.from_currency
WHERE
  PV_DATE_DATE = @CommPrd
  AND STATUS IN (2,3,4,5)
  AND DIST_ID IN (SELECT DIST_ID FROM DST WHERE COUNTRY_CODE = 'MMR')	
  AND DISTRIBUTOR_STATUS IN ('A','B')			
  AND PRICE_1 != 0
  AND to_currency = 'USD'
  AND EOMONTH(@CommPrd) BETWEEN starting_date AND VW_NAV_CURRENCY_ALL_DSR.end_date
GROUP BY DIST_ID, CURRENCY_CODE, exchange_rate_amount
) A
WHERE CURRENT_SALES != 0
GROUP BY DIST_ID
ORDER BY 2 DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CURRENT_SALES_MMK">
          <DataField>CURRENT_SALES_MMK</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PriorCumulativeSales">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CommPrd">
            <Value>=Parameters!CommPrd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DIST_ID, 
  SUM(CASE WHEN CURRENCY_CODE = 'MMK' THEN CUMULATIVE_SALES ELSE CUMULATIVE_SALES * ExchangeToUSD * USDtoMMK END) CUMULATIVE_SALES_MMK
FROM 
(
SELECT
  DIST_ID, 
  CURRENCY_CODE,
  SUM(CASE ODH_TYPE WHEN 'I' THEN PRICE_1 WHEN 'C' THEN -PRICE_1 END) CUMULATIVE_SALES,
  exchange_rate_amount ExchangeToUSD,
  (  SELECT exchange_rate_amount 
     FROM VW_NAV_CURRENCY_ALL_DSR 
	 WHERE from_currency = 'USD' 
	   AND to_currency = 'MMK' 
	   AND EOMONTH(@CommPrd) BETWEEN starting_date AND end_date) USDtoMMK
FROM
  ODH LEFT JOIN VW_NAV_CURRENCY_ALL_DSR
    ON ODH.CURRENCY_CODE = VW_NAV_CURRENCY_ALL_DSR.from_currency	
WHERE		
  PV_DATE_DATE BETWEEN '2017-08-01' AND DATEADD(MONTH,-1,@CommPrd)
  AND STATUS IN (2,3,4,5)	
  AND DISTRIBUTOR_STATUS IN ('A','B')	
  AND DIST_ID IN (  SELECT DIST_ID FROM DST WHERE COUNTRY_CODE = 'MMR' AND DIST_STATUS IN ('A','B')  )
  AND PRICE_1 != 0
  AND to_currency = 'USD'
  AND EOMONTH(@CommPrd) BETWEEN starting_date AND VW_NAV_CURRENCY_ALL_DSR.end_date
GROUP BY		
  DIST_ID, CURRENCY_CODE, exchange_rate_amount	
) A
WHERE CUMULATIVE_SALES != 0
GROUP BY DIST_ID
ORDER BY 2 DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CUMULATIVE_SALES_MMK">
          <DataField>CUMULATIVE_SALES_MMK</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
