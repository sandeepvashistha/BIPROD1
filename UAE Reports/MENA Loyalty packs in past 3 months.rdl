    <DataSet Name="Country_Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select country_code,country_code+' - '+country_name descri from dw.dbo.dm_location_manager where backoffice_mkting_unit=97
and country_code in ('DZA','JOR','LBN','MAR','OMN','UAE','KWT','BHR')
order by country_code</CommandText>
      </Query>
      <Fields>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="descri">
          <DataField>descri</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
