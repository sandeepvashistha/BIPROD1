<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>6c15c4a0-5135-4ed1-8575-fa15933b585a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Main_Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PVDate">
            <Value>=Parameters!PVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dst.dist_id,dst.roman_name_1,dst.dist_status,dst.country_code dist_country_code,dst.entry_date,odh.country_code order_country_code,odh.pv_date_date,
odl.item_code,itd.item_name_1,odl.qty_ordered,odl.price_1,(odl.qty_ordered*odl.price_1) [Amount],cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate)-3, 0) as date) last_3month,cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate), 0) as date) selected_month
from dw.dbo.odh
inner join dw.dbo.dst on (odh.dist_id=dst.dist_id)
inner join dw.dbo.odl on (odh.morder=odl.morder)
left outer join dw.dbo.itd on (odl.item_code=itd.item_code and odl.mkting_unit=itd.uni_mkting_unit)
where odh.uni_mkting_unit=97 and odh.status in (2,3,4,5) and dst.entry_date between cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate)-6, 0) as date) and cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate), 0) as date)
and odh.pv_date_date between cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate)-3, 0) as date) and cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate), 0) as date)
and odh.country_code=@CountryCode
and odl.item_code in
(
'31981','31980','31982','31983','31985','31979','31984','31823','31826','31827','31825','31821','31824','31823','31822','31831','31829','31832',
'31828','31833','31461','31830','31876','31874','31872','31875','31928','31873','31816','31814','31812','31815','31926','31813','31817','31818',
'31819','31820','31927','31896','31900','31897','31898','31899'
)</CommandText>
      </Query>
