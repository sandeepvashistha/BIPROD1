<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2d2e369b-2e5a-4ddb-afe3-66bb76d16b3b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT ds.[PV Date]
, ds.[Item Code]
, ds.Description
, d.Home_Country
, ds.[Country Code] 
, SUM(ds.[Qty Ordered]) as Qty 
, SUM(ds.[Qty Ordered]) - LAG(SUM(ds.[Qty Ordered]),1,0) OVER(PARTITION BY ds.[Item Code], ds.Description, d.Home_Country, ds.[Country Code] ORDER BY ds.[Pv Date] ) as LagDiff
, COUNT(DISTINCT ds.[Dist Id]) as Dists
FROM DW.dbo.DM_SHIPPED_SALES_PACKS ds 
INNER JOIN DW.dbo.DST d ON d.DIST_ID = ds.[Dist ID] 
WHERE 1=1 
AND d.UNI_MKTING_UNIT = 97 
--AND d.HOME_COUNTRY = 'SAU' 
AND ds.[Country Code] = 'MYS' 
AND ds.[Pv Date] &gt;= '2019-04-01' 
GROUP BY 
ds.[PV Date]
, ds.[Item Code]
, ds.Description
, d.Home_Country
, ds.[Country Code] </CommandText>
      </Query>
      <Fields>
        <Field Name="PV_Date">
          <DataField>PV Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Code">
