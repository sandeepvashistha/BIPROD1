        </Field>
        <Field Name="LagDiff">
          <DataField>LagDiff</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Dists">
          <DataField>Dists</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
