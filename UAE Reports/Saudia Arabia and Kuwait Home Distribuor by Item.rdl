  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT ds.[PV Date]
, ds.[Item Code]
, ds.Description
, d.Home_Country
, ds.[Country Code] 
, SUM(ds.[Qty Ordered]) as Qty 
, COUNT(DISTINCT ds.[Dist Id]) as Dists
FROM DW.dbo.DM_SHIPPED_SALES_PACKS ds 
INNER JOIN DW.dbo.DST d ON d.DIST_ID = ds.[Dist ID] 
WHERE 1=1 
AND d.HOME_COUNTRY = 'SAU' 
AND ds.[Country Code] = 'MYS' 
AND ds.[Pv Date] &gt;= '2019-04-01' 
GROUP BY 
ds.[PV Date]
, ds.[Item Code]
, ds.Description
, d.Home_Country
, ds.[Country Code] 

ORDER BY 1, 4 DESC </CommandText>
      </Query>
      <Fields>
        <Field Name="PV_Date">
          <DataField>PV Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Code">
