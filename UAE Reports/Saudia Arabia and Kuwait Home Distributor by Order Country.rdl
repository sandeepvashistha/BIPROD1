  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT DST.HOME_COUNTRY as "Home Country"
, DM_DSR.COUNTRY_CODE as "Country"
, DM_DSR.PV_DATE as "Pv Date"
, SUM(CASE WHEN DM_DSR.ODH_TYPE = 'C' THEN DM_DSR.PRICE_2*-1 ELSE DM_DSR.PRICE_2 END)  as Pv 
, LAG(SUM(CASE WHEN DM_DSR.ODH_TYPE = 'C' THEN DM_DSR.PRICE_2*-1 ELSE DM_DSR.PRICE_2 END),1,0) 
	OVER(PARTITION BY DST.HOME_COUNTRY,  DM_DSR.COUNTRY_CODE ORDER BY DM_DSR.PV_DATE) as PvLag
, SUM(CASE WHEN DM_DSR.ODH_TYPE = 'C' THEN DM_DSR.PRICE_2*-1 ELSE DM_DSR.PRICE_2 END) 
	- LAG(SUM(CASE WHEN DM_DSR.ODH_TYPE = 'C' THEN DM_DSR.PRICE_2*-1 ELSE DM_DSR.PRICE_2 END),1,0) OVER(PARTITION BY DST.HOME_COUNTRY,  DM_DSR.COUNTRY_CODE ORDER BY DM_DSR.PV_DATE) as LagDiff
FROM DW.dbo.DM_DSR 
INNER JOIN DW.dbo.DST ON DST.DIST_ID = DM_DSR.DIST_ID 
WHERE DST.HOME_COUNTRY IN ('SAU','KWT')
AND PV_DATE &gt;= '2018-08-01' 
GROUP BY DST.HOME_COUNTRY 
, DM_DSR.COUNTRY_CODE 
, DM_DSR.PV_DATE</CommandText>
      </Query>
