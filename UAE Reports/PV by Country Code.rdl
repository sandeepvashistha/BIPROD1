  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.DST.COUNTRY_CODE,
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103) as pv_date,
  DW.dbo.OTY.DESCRIPTION,
  sum(DW.dbo.ODH.PRICE_2) as pv_amount
FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.OTY
WHERE
  ( DW.dbo.OTY.MKTING_UNIT=DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.OTY.ORDER_TYPE=DW.dbo.ODH.ODH_TYPE  )
  AND  ( DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID  )
  AND  
  (
   CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)  IN  (@StartDate)
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 97  )
  )
GROUP BY
  DW.dbo.ODH.ENTRY_DATE, 
  DW.dbo.DST.COUNTRY_CODE, 
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103), 
  DW.dbo.OTY.DESCRIPTION
</CommandText>
      </Query>
      <Fields>
        <Field Name="ENTRY_DATE">
          <DataField>ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="COUNTRY_CODE">
          <DataField>COUNTRY_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pv_date">
          <DataField>pv_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
