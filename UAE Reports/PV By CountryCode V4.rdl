<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>3ced5140-cdfe-4f6d-82a1-72ae6703f837</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PVDate">
            <Value>=Parameters!PVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.DST.COUNTRY_CODE as DST_COUNTRY_CODE,
  DW.dbo.ODH.ODH_TYPE,
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) as PV_DATE,
  DW.dbo.OTY.DESCRIPTION,
  CASE WHEN DW.dbo.ODH.ODH_TYPE = 'C' THEN sum(DW.dbo.ODH.PRICE_2*-1) ELSE sum(DW.dbo.ODH.PRICE_2) END  PV,
  DW.dbo.ODH.POST_DATE,
  CASE WHEN DW.dbo.ODH.ODH_TYPE = 'C' THEN  sum(DW.dbo.ODH.price_1*-1 /  ISNULL(VW_NAV_CURRENCY_ALL_USD.Exchange_Rate_Amount,1)) 
	ELSE  sum(DW.dbo.ODH.price_1 /  ISNULL(VW_NAV_CURRENCY_ALL_USD.Exchange_Rate_Amount,1)) END  SALES_USD,
  CASE WHEN DW.dbo.ODH.ODH_TYPE = 'C' THEN 
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1*-1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1*-1 END) 
  ELSE sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1*-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) 
  END  as SALES_LOCAL,
  VW_NAV_CURRENCY_ALL_BHD.Exchange_Rate_Amount amt_bhd,
  DW.dbo.ODH.COUNTRY_CODE as ODH_COUNTRY_CODE,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.WHS.WH_NAME,
  DW.dbo.ODH.CURRENCY_CODE, 
  CASE WHEN DW.dbo.ODH.ODH_TYPE = 'C' THEN  sum(((DW.dbo.ODH.price_1*VW_NAV_CURRENCY_ALL_BHD.Exchange_Rate_Amount)*-1) /  ISNULL(VW_NAV_CURRENCY_ALL_USD.Exchange_Rate_Amount,1)) ELSE 
   sum((DW.dbo.ODH.price_1*VW_NAV_CURRENCY_ALL_BHD.Exchange_Rate_Amount) /  ISNULL(VW_NAV_CURRENCY_ALL_USD.Exchange_Rate_Amount,1)) 
   END as AmntFromUSDtoBHD
FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.OTY,
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR  VW_NAV_CURRENCY_ALL_USD,
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR  VW_NAV_CURRENCY_ALL_BHD,
  DW.dbo.WHS
WHERE
  ( DW.dbo.OTY.MKTING_UNIT=DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.OTY.ORDER_TYPE=DW.dbo.ODH.ODH_TYPE  )
  AND  ( DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID  )
  AND  ( VW_NAV_CURRENCY_ALL_USD.TO_CURRENCY=DW.dbo.ODH.CURRENCY_CODE and DATEADD(month, DATEDIFF(month, 0, ODH.ENTRY_DATE), 0) 
		between VW_NAV_CURRENCY_ALL_USD.STARTING_DATE and VW_NAV_CURRENCY_ALL_USD.END_DATE )
  AND  ( VW_NAV_CURRENCY_ALL_USD.from_currency='USD'  )
  AND  ( DATEADD(month, DATEDIFF(month, 0, ODH.ENTRY_DATE), 0) between VW_NAV_CURRENCY_ALL_BHD.STARTING_DATE and VW_NAV_CURRENCY_ALL_BHD.END_DATE )
  AND  ( VW_NAV_CURRENCY_ALL_BHD.from_currency='USD' AND VW_NAV_CURRENCY_ALL_BHD.to_currency='BHD'  )
  AND  ( WHS.MKTING_UNIT=DW.dbo.ODH.UNI_MKTING_UNIT  )
  AND  ( WHS.WH_NUMBER=DW.dbo.ODH.WAREHOUSE  )
  AND  
  (
   (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103))  &gt;= @PVDate
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 97  )
  )
GROUP BY
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.DST.COUNTRY_CODE,
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)),
  DW.dbo.OTY.DESCRIPTION,
  DW.dbo.ODH.POST_DATE,
  VW_NAV_CURRENCY_ALL_BHD.Exchange_Rate_Amount,
  DW.dbo.ODH.COUNTRY_CODE,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.WHS.WH_NAME,
  DW.dbo.ODH.CURRENCY_CODE, 
  DW.dbo.ODH.ODH_TYPE</CommandText>
      </Query>
