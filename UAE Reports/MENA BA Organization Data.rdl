  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BA_ID">
            <Value>=Parameters!BA_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH cteLevels
AS
(
SELECT T1.TOP_MSPON T_MSPON,T1.DIST_ID,DST.ROMAN_NAME_1,T1.MDIST AS MDIST,DST.EMAIL_ADDR, DST.HOME_PHONE, DST.CELL_PHONE,
         T1.TOP_MSPON,0 AS GL,DST.ENTRY_DATE,
		 CAST (DST.ROMAN_NAME_1 AS VARCHAR(25)) AS [Dist Level],
		 CAST(T1.TOP_MSPON AS VARCHAR(MAX))+T1.MDIST as [Order],
		 DST.ADDR,DST.ADDR_2,DST.ADDR_3,DST.DIST_STATUS
  FROM   DW.DBO.GENIE T1
  INNER JOIN DW.DBO.DST ON T1.DIST_ID=DST.DIST_ID
  INNER JOIN DW.DBO.DST DST_TOP ON T1.TOP_MSPON=DST_TOP.MDIST
  WHERE  DST_TOP.DIST_ID IN (@BA_ID)--('21768897')
  AND T1.GROUP_LEVEL IN (100) 
UNION ALL
SELECT T1.T_MSPON,T2.DIST_ID,DST.ROMAN_NAME_1,T2.MDIST,DST.EMAIL_ADDR, DST.HOME_PHONE, DST.CELL_PHONE,
         T2.TOP_MSPON,GL+1,DST.ENTRY_DATE,
		 CAST (Replicate('*', GL+1) + DST.ROMAN_NAME_1 AS VARCHAR(25)) AS [Dist Level],
		 CAST([Order] AS VARCHAR(MAX)) + '_' + CAST(T2.MDIST AS VARCHAR(MAX)) as [Order],
		 DST.ADDR,DST.ADDR_2,DST.ADDR_3,DST.DIST_STATUS
  FROM   DW.DBO.GENIE T2 INNER JOIN cteLevels T1 ON (T2.TOP_MSPON = T1.MDIST)
						 INNER JOIN DW.DBO.DST ON (DST.DIST_ID=T2.DIST_ID)
  WHERE T2.GROUP_LEVEL=101
 )
 SELECT
  cteLevels.T_MSPON,cteLevels.DIST_ID,cteLevels.ROMAN_NAME_1,cteLevels.MDIST,cteLevels.DIST_STATUS,cteLevels.ENTRY_DATE,
  cteLevels.EMAIL_ADDR,cteLevels.HOME_PHONE,cteLevels.CELL_PHONE,cteLevels.TOP_MSPON,
  cteLevels.GL,cteLevels.[Dist Level],
  ROW_NUMBER() OVER (ORDER BY cteLevels.[order]) AS [serial number]
FROM   cteLevels
ORDER BY [order]</CommandText>
      </Query>
