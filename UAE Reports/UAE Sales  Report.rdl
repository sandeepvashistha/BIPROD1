    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Post_Month">
            <Value>=Parameters!Start_Post_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Post_Month">
            <Value>=Parameters!End_Post_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dist_Country">
            <Value>=Parameters!Dist_Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  --DW.dbo.ODH.POST_DATE,
  DW.dbo.DST.Uni_Mkting_unit As Dist_Market,
  DW.dbo.DST.COUNTRY_CODE As Dist_Country,
  DW.dbo.ODH.Uni_Mkting_unit As Order_Market,
  DW.dbo.ODH.Country_Code As Order_Country,
  DW.dbo.ODH.ODH_Type,
  --CONVERT(DATETIME,'1/'+ODH.PV_DATE,103) as Post_Month,
  --DATEADD(mm, DATEDIFF(mm, 0, DW.dbo.ODH.POST_DATE), 0) as Post_Month,
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) as Post_Month,
  --DW.dbo.ODH.Warehouse,
  --sum(DW.dbo.ODH.PRICE_2) as pv_amount,
  Sum(DW.dbo.ODH.PRICE_1) as Amount,
  sum(DW.dbo.ODH.PRICE_1 /  ISNULL(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) As Amount_USD
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID  )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
    AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
    And DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' )
WHERE
  (
   --CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)Between @Start_Post_Month and --@End_Post_Month
   --DATEADD(mm, DATEDIFF(mm, 0, DW.dbo.ODH.POST_DATE), 0) Between '01-JUNE-2017' --and '01-JULY-2017'
   --DATEADD(mm, DATEDIFF(mm, 0, DW.dbo.ODH.POST_DATE), 0) Between --@Start_Post_Month and @End_Post_Month
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) Between @Start_Post_Month and @End_Post_Month
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.DST.UNI_MKTING_UNIT  IN  ( 97  )
   and 
   --DW.dbo.DST.Country_Code = 'UAE'
   DW.dbo.DST.Country_Code in (@Dist_Country)
   --and
   --DW.dbo.ODH.Warehouse = '200'
  )
GROUP BY
  --DW.dbo.ODH.POST_DATE,
  DW.dbo.DST.Uni_Mkting_unit, 
  DW.dbo.DST.COUNTRY_CODE,
  DW.dbo.ODH.Uni_Mkting_unit,
  DW.dbo.ODH.Country_Code,
  DW.dbo.ODH.ODH_Type, 
  CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)
  --DATEADD(mm, DATEDIFF(mm, 0, DW.dbo.ODH.POST_DATE), 0)
  --DW.dbo.ODH.Warehouse</CommandText>
      </Query>
