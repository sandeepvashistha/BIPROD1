    <DataSet Name="Country_Code">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select country_code,country_code+'-'+country_name descri 
	from dw.dbo.dm_location_manager where  backoffice_mkting_unit=97
	order by country_code</CommandText>
      </Query>
