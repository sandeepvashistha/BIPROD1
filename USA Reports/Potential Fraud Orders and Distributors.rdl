<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Query1">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c1a6d3c8-9be1-4139-a658-499a8b064c43</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>Query1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LastXDays">
            <Value>=Parameters!LastXDays.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ScoreOutOf7">
            <Value>=Parameters!ScoreOutOf7.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT * 
, Flag_ExpiditedShipping 
	+ Flag_BelongsToSponsor 
	+ Flag_LargeOrder 
	+ Flag_RetailCust 
	+ Flag_InvalidGovernmentId	
	+ Flag_InvalidEmail	
	+ Flag_BillingShippingZipMismatch
 as Score 

FROM 
(
	SELECT ODH.DIST_ID, 
	DST.DIST_STATUS,
	DST.EMAIL_ADDR, 
	DST.ROMAN_NAME_1,
	ODH.ORDER_NUMBER, 
	ODH.SHIP_VIA_CODE, 
	ODH.SHIP_VIA_DESC, 
	ODH.TOT_INVCE_AMOUNT, 
	CASE WHEN DATEDIFF(d,DST.ENTRY_DATE, ODH.POST_DATE) &lt; 7 THEN 1 ELSE 0 END as Flag_NewCustomer, 
	CASE WHEN ODH.SHIP_VIA_CODE = 40 THEN 1 ELSE 0 END as Flag_ExpiditedShipping, 
	CASE WHEN DST.SPONSOR_DIST_ID IN (3978608,218835066,96676502,2) THEN 1 ELSE 0 END as Flag_BelongsToSponsor,
	CASE WHEN ODH.TOT_INVCE_AMOUNT &gt; 1000 THEN 1 ELSE 0 END as Flag_LargeOrder,
	CASE WHEN DST.DIST_STATUS = 'C' THEN 1 ELSE 0 END as Flag_RetailCust, 
	CASE WHEN LEN(REPLACE(DST.GOVERNMENT_ID,'-','')) != 9 THEN 1 ELSE 0 END as Flag_InvalidGovernmentId,
	CASE WHEN (NOT DST.EMAIL_ADDR LIKE '%_@__%.__%' AND PATINDEX('%[^a-z,0-9,@,.,_,\-]%', DST.EMAIL_ADDR) = 0) THEN 1 ELSE 0 END as Flag_InvalidEmail,
	CASE WHEN LEFT(DW.dbo.NumericOnly(ODH.SHIP_TO_ZIP),5) != LEFT(DW.dbo.NumericOnly(Payments.BillingZipAddress),5) THEN 1 ELSE 0 END as Flag_BillingShippingZipMismatch
	FROM DW.dbo.ODH 
	INNER JOIN DW.dbo.DST ON ODH.MDIST = DST.MDIST 
	INNER JOIN Exigo_Repo.dbo.Orders ON Orders.Other11 = CAST(ODH.ORDER_NUMBER as NVARCHAR(21))
	INNER JOIN Exigo_Repo.dbo.Payments ON Payments.OrderID = Orders.OrderID
	WHERE ODH.UNI_MKTING_UNIT = 1 
	AND ODH.POST_DATE &gt;= GETDATE() - @LastXDays
	/*Rules*/ 
	AND (
		DATEDIFF(d,DST.ENTRY_DATE, ODH.POST_DATE) &lt; @LastXDays --New Customers, num days recently signed up 
		OR ODH.SHIP_VIA_CODE = 40 --UPS NextBusinessDay 
		OR DST.SPONSOR_DIST_ID IN (3978608,218835066,96676502,2)--Belongs to any one of these sponsors 
		OR ODH.TOT_INVCE_AMOUNT &gt; 1000--A large order
		OR DST.DIST_STATUS = 'C' --Retail Customers 
		OR LEN(REPLACE(DST.GOVERNMENT_ID,'-','')) != 9 --Valid Government ID 
		OR (NOT DST.EMAIL_ADDR LIKE '%_@__%.__%' AND PATINDEX('%[^a-z,0-9,@,.,_,\-]%', DST.EMAIL_ADDR) = 0)--Valid Email Address 
		OR LEFT(DW.dbo.NumericOnly(ODH.SHIP_TO_ZIP),5) != LEFT(DW.dbo.NumericOnly(Payments.BillingZipAddress),5)--BillingAddressShipToMismatch 
		)
) a 
WHERE 1=1 
AND Flag_ExpiditedShipping + Flag_BelongsToSponsor + Flag_LargeOrder + Flag_RetailCust + Flag_InvalidGovernmentId + Flag_InvalidEmail + Flag_BillingShippingZipMismatch &gt;= @ScoreOutOf7 </CommandText>
      </Query>
