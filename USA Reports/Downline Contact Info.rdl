  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TopMspon">
            <Value>=Parameters!TopMspon.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*used this for the text promo Micah is doing 2020-04-06 */ 
SELECT DST.DIST_ID as Dist_Id ,
DST.API_NATIVE_FIRST_NAME as First_Name, 
DST.API_NATIVE_LAST_NAME as Last_Name , 
DST.ROMAN_NAME_1 as Name , 
DST.COUNTRY_CODE as Country, 
DST.DIST_STATUS as Status, 
ISNULL(DSR.LAST_ORDER,'') as Last_Order, 
DST.EMAIL_ADDR as Email_Address, 
ISNULL(DST.FLAG_19,'Y') as Email_Allow, 
COALESCE(DST.CELL_PHONE, DST.WORK_PHONE, DST.HOME_PHONE) as Phone
FROM DW.dbo.DST 
INNER JOIN 
	(SELECT DIST_ID, MAX(POST_DATE) as LAST_ORDER 
	FROM DW.dbo.DM_DSR 
	GROUP BY DIST_ID
	) DSR ON DSR.DIST_ID = DST.DIST_ID 

WHERE DST.DIST_ID IN (SELECT DIST_ID FROM DW.dbo.GENIE WHERE TOP_MSPON = @TopMspon)--'01-116971801'	) --Darlene Longs Downline 
AND DSR.LAST_ORDER &gt;=  DATEADD(m,-6,DW.dbo.fnFirstDayOfMonth(GETDATE()))
AND ISNULL(DST.FLAG_19,'Y') = 'Y' 
ORDER BY ISNULL(DSR.LAST_ORDER,'') DESC</CommandText>
      </Query>
