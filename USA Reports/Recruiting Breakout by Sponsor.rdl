  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT DST.DIST_ID, 
DST.HOME_COUNTRY, 
DST.COUNTRY_CODE, 
DST.UNI_MKTING_UNIT, 
DST.ENTRY_DATE, 
DST.SPONSOR_DIST_ID, 
DSTS.ROMAN_NAME_1 as SPONSOR_NAME,
DW.dbo.fnFirstDayOfMonth(DST.ENTRY_DATE) as ENTRY_MONTH, 
DST.DIST_STATUS, 
a.LAST_ORDER_DATE
FROM DW.dbo.DST 
INNER JOIN DW.dbo.DST DSTS ON DSTS.DIST_ID = DST.SPONSOR_DIST_ID 
LEFT OUTER JOIN (SELECT DIST_ID, MAX(POST_DATE) LAST_ORDER_DATE FROM DW.dbo.DM_DSR GROUP BY DIST_ID) a ON a.DIST_ID = DST.DIST_ID 
WHERE DST.UNI_MKTING_UNIT IN (1,2,58) 
AND DST.COUNTRY_CODE IN ('USA', 'CAN', 'MEX')
AND DST.SPONSOR_DIST_ID NOT IN (1,2,3,4,5)  
AND DATEDIFF(m,DST.ENTRY_DATE,GETDATE()) &lt;= 12</CommandText>
      </Query>
