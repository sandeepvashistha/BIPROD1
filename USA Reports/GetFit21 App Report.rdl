  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT a.Id
, a.CoachId
, ds.ROMAN_NAME_1 as CoachName
, a.ChallengeName
, a.Created
, a.StartDate
, a.Code
, a.ChallengerId 
, d.ROMAN_NAME_1 as ChallengerName 
, p.pv as CoachPv 
FROM 
(
	SELECT c.id
	, c.coachid  
	, c.created
	, c.startdate
	, c.code
	, c.description as ChallengeName 
	, ChallengerId
	FROM Prospects.dbo.Challenge c
	CROSS APPLY OPENJSON(c.challengers)
	WITH (ChallengerId nvarchar(255) '$.customer.id.unicity') as ChallengerId
) a 
LEFT OUTER JOIN DW.dbo.DST d ON d.DIST_ID = a.ChallengerId
LEFT OUTER JOIN DW.dbo.DST ds ON ds.DIST_ID = a.CoachId 
LEFT OUTER JOIN 
	(SELECT dist_id 
	, pv_date 
	, SUM(price_2) as pv 
	FROM DW.dbo.DM_DSR
	GROUP BY dist_id 
	, pv_date 
	) p ON p.dist_id = a.coachid AND p.pv_date = DATEADD(month, DATEDIFF(month, 0, a.startdate), 0) 


SELECT * FROM Prospects.dbo.Challenge</CommandText>
      </Query>
