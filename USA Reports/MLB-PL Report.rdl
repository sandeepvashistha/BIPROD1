    <DataSet Name="MLB_DIFFERENT_PL">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select c.No_ as MLB_PO, d.No_ as PL_PO, c.[Buy-from Vendor No_], c.[Buy-from Vendor Name], c.MLB_item_no as item_no, c.MLB_date as receipt_date, cast(c.MLB_quantity as numeric(20,0)) MLB_quantity,cast(d.PL_quantity as numeric(20,0)) PL_quantity,
cast(c.MLB_quantity as numeric(20,0)) - cast(d.PL_quantity as numeric(20,0)) Different,c.[Purchaser Code]
from
(
       select a.No_, a.[Vendor Order No_], a.[Buy-from Vendor No_], a.[Buy-from Vendor Name],a.[Purchaser Code], b.No_ as MLB_item_no, b.[Expected Receipt Date] as MLB_date, sum(b.Quantity) as MLB_quantity
       from purchase_header_nc_vw a
       join Purchase_Line_nc_vw b
       on a.[Document Type] = b.[Document Type]
       and a.No_ = b.[Document No_]
       where 1=1 
       and a.uni_company_name = 'MLB Exports'
       and a.[Document Type] = 1 --PO, not a quote
       and a.[Completed] = 0
       and b.[Type] = 2 --Item
       and b.No_ &lt;&gt; '' --contains an item number
       group by a.No_, a.[Vendor Order No_], a.[Buy-from Vendor No_],a.[Purchaser Code],a.[Buy-from Vendor Name], b.No_, b.[Expected Receipt Date]
) c
join
(
       select a.No_, a.[Vendor Order No_], a.[Buy-from Vendor No_], a.[Buy-from Vendor Name],   b.No_ as PL_item_no, b.[Expected Receipt Date] as PL_date, sum(b.Quantity) as PL_quantity
       from purchase_header_nc_vw a
       join Purchase_Line_nc_vw b
       on a.[Document Type] = b.[Document Type]
       and a.No_ = b.[Document No_]
       where 1=1 
       and a.uni_company_name = 'Performance Labs'
       and a.[Document Type] = 1 --PO, not a quote
       and a.[Completed] = 0
       and b.[Type] = 2 --Item
       and b.No_ &lt;&gt; '' --contains an item number
       group by a.No_, a.[Vendor Order No_], a.[Buy-from Vendor No_], a.[Buy-from Vendor Name], b.No_, b.[Expected Receipt Date]
) d
on c.[Vendor Order No_] = d.No_
and c.MLB_item_no = d.PL_item_no
and c.MLB_date = d.PL_date
where 1=1
and c.MLB_quantity &lt;&gt; d.PL_quantity</CommandText>
      </Query>
