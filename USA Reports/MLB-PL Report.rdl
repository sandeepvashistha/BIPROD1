  <DataSets>
    <DataSet Name="MLB_NOT_PO">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 
       distinct 
       c.No_
       , c.[Vendor Order No_]
       , c.uni_company_name
       , c.[Buy-from Vendor No_]
       , c.[Buy-from Vendor Name]
	   , c.[Purchaser Code]
	   , c.[Ship-to Name]
	   , c.[Location Code]
from
(
       select ph.*
       from [Prod_Navision].dbo.[Purchase_Header_nc_vw] ph
                    join Prod_Navision.[dbo].[Purchase_Line_nc_vw] pl
                    on 1=1
                    and ph.[No_]=pl.[Document No_] 
                    and ph.[uni_company_name]=pl.[uni_company_name]
                    and ph.[Document Type] = pl.[Document Type]
             Where
                    1=1
                    and pl.[uni_company_name] = 'MLB Exports' 
                    and pl.[Document Type] = 1
                    and pl.[Type] = 2 
                    and ph.[Completed] = 0
					and (pl.[Location Code] like 'PL%' or pl.[Location Code] like 'UGM%')
) c
left outer join
(
       select ph.*
       from [Prod_Navision].dbo.[Purchase_Header_nc_vw] ph
                    join Prod_Navision.[dbo].[Purchase_Line_nc_vw] pl
                    on 1=1
                    and ph.[No_]=pl.[Document No_] 
                    and ph.[uni_company_name]=pl.[uni_company_name]
                    and ph.[Document Type] = pl.[Document Type]
             Where
                    1=1
                    and pl.[uni_company_name] = 'Performance Labs' 
                    and pl.[Document Type] = 1
                    and pl.[Type] = 2
                    and ph.[Completed] = 0
) d
on c.[Vendor Order No_] = d.No_
--and c.[Vendor Order No_] &lt;&gt; ''
where d.No_ is null</CommandText>
      </Query>
      <Fields>
        <Field Name="No_">
          <DataField>No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Vendor_Order_No_">
          <DataField>Vendor Order No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="uni_company_name">
          <DataField>uni_company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Buy_from_Vendor_No_">
          <DataField>Buy-from Vendor No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Buy_from_Vendor_Name">
          <DataField>Buy-from Vendor Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Purchaser_Code">
          <DataField>Purchaser Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ship_to_Name">
          <DataField>Ship-to Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_Code">
          <DataField>Location Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MLB_CLOSED_PL">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select c.No_ as MLB_PO, c.[Vendor Order No_] as PL_PO, c.[Buy-from Vendor No_], c.[Buy-from Vendor Name]
from
(
       select a.*
       from purchase_header_nc_vw a
       where 1=1 
       and a.uni_company_name = 'MLB Exports'
       and a.[Document Type] = 1
       and a.[Completed] = 0
) c
join
(
       select b.*
       from purchase_header_nc_vw b
       where 1=1 
       and b.uni_company_name = 'Performance Labs'
       and b.[Document Type] = 1
       and b.[Completed] = 1
) d
on c.[Vendor Order No_] = d.No_</CommandText>
      </Query>
