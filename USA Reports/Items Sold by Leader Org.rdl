  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT GENIE.TOP_MSPON, 
ITEMS.[Pv Date], 
ITEMS.[Item Code], 
ITEMS.Description, 
SUM(ITEMS.[Qty Shipped]) as QtyShipped, 
SUM(ITEMS.[Shipped Amount USD]) as SalesUsd, 
a.[Qty Shipped] as MarketQtyShipped 
FROM DW.dbo.GENIE 
INNER JOIN DW.dbo.DM_SHIPPED_SALES_PACKS ITEMS ON ITEMS.[Dist Id] = GENIE.DIST_ID 
INNER JOIN
	( SELECT ITEMS.[Pv Date], 
		ITEMS.[Item Code], 
		ITEMS.Description, 
		SUM(ITEMS.[Qty Shipped]) as [Qty Shipped], 
		SUM(ITEMS.[Shipped Amount USD]) as [Sales USD]
		FROM DW.dbo.DM_SHIPPED_SALES_PACKS ITEMS 
		WHERE 1=1 
		AND ITEMS.[Uni Mkting Unit] IN (1,2,58,63) 
		AND ITEMS.[Item Code] IN ('26933', '28332', '26996', '28330')
		GROUP BY ITEMS.[Pv Date], 
		ITEMS.[Item Code], 
		ITEMS.Description
	) a ON a.[Pv Date] = ITEMS.[Pv Date] 
	AND a.[Item Code] = ITEMS.[Item Code]
WHERE 1=1 
AND SUBSTRING(GENIE.TOP_MSPON,1,2) IN ('01','02','58','63')
AND GENIE.top_mspon ='02-97710302' 
AND ITEMS.[Item Code] IN ('26933', '28332', '26996', '28330')
GROUP BY GENIE.TOP_MSPON, 
ITEMS.[Pv Date], 
ITEMS.[Item Code], 
ITEMS.Description, 
a.[Qty Shipped] 
</CommandText>
      </Query>
