  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
       x.[Order Entry Warehouse Code],
       x.uni_company_name,
       x.[Item No_],
       Max(isnull(x.[Item Name],'')) [Item Name],
       sum(case when x.DATA_BASE='BACK_OFFICE' then x.on_hand else 0 end) BACK_OFFICE_QTY,
       sum(case when x.DATA_BASE='NAVISION' then x.on_hand else 0 end) NAVISION_QTY,
       sum(case when x.DATA_BASE='NAVISION' then x.on_hand else 0 end)-sum(case when x.DATA_BASE='BACK_OFFICE' then x.on_hand else 0 end) DIFFERENCE_QTY
from
(
       select 
             'NAVISION' DATA_BASE
                     , a.uni_company_name COLLATE DATABASE_DEFAULT uni_company_name
             , a.[Item No_] COLLATE DATABASE_DEFAULT [Item No_]
                     , a.[Description] [Item Name]
             , b.[Order Entry Warehouse Code]
             , sum(a.[Qty_ (Base)]) as on_hand
       from uninav.prod_navision.dbo.warehouse_entry_nc_vw a
              inner join uninav.prod_navision.dbo.location_nc_vw b on a.uni_company_name=b.uni_company_name and a.[Location Code]=b.code
              inner join uninav.prod_navision.dbo.item_nc_vw f on a.uni_company_name = f.uni_company_name and a.[Item No_] = f.No_
              inner join uninav.prod_navision.dbo.bin_nc_vw d on b.uni_company_name = d.uni_company_name and a.[Location Code] = d.[Location Code] and a.[Bin Code] = d.Code
              left outer join uninav.[Prod_Navision].[dbo].[Item_nc_vw] as itm on (a.uni_company_name=itm.[uni_company_name] and itm.[No_]=a.[Item No_])
              --inner join dw.dbo.dm_location_manager dlm on b.uni_company_name=dlm.dw_navision_market_name COLLATE DATABASE_DEFAULT 
       where 
              a.uni_company_name in ('Canada','Colombia','Dominican Republic','Jamaica','Mexico','Nigeria','USA','Venezuela') 
              --and a.uni_company_name in (select distinct dlm.dw_navision_market_name COLLATE DATABASE_DEFAULT from dw.dbo.dm_location_manager dlm where len(dlm.country_code)=3 and dlm.country_code&lt;&gt;'TTO' and dlm.backoffice_mkting_unit=1)
              and [Order Entry Warehouse Code] &lt;&gt; '' and f.[OE Transactable] = 1 and d.[Use for Inventory Sync] = 1
       group by 
              a.uni_company_name, a.[Item No_], a.[Description], a.[Location Code],b.[Order Entry Warehouse Code]--,a.[Bin Code]
       having 
              sum(a.[Qty_ (Base)]) &lt;&gt; 0
 
       union all
 
       select 
              'BACK_OFFICE' DATA_BASE,
              dlm.dw_navision_market_name uni_mkting_unit,
              inv.item_code,
              itd.item_name_1 [Item Name],
              inv.warehouse_number,sum(inv.quantity_on_hand) quantity_on_hand
       from 
              dw.dbo.inv 
              inner join dw.dbo.whs on whs.mkting_unit=inv.uni_mkting_unit and whs.wh_number=warehouse_number
              left outer join dw.dbo.dm_location_manager dlm on whs.mkting_unit=dlm.backoffice_mkting_unit and whs.country_code=dlm.country_code
              left outer join dw.dbo.itd on itd.uni_mkting_unit=inv.uni_mkting_unit and itd.item_code=inv.item_code COLLATE DATABASE_DEFAULT
       where 
              inv.uni_mkting_unit in (1,2,58)
       group by 
              dlm.dw_navision_market_name,inv.warehouse_number,inv.item_code,itd.item_name_1
       having 
              sum(inv.quantity_on_hand) &lt;&gt;0
) x
group by 
       x.uni_company_name,[Item No_],[Order Entry Warehouse Code]
order by 
       [Order Entry Warehouse Code],[Item No_],x.uni_company_name
 
</CommandText>
      </Query>
