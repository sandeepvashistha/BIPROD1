  <DataSets>
    <DataSet Name="OrderDetail">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartCreatedDate">
            <Value>=Parameters!StartCreatedDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndCreatedDate">
            <Value>=Parameters!EndCreatedDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  DISTINCT
  Orders.OrderID,
  Orders.CreatedDate,
  Orders.CustomerID,
  Orders.City,
  Orders.State,
  Orders.Zip,
  OrderDetails.ItemCode,
  OrderDetails.ItemDescription,
  PriceTypes.PriceTypeID,
  PriceTypes.PriceTypeDescription,
  OrderDetails.Quantity,
  OrderDetails.PriceTotal,
  OrderDetails.Tax,
  Customers.LastName,
  Customers.FirstName,
  Orders.Total,
  Orders.SubTotal,
  Orders.ShippingTotal,
  Orders.DiscountTotal,
  Orders.TaxTotal
FROM
  OrderDetails INNER JOIN Orders ON (Orders.OrderID=OrderDetails.OrderID)
   INNER JOIN Customers ON (Customers.CustomerID=Orders.CustomerID)
   INNER JOIN PriceTypes ON (PriceTypes.PriceTypeID=Orders.PriceTypeID)
WHERE Orders.CreatedDate  BETWEEN @StartCreatedDate  AND @EndCreatedDate
	AND Orders.State  IN  ( 'PR'  )
	AND Orders.OrderStatusID &gt;= 7 
GROUP BY Orders.OrderID,
  Orders.CreatedDate,
  Orders.CustomerID,
  Orders.City,
  Orders.State,
  Orders.Zip,
  OrderDetails.ItemCode,
  OrderDetails.ItemDescription,
  PriceTypes.PriceTypeID,
  PriceTypes.PriceTypeDescription,
  OrderDetails.Quantity,
  OrderDetails.PriceTotal,
  OrderDetails.Tax,
  Customers.LastName,
  Customers.FirstName,
  Orders.Total,
  Orders.SubTotal,
  Orders.ShippingTotal,
  Orders.DiscountTotal,
  Orders.TaxTotal</CommandText>
      </Query>
