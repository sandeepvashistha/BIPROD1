    <DataSet Name="OrderSummary">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <CommandText>SELECT DATEADD(month, DATEDIFF(month, 0, CreatedDate), 0) as [Month], SUM(Total) as Total, SUM(SubTotal) as SubTotal, sum(TaxTotal) as TaxTotal
, SUM(ShippingTotal) as ShippingTotal, SUM(DiscountTotal) as DiscountTotal
 FROM Orders 
 WHERE State = 'PR' AND CreatedDate &gt;= '2016-01-01'
 AND OrderStatusID &gt;= 7 
 GROUP BY  DATEADD(month, DATEDIFF(month, 0, CreatedDate), 0) 
 ORDER BY  DATEADD(month, DATEDIFF(month, 0, CreatedDate), 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
