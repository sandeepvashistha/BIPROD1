  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select 
    ODL.Item_Code,
    ODL.Item_Name_1,
    ODH.PV_Date_Date,
    Sum( Case When ODH.ODH_Type = 'C' Then -1*ODL.Qty_Ordered Else ODL.Qty_Ordered End ) As Qty_Ordered,
    Sum( Case When ODH.ODH_Type = 'C' Then -1*ODL.Qty_Shipped Else ODL.Qty_Shipped End ) As Qty_Shipped
From 
    ODH
    Inner Join ODL On ( ODH.Order_Number = ODL.Order_Number And ODH.Uni_Mkting_Unit = ODL.Mkting_Unit )
Where
    ODH.Uni_Mkting_Unit = '01'
    And
    ODH.Country_Code = 'USA'
    And
    ODH.Status in (2,3,4,5)
    And
    ODH.PV_Date_Date Between '01-Jan-2019' And '01-Nov-2019'
    And
    (
      ODL.Flag_2 = 'C'
      Or
      ODL.Flag_2 is Null
    )
Group by
    ODL.Item_Code,
    ODL.Item_Name_1,
    ODH.PV_Date_Date
Order By
    ODL.Item_Code</CommandText>
      </Query>
