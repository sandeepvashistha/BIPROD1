  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <CommandText>SELECT 
ao.AutoOrderID
, ao.CustomerID
, wh.WarehouseDescription
, ao.NextRunDate
, ad.ItemCode
, ad.ItemDescription
, iv.SellableQty
FROM 
AutoOrders ao
INNER JOIN Customers c 
	ON ao.CustomerID = c.CustomerID
INNER JOIN dbo.AutoOrderDetails ad 
	ON ao.AutoOrderID = ad.AutoOrderID
INNER JOIN Items it 
	ON ad.ItemCode = it.ItemCode
INNER JOIN unicitycustom.InventoryNAVs iv 
	ON ad.ItemCode = iv.itemSKU 
	AND ao.WarehouseID = iv.WarehouseID
INNER JOIN Warehouses wh 
	ON ao.WarehouseID = wh.WarehouseID
WHERE 
ao.AutoOrderStatusID = '0'
AND c.CustomerStatusID IN ('1','4','7')
AND ao.AutoOrderProcessTypeID = '1'
AND iv.SellableQty &lt; '1.00'
AND iv.ProductType = 'P'
AND it.ItemTypeID = '0'</CommandText>
      </Query>
      <Fields>
        <Field Name="AutoOrderID">
          <DataField>AutoOrderID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CustomerID">
          <DataField>CustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="WarehouseDescription">
          <DataField>WarehouseDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NextRunDate">
          <DataField>NextRunDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ItemCode">
          <DataField>ItemCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemDescription">
          <DataField>ItemDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SellableQty">
          <DataField>SellableQty</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
