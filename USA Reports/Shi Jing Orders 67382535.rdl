  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  DW.dbo.ODH.MORDER,
  DW.dbo.ODH.POST_DATE,
  DW.dbo.ODH.PV_DATE,
  DW.dbo.ODH.PRICE_1
FROM
  DW.dbo.ODH,
  DW.dbo.DST
WHERE
  ( DW.dbo.DST.UNI_MKTING_UNIT = DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  AND  
  DW.dbo.DST.MDIST  IN  ( '01-111627001' )</CommandText>
      </Query>
      <Fields>
        <Field Name="MORDER">
          <DataField>MORDER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="POST_DATE">
