  <DataSets>
    <DataSet Name="Cancelled">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <CommandText>SELECT  DATEADD(month, DATEDIFF(month, 0, CancelledDate), 0) as CancelledMonth
, CancelledDate
, COUNT(AutoOrderID) as Orders 
FROM AutoOrders WHERe AutoOrderStatusID = 2 AND CustomerID NOT IN ( SELECT CustomerID FROM AutoOrders WHERE AutoOrderStatusID IN (0,1) )
GROUP BY DATEADD(month, DATEDIFF(month, 0, CancelledDate), 0), CancelledDate
ORDER BY DATEADD(month, DATEDIFF(month, 0, CancelledDate), 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="CancelledMonth">
          <DataField>CancelledMonth</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CancelledDate">
          <DataField>CancelledDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Orders">
          <DataField>Orders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AutoOrders">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT d.PV_DATE
, d.POST_DATE
, COUNT(DISTINCT d.order_number) as ORDER_COUNT 
, SUM(PRICE_1) as SALES_USD
, SUM(PRICE_2) as PV_AMOUNT 
FROM DM_DSR d 
WHERE entry_init = '*AS' 
AND d.UNI_MKTING_UNIT = 1 
AND d.COUNTRY_CODE = 'USA' 
AND  d.PV_DATE &gt;= DATEADD(month,-18, DATEADD(m, DATEDIFF(m, 0, GETDATE()), 0))
GROUP  BY d.PV_DATE
, d.POST_DATE
ORDER BY d.PV_DATE</CommandText>
      </Query>
      <Fields>
        <Field Name="PV_DATE">
          <DataField>PV_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="POST_DATE">
          <DataField>POST_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ORDER_COUNT">
          <DataField>ORDER_COUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SALES_USD">
          <DataField>SALES_USD</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PV_AMOUNT">
          <DataField>PV_AMOUNT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="NewAutoOrderTemplates">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <CommandText>SELECT CreatedDateDay
, CreatedDateMonth
, DATEPART(d,CreatedDateDay) as CreatedDay
, COUNT(DISTINCT AutoOrderID) as OrderCount
, SUM(Total) as Total 
, SUM(CvTotal) as CvTotal 
, SUM(Total) / COUNT(DISTINCT AutoOrderID) as AvgSalesTotal
, SUM(CvTotal) / COUNT(DISTINCT AutoOrderID) as AvgCvTotal 
FROM 
(
	SELECT a.AutoOrderID
	, a.CustomerID
	, a.AutoOrderStatusID
	, dateadd(d, 0, datediff(d, 0, a.CreatedDate)) as CreatedDateDay
	, DATEADD(month, DATEDIFF(month, 0, a.CreatedDate), 0) as CreatedDateMonth
	, a.StartDate
	, a.StopDate
	, a.LastRunDate
	, a.NextRunDate
	, DATEADD(month, DATEDIFF(month, 0, a.CancelledDate), 0) as CancelledDate
	, b.CancelledDate as CancelledDateCheck
	, CASE WHEN b.CancelledDate = DATEADD(month, DATEDIFF(month, 0, a.CreatedDate), 0) THEN 1 END as "NewAO" 
	, a.Total
	, a.CommissionableVolumeTotal as CvTotal 
	FROM AutoOrders a
	LEFT OUTER JOIN 
		(
		SELECT AutoOrderID
		, CustomerID
		, DATEADD(month, DATEDIFF(month, 0, CreatedDate), 0) as CreatedDate
		, DATEADD(month, DATEDIFF(month, 0, CancelledDate), 0) as CancelledDate
		FROM AutoOrders 
		WHERE AutoOrderStatusID = 2 
		) b ON b.CustomerID = a.CustomerID
			AND b.CancelledDate = DATEADD(month, DATEDIFF(month, 0, a.CreatedDate), 0)
	WHERE 1=1
) c
WHERE c.NewAO IS NULL
AND  CreatedDateMonth &gt;=GETDATE()-180
GROUP BY CreatedDateDay
, CreatedDateMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="CreatedDateDay">
          <DataField>CreatedDateDay</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreatedDateMonth">
          <DataField>CreatedDateMonth</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CreatedDay">
          <DataField>CreatedDay</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OrderCount">
          <DataField>OrderCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CvTotal">
          <DataField>CvTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgSalesTotal">
          <DataField>AvgSalesTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgCvTotal">
          <DataField>AvgCvTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
