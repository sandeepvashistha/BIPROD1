  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT 
	ODH.ship_via_code,
  --ODH.ship_via_desc,
  FSV.DESCRIPTION ship_via_description,
  ODH.Odh_Type,
  --ODS.DESCRIPTION Odh_Type_Description,
	ODH.PV_DATE_DATE AS PV_DATE,
	ODH.VERIFY_DATE,
	DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0) VERIFY_MONTH,
	(CASE 	WHEN DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0)=DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()), 0) THEN 'CURRENT'
			WHEN DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0)=DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE())-1, 0) THEN 'LAST'
			ELSE 'HISTORY' END
	) AS FILTER_MONTH,
	SUM(CASE WHEN ODH.Odh_Type='C' THEN -1 ELSE 1 END) AS ORDER_COUNT
FROM DW.DBO.ODH ODH
LEFT OUTER JOIN DW.dbo.ODS ON (ODH.UNI_MKTING_UNIT = ODS.MKTING_UNIT and ODH.ORDER_SOURCE = ODS.ODS_NUMBER)
LEFT OUTER JOIN DW.dbo.FSV ON (ODH.UNI_MKTING_UNIT = FSV.UNI_MKTING_UNIT AND ODH.SHIP_VIA_CODE = FSV.SHIP_VIA_CODE AND ODH.WAREHOUSE=FSV.WAREHOUSE_NUMBER AND ODH.SHIP_TO_COUNTRY=FSV.SHIP_TO_COUNTRY)
WHERE ODH.UNI_MKTING_UNIT=60 AND DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0)&gt;=DATEADD(YEAR, DATEDIFF(YEAR, 0,DATEADD(YEAR, -1, GETDATE())), 0)
AND STATUS IN (2,3,4,5)
GROUP BY
	ODH.ship_via_code,
  	FSV.DESCRIPTION,
	ODH.Odh_Type,
	ODH.PV_DATE_DATE,
	ODH.VERIFY_DATE,
	DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0),
	(CASE 	WHEN DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0)=DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()), 0) THEN 'CURRENT'
			WHEN DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.VERIFY_DATE), 0)=DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE())-1, 0) THEN 'LAST'
			ELSE 'HISTORY' END
	)</CommandText>
      </Query>
