  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.DIST_ID,
  DW.dbo.DST.GOVERNMENT_ID,
  DW.dbo.DST.ENTRY_INIT,
  DW.dbo.DST.ROMAN_NAME_1,
  DW.dbo.ODL.ORDER_NUMBER,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.DST.COUNTRY_CODE As Dist_Country,
  DW.dbo.DST.UNI_MKTING_UNIT As Dist_Market,
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODH.UNI_MKTING_UNIT As Order_Market,
  DW.dbo.ODH.COUNTRY_CODE As Order_Country,
  CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103) As PV_Month,
  DW.dbo.ODH.record_date,
  DW.dbo.ODL.QTY_ORDERED,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_4) As Comm_Volume,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_2) As PV,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_1) As Sales_Amount
FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.ODL
WHERE
  ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER  )
  AND  ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID  )
  AND  
  (
   DW.dbo.ODH.WAREHOUSE  IN  ( 4  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 65  )
   AND
   CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103) Between @Start_PV_Month and @End_PV_Month  --@Prompt
   AND
   DW.dbo.ODH.STATUS  IN  ( 5  )
   --And
   --DW.dbo.ODL.ORDER_NUMBER = '415912786'
  )
GROUP BY
  DW.dbo.ODH.DIST_ID, 
  DW.dbo.DST.GOVERNMENT_ID, 
  DW.dbo.DST.ENTRY_INIT, 
  DW.dbo.DST.ROMAN_NAME_1, 
  DW.dbo.ODL.ORDER_NUMBER, 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.DST.UNI_MKTING_UNIT,
  DW.dbo.ODL.ITEM_CODE, 
  DW.dbo.ODH.UNI_MKTING_UNIT, 
  DW.dbo.ODH.COUNTRY_CODE,
  CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103), 
  DW.dbo.ODH.record_date, 
  DW.dbo.ODL.QTY_ORDERED</CommandText>
      </Query>
