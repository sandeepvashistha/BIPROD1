  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@VendorNo">
            <Value>=Parameters!VendorNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BeginPostingDate">
            <Value>=Parameters!BeginPostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPostingDate">
            <Value>=Parameters!EndPostingDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	[purch_inv_line_nc_vw].[No_] [Item_No],
	[purch_inv_line_nc_vw].[Description] [Item_Name],
	cast([purch_inv_line_nc_vw].[Quantity] as numeric(20,0)) [Quantity],
	[purch_inv_line_nc_vw].[Line Amount] [Amount USD],
	[purch_inv_header_nc_vw].[Currency Factor],
	[purch_inv_line_nc_vw].[Line Amount] [Amount Local],
	(case when [purch_inv_header_nc_vw].[Currency code]='' then 'LOCAL' else [purch_inv_header_nc_vw].[Currency code] end) [Currency code],
	[purch_inv_header_nc_vw].[No_] [Invoice_Number],
	cast([purch_inv_header_nc_vw].[Posting Date] as date) [Posting Date],
	[purch_inv_header_nc_vw].[Buy-from Vendor No_]
	--[purch_inv_header_nc_vw].*
from 
	prod_navision.[dbo].[purch_inv_header_nc_vw]
	left outer join prod_navision.[dbo].[purch_inv_line_nc_vw] 
	on [purch_inv_line_nc_vw].uni_company_name=[purch_inv_header_nc_vw].uni_company_name and [purch_inv_line_nc_vw].[Document No_]=[purch_inv_header_nc_vw].[No_]
	and [purch_inv_line_nc_vw].[Buy-from Vendor No_]=[purch_inv_header_nc_vw].[Buy-from Vendor No_]
--left outer join prod_navision.[dbo].[vendor_nc_vw] on [vendor_nc_vw].uni_company_name=[purch_inv_header_nc_vw].uni_company_name and [vendor_nc_vw].[No_]=[purch_inv_header_nc_vw].[Buy-from Vendor No_]
where 
	[purch_inv_header_nc_vw].uni_company_name='Malaysia' and [purch_inv_line_nc_vw].[Type]=2
	and [purch_inv_header_nc_vw].[Source Code]='PURCHASES' and [purch_inv_header_nc_vw].[Buy-from Vendor No_]=@VendorNo-- in ('IC001','PL')
	and cast([purch_inv_header_nc_vw].[Posting Date] as date) between @BeginPostingDate and @EndPostingDate --'2019-01-01' and '2020-01-31'
	--and [purch_inv_header_nc_vw].no_ in ('PI1014425','PI1014601','PI1014981','PI1015158');</CommandText>
      </Query>
