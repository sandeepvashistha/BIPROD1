    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BeginPVDate">
            <Value>=Parameters!BeginPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OrderSource">
            <Value>=Parameters!OrderSource.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select odh.pv_date_date,odh.country_code,odl.item_code,itd.item_name_1,odh.odh_type,
odh.order_source,cast(odh.order_source as varchar)+' - '+ods.description ods_description,
sum(case when odh.odh_type='C' then -1*odl.qty_ordered else odl.qty_ordered end) qty_ordered
from
dw.dbo.odh 
inner join dw.dbo.odl on odh.morder=odl.morder
inner join dw.dbo.ods on ods.ods_number=odh.order_source and ods.mkting_unit=odh.uni_mkting_unit
inner join dw.dbo.itd on itd.uni_mkting_unit=odl.mkting_unit and itd.item_code=odl.item_code
where
odh.uni_mkting_unit=60 and odh.status in (2,3,4,5)
and (odl.flag_2 is null or odl.flag_2='C') --and odh.odh_type='C'
and odh.pv_date_date between @BeginPVDate and  @EndPVDate
and ( odl.item_code in  (@ItemCode) or ( ('All') in (@ItemCode) and 1=1))
and odh.order_source in (@OrderSource)
group by
odh.pv_date_date,odh.country_code,odl.item_code,itd.item_name_1,odh.odh_type,
odh.order_source,cast(odh.order_source as varchar)+' - '+ods.description
order by
odh.country_code,odh.pv_date_date,odl.item_code,odh.order_source</CommandText>
      </Query>
      <Fields>
        <Field Name="pv_date_date">
          <DataField>pv_date_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="item_code">
          <DataField>item_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="item_name_1">
          <DataField>item_name_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="odh_type">
          <DataField>odh_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="order_source">
          <DataField>order_source</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ods_description">
          <DataField>ods_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="qty_ordered">
          <DataField>qty_ordered</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ODS_Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select ods_number,cast(ods_number as varchar)+' - '+description ods_desc 
from dw.dbo.ods where mkting_unit=60 --and ods_number&gt;900
order by ods_number</CommandText>
      </Query>
      <Fields>
        <Field Name="ods_number">
          <DataField>ods_number</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ods_desc">
          <DataField>ods_desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
