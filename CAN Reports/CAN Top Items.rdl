    <DataSet Name="Autoships">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT PV_DATE
, COUNT(ORDER_NUMBER) as ORDERS
, COUNT(DISTINCT DIST_ID) as DISTS
, SUM(PRICE_1) as SALES
, SUM(PRICE_2) as PV 
, SUM(PRICE_2) / COUNT(DISTINCT DIST_ID) as AVG_DIST
, SUM(PRICE_2) / COUNT(DISTINCT ORDER_NUMBER) as AVG_ORDER
, SUM(PRICE_1) / COUNT(DISTINCT DIST_ID) as AVG_SALE_ORDER
FROM DW.dbo.DM_DSR 
WHERE UNI_MKTING_UNIT = 2
AND ENTRY_INIT = '*AS' 
AND DATEDIFF(month,pv_date,getdate()) &lt;= 13
GROUP BY PV_DATE</CommandText>
      </Query>
