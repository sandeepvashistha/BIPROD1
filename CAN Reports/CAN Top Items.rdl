  <DataSets>
    <DataSet Name="CA_Items_Qty">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT [PV Date]
,  [Item Code] 
,  [Description]
, [Qty Shipped] 
, CAST([Sales USD] as NUMERIC) as Sales_USD 
FROM 
(
	SELECT * 
	, DENSE_RANK() OVER(PARTITION BY [Pv Date] ORDER BY [Qty Shipped] DESC) as qty_rw
	, DENSE_RANK() OVER(PARTITION BY [Pv Date] ORDER BY [Sales USD] DESC) as sales_rw
	FROM 
	(
	SELECT [PV Date]
	, [Item Code] 
	, [Description]
	, SUM([Qty Shipped]) as "Qty Shipped" 
	, ROUND(SUM([Shipped Amount USD]),2) as "Sales USD"  
	FROM DM_SHIPPED_SALES_PACKS
	WHERE DATEDIFF(month,[Pv Date],DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0)) &lt;= 13 
	AND [Uni Mkting Unit] = 2
	AND [Country Code] = 'CAN' 
	AND [Sales Category] = 'P' 
	GROUP BY [PV Date]
	, [Item Code] 
	, [Description]
	) a
) b 


ORDER BY 4 DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="PV_Date">
          <DataField>PV Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Code">
          <DataField>Item Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qty_Shipped">
          <DataField>Qty Shipped</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Sales_USD">
          <DataField>Sales_USD</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
