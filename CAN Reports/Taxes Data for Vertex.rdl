  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartOrderDate">
            <Value>=Parameters!StartOrderDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndOrderDate">
            <Value>=Parameters!EndOrderDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  Orders.OrderID,
  Orders.CurrencyCode,
  Orders.Country,
  OrderStatuses.OrderStatusID,
  Orders.City,
  Orders.State,
  Orders.OrderDate,
  Orders.TaxTotal,
  Orders.ShippingTax,
  Orders.OrderTax,
  Orders.FedTaxTotal,
  Orders.StateTaxTotal,
  Orders.FedShippingTax,
  Orders.StateShippingTax,
  Orders.CityShippingTax,
  Orders.CityLocalShippingTax,
  Orders.CountyShippingTax,
  Orders.CountyLocalShippingTax,
  Orders.Total,
  Orders.SubTotal,
  case 
when Orders.City like '%AB%' Then 'AB' 
when Orders.City like '%BC%' Then 'BC' 
when Orders.City like '%MB%' Then 'MB' 
when Orders.City like '%NB%' Then 'NB' 
when Orders.City like '%NL%' Then 'NL' 
when Orders.City like '%NS%' Then 'NS' 
when Orders.City like '%NT%' Then 'NT' 
when Orders.City like '%ON%' Then 'ON' 
when Orders.City like '%QC%' Then 'QC' 
when Orders.City like '%SK%' Then 'SK' 
else Orders.State
END 
FROM
  Orders INNER JOIN OrderStatuses ON (OrderStatuses.OrderStatusID=Orders.OrderStatusID)
  
WHERE
  (
   Orders.Country  =  'CA'
   AND
   OrderStatuses.OrderStatusID  &gt;  6
   AND
   Orders.OrderDate  BETWEEN  @StartOrderDate  AND  @EndOrderDate
   AND
   Orders.ShipMethodID  NOT IN  ( 99  )
  )</CommandText>
      </Query>
