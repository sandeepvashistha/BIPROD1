  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  Table__1.ITEM_CODE,
  Table__1.ITEM_NAME_1,
  Table__1.WAREHOUSE,
  Table__1.AVERAGE_SOLD_PER_WEEK,
  Table__1.QTY_ON_HAND,
  Table__1.WEEKS_ON_HAND
FROM
( 
SELECT a.item_code
, d.item_name_1
, 1 as warehouse
, b.average_sold_per_week  
, ISNULL(c.quantity_on_hand,0) as qty_on_hand
, ISNULL(c.quantity_on_hand,0)/b.average_sold_per_week as weeks_on_hand
FROM 
(
  SELECT l.item_code, l.mitem
  FROM DW.dbo.ODH h 
  INNER JOIN DW.dbo.ODL l ON h.morder = l.morder 
  WHERE 1=1 
  AND h.status IN (2,3,4,5) 
  AND h.post_date &gt;= GetDate() -180 
  AND h.uni_mkting_unit = 2 
  AND (l.FLAG_2 &lt;&gt; 'M' OR l.flag_2 IS NULL) 
  GROUP BY l.item_code, l.mitem 
  ) a 
LEFT OUTER JOIN 
  (
  SELECT
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODL.mitem, 
  SUM(DW.dbo.ODL.QTY_ORDERED)/26 as average_sold_per_week
  FROM
    DW.dbo.ODL,
    DW.dbo.ODH
  WHERE
    ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER  )
    AND  
    (
      DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 2  )
      AND DW.dbo.ODH.POST_DATE  &gt;=  GetDate() - 183
    )
  GROUP BY 
    DW.dbo.ODL.ITEM_CODE,
    DW.dbo.ODL.mitem
  ) b ON b.mitem = a.mitem 
LEFT OUTER JOIN 
  (
    SELECT DW.dbo.INV.item_code, DW.dbo.INV.mitem, DW.dbo.INV.warehouse_number, DW.dbo.INV.quantity_on_hand
    FROM DW.dbo.INV 
   ) c ON c.mitem COLLATE Latin1_General_CI_AS = a.mitem AND c.warehouse_number = 1
INNER JOIN 
  (
   SELECT 
   DW.dbo.ITD.item_code, DW.dbo.ITD.mitem,  DW.dbo.ITD.item_name_1
   FROM 
   DW.dbo.ITD
   WHERE DW.dbo.ITD.sales_category = 'P' 
   ) d ON d.mitem COLLATE Latin1_General_CI_AS = a.mitem
)  Table__1</CommandText>
      </Query>
