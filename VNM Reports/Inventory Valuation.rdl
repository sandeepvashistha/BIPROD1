<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ProdNavision">
      <DataSourceReference>ProdNavision</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2804d22c-bc72-4cfc-9f63-b919a3d695aa</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostingDate">
            <Value>=Parameters!PostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Uni_Company_Name">
            <Value>=Parameters!Uni_Company_Name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.uni_company_name, c.[Location Code], b.[No_], b.description, b.[Base Unit of Measure], b.[Inventory Posting Group],cast(c.[Remaining Quantity] as decimal(17,2)) [Remaining Quantity],cast(c.[Unit Cost] as decimal(17,2)) [Unit Cost],cast(c.[Inventory Value] as decimal(17,2)) [Inventory Value], b.[Vendor No_], b.buyer 
from [Prod_navision].dbo.[Item_nc_vw] b with (NOLOCK)
join (
  select a.[Item No_]
      , a.[Location Code]
      , sum(a.[Item Ledger Entry Quantity]) as [Remaining Quantity]
      , case when sum(a.[Item Ledger Entry Quantity])=0 then 0 else sum(a.[Cost Amount (Actual)]+a.[Cost Amount (Expected)])/sum(a.[Item Ledger Entry Quantity]) end as [Unit Cost]
      , sum(a.[Cost Amount (Actual)]+a.[Cost Amount (Expected)]) as [Inventory Value]
      , a.uni_company_name
  from [Prod_navision].dbo.[value_entry_nc_vw] a with (NOLOCK)
  where 1=1
  and 
a.[Posting Date] &lt;= @PostingDate
  group by a.[Item No_],a.[Location Code],a.uni_company_name
  having sum(a.[Item Ledger Entry Quantity]) &lt;&gt; 0 or sum(a.[Cost Amount (Actual)]+a.[Cost Amount (Expected)]) &lt;&gt; 0
) c
on c.[Item No_] = b.[No_]
  and c.uni_company_name = b.uni_company_name 
where 1=1
  --and c.uni_company_name = 'Vietnam'
--and c.uni_company_name in (@Uni_Company_Name)
and c.uni_company_name in ('Vietnam','Vietnam Manufacturing')
and c.uni_company_name in (@Uni_Company_Name)
--  and c.[Inventory Value] &gt; c.[Unit Cost]
--  and c.[Remaining Quantity] &lt;&gt; 0
--  and c.[Item No_] = '26088'
order by c.[Location Code], b.[No_]</CommandText>
      </Query>
