  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4ec27342-2c54-4c28-82a1-f976e5ad3f1e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  DST.DIST_ID,
  DST.ROMAN_NAME_1,
  DST.ENTRY_DATE,
  DST.DIST_STATUS,
  MAX(ISNULL(ODH.ENTRY_DATE,'1999-01-01')) as order_entry_date
FROM
  DW.DBO.DST DST 
  LEFT OUTER JOIN DW.DBO.ODH ODH ON
  (DST.dist_id=ODH.dist_id AND ODH.STATUS &lt;&gt; 9 AND ODH.ODH_TYPE &lt;&gt; 'C')
WHERE
   DST.UNI_MKTING_UNIT  =  84
   AND   DST.DIST_STATUS  IN  ('A','S')
   AND   DST.ENTRY_DATE  &lt;  Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, GETDATE())), 0)
   AND ISNULL(UPPER(DST.ROMAN_NAME_1),'A') NOT LIKE '%PLACEHOLDER%'
GROUP BY
	DST.DIST_ID,
	DST.ROMAN_NAME_1,
	DST.ENTRY_DATE,
	DST.DIST_STATUS
	having MAX(ISNULL(ODH.ENTRY_DATE,'1999-01-01'))&lt;Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, GETDATE())), 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ROMAN_NAME_1">
          <DataField>ROMAN_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ENTRY_DATE">
          <DataField>ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DIST_STATUS">
          <DataField>DIST_STATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="order_entry_date">
          <DataField>order_entry_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
