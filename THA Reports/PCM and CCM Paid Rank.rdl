  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>BISQL01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PCMPVDate">
            <Value>=Parameters!PCMPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartPVDate">
            <Value>=Parameters!StartPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  GENIE.TOP_MSPON,
  SAPDW_GENIE2.ROMAN_NAME_1 TOP_DIST_NAME,
  GENIE.MDIST,
  DST.ROMAN_NAME_1 DIST_NAME,
  GENIE.GROUP_LEVEL,
  btr.pv_date,
  count(distinct BTR.sold_dist_id)
FROM
  DW.dbo.GENIE_TEST GENIE INNER JOIN DW.DBO.DST SAPDW_GENIE2 ON (GENIE.TOP_MSPON=SAPDW_GENIE2.MDIST)
  INNER JOIN DW.DBO.DST ON (DST.DIST_ID=GENIE.DIST_ID)
  INNER JOIN DW.DBO.DM_PCM ON (SAPDW_GENIE2.DIST_ID=DM_PCM.ID AND pv_date=@PCMPVDate)
  INNER JOIN DW.DBO.BTR ON (BTR.mdist=GENIE.MDIST and btr.target_country = 'THA' and CONVERT(DATE,LTRIM(RTRIM(btr.pv_date))+'01',103) between @StartPVDate and @EndPVDate and btr.bonus_type = '8')
--WHERE
   --GENIE.TOP_MSPON  = '66-2996566' --AND GENIE.MDIST='66-103319366'
GROUP BY GENIE.TOP_MSPON,
  SAPDW_GENIE2.ROMAN_NAME_1,
  GENIE.MDIST,
  DST.ROMAN_NAME_1,
  GENIE.GROUP_LEVEL,
  btr.pv_date;</CommandText>
      </Query>
