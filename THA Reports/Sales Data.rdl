  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Market_No">
            <Value>=Parameters!Market_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Order_Country">
            <Value>=Parameters!Order_Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.COUNTRY_CODE,
  DW.dbo.ODH.Order_number,
  DW.dbo.ODH.POST_DATE,
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) As PV_Date,
  DW.dbo.ODH.PV_YEAR,
  DW.dbo.ODH.STATUS,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.ODL.QTY_ORDERED,
  Sum(DW.dbo.ODL.QTY_ORDERED) as Total_Qty,
  DW.dbo.ODL.ITEM_CODE,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_1) As Amount_Local,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_1 /  ISNULL(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_amount,1)) As Amount_USD
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' )
WHERE
  (
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  BETWEEN  @Start_PV_Month and @End_PV_Month
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  =  @Market_No
   AND
   DW.dbo.ODH.COUNTRY_CODE  =  @Order_Country
   AND
   (
    DW.dbo.ODL.FLAG_2  &lt;&gt;  'M'
    OR
    DW.dbo.ODL.FLAG_2  Is Null  
   )
  )
GROUP BY
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.Order_number, 
  DW.dbo.ODH.POST_DATE, 
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)), 
  DW.dbo.ODH.PV_YEAR, 
  DW.dbo.ODH.STATUS, 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.ODL.QTY_ORDERED, 
  DW.dbo.ODL.ITEM_CODE</CommandText>
      </Query>
