  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) As PV_Month,
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ITD.ITEM_NAME_1,
  DW.dbo.ODL.FLAG_2,
  Sum(DW.dbo.ODL.QTY_ORDERED) As Total_Qty,
  DW.dbo.IPL.PRICE_2 As Qualifying_Volume,
  DW.dbo.IPL.PRICE_LEVEL,
  DW.dbo.IPL.PRICE_1 As Selling_Price
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.MORDER=DW.dbo.ODL.MORDER )
  LEFT OUTER JOIN DW.dbo.ITD ON ( DW.dbo.ODL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS = DW.dbo.ITD.MITEM )
  LEFT OUTER JOIN DW.dbo.IPL ON ( DW.dbo.IPL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS =DW.dbo.ODL.MITEM AND DW.dbo.ODL.PRICE_LEVEL=DW.dbo.IPL.PRICE_LEVEL )
WHERE
  (
   DW.dbo.ODH.STATUS  IN  ( 3, 4, 5 )
   AND
   --(CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  =  '01-FEB-2016'   --@Prompt
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) Between @Start_PV_Month and @End_PV_Month
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 66 )
   AND
   DW.dbo.IPL.PRICE_LEVEL  &lt;&gt;  1
   AND
   DW.dbo.ODH.CURRENCY_CODE  IN  ( 'THB' )
   AND
   DW.dbo.ODH.WAREHOUSE  =  1
  )
Group By
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)),
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ITD.ITEM_NAME_1,
  DW.dbo.ODL.FLAG_2,
  --Sum(DW.dbo.ODL.QTY_ORDERED) As Total_Qty,
  DW.dbo.IPL.PRICE_2,
  DW.dbo.IPL.PRICE_LEVEL,
  DW.dbo.IPL.PRICE_1</CommandText>
      </Query>
