    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>3f9123a8-ece0-4b42-a904-0bd2779266d4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) as PV_Month,
  DW.dbo.ODH.COUNTRY_CODE,
  sum(DW.dbo.ODH.price_1 /  ISNULL(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) as Amount_USD,
  DW.dbo.ODH.ODH_TYPE
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' )
WHERE
  (
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'THA','MMR','LAO','KHM'  )
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103))  &gt;=  @Start_PV_Month
  )
GROUP BY
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)), 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.ODH_TYPE</CommandText>
      </Query>
      <Fields>
