  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>44bd1c99-cc0b-4036-bc6f-4473ff63f59c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Date">
            <Value>=Parameters!PV_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  ODH.DIST_ID,
  DST.ROMAN_NAME_1,
  ODH.COUNTRY_CODE as Order_Country_Code,
  ODH.ODH_TYPE,
  IPL.PRICE_LEVEL,
  PLD.DESCRIPTION,
  --ODL.PRICE_1,
  (case when ODH.ODH_TYPE='C' then -1*ODL.PRICE_1 ELSE ODL.PRICE_1 END) as PRICE_1,
  ODL.ITEM_CODE,
  ITD.ITEM_NAME_1,
  --ODL.QTY_ORDERED,
  (case when ODH.ODH_TYPE='C' then -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) as QTY_ORDERED,
  ODH.ENTRY_DATE,
  ODH.PV_DATE,
  ODL.ORDER_NUMBER,
  odl.flag_2,
  --(ODL.QTY_ORDERED*ODL.price_1) 
  (case when ODH.ODH_TYPE='C' then -1*(ODL.QTY_ORDERED*ODL.price_1) ELSE (ODL.QTY_ORDERED*ODL.price_1) END) as sales_thb

FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.IPL,
  DW.dbo.PLD,
  DW.dbo.ODL,
  DW.dbo.ITD
WHERE
  ( ODH.MORDER=ODL.MORDER  )
  AND  ( ODL.MITEM=ITD.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS )
  AND  ( IPL.MITEM=ODL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS  )
  AND  ( ODL.PRICE_LEVEL=IPL.PRICE_LEVEL  )
  AND  ( DST.DIST_ID=ODH.DIST_ID  )
  AND  ( IPL.MKTING_UNIT=PLD.MKTING_UNIT and IPL.PRICE_LEVEL=PLD.PRICE_LEVEL  )
  AND  ( ODH.UNI_MKTING_UNIT  IN (60,62,63,65,66,81,82,85,88,84)  )  AND  ( (DST.UNI_MKTING_UNIT  IN (60,62,63,65,66,81,82,85,88,84) OR DST.HFLAG_2 = 'X')  )  AND  ( ODL.MKTING_UNIT IN (60,62,63,65,66,81,82,85,88,84)  )
  AND  
  (
   ODH.COUNTRY_CODE  =  'THA'
   AND
   ODH.STATUS  in ( 2, 3, 4, 5  )
   AND
   DST.COUNTRY_CODE  IN  ( 'KHM'  )
   AND
   ODL.ITEM_CODE NOT IN ('29480','27960','27972','30190','27976','30336','23426','27964','27966','30915','30638')
   AND
   ODH.ODH_TYPE  IN  ( 'I','C'  )
   AND
   ODH.PV_DATE_DATE = @PV_Date
  )
GROUP BY
  ODH.DIST_ID, 
  DST.ROMAN_NAME_1, 
  ODH.COUNTRY_CODE, 
  ODH.ODH_TYPE, 
  IPL.PRICE_LEVEL, 
  PLD.DESCRIPTION, 
  ODL.PRICE_1, 
  ODL.ITEM_CODE, 
  ITD.ITEM_NAME_1, 
  ODL.QTY_ORDERED, 
  ODH.ENTRY_DATE, 
  ODH.PV_DATE, 
  ODL.ORDER_NUMBER,
  odl.flag_2</CommandText>
      </Query>
