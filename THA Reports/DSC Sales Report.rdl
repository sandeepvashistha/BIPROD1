  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MarketUnit">
            <Value>=Parameters!MarketUnit.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	a.BackOffice_Unit,
	a.WH,
	cast(getdate() as date) today,
	cast(getdate()-1 as date) yesturday,
	sum(a.ITX_Begin) ITX_Begin,
	cast(sum(a.NAV_Shipment) as numeric(10,0)) NAV_Shipment,
	sum(a.ITX_END) ITX_END,
	(sum(a.ITX_Begin)+sum(a.NAV_Shipment))-sum(a.ITX_END) ITX_Sales,
	sum(a.NAV_Sales) NAV_Sales,
	cast(sum(a.NAV_Adjustment) as numeric(10,0)) NAV_Adjustment,
	((sum(a.ITX_Begin)+sum(a.NAV_Shipment))-sum(a.ITX_END)) - (sum(a.NAV_Sales)-sum(a.NAV_Adjustment)) Different
from
(
select 
	[Actual BackOffice Unit] BackOffice_Unit, 
	[Order Entry Warehouse Code] WH,
	0 ITX_Begin,
	0 NAV_Shipment,
	0 ITX_END,
	0 NAV_Sales,
	0 NAV_Adjustment
from UNINAV.PROD_NAVISION.DBO.location_nc_vw b
join UNINAV.PROD_NAVISION.DBO.company_information_nc_vw c
on b.uni_company_name = c.uni_company_name
where [Order Entry Warehouse Code] &lt;&gt; '' --must connect to InfoTrax
and b.[DSC Warehouse Flag] = 1 --dsc's only
and b.uni_company_name = @CompanyName
union
select 
	uni_mkting_unit BackOffice_Unit,
	warehouse_number WH,
	sum(quantity_on_hand) ITX_Begin,
	0 NAV_Shipment,
	0 ITX_END,
	0 NAV_Sales,
	0 NAV_Adjustment
from dw.dbo.inv_yesterday
where cast(warehouse_number as varchar) in 
(select [order Entry Warehouse Code] from UNINAV.PROD_NAVISION.DBO.location_nc_vw where [DSC Warehouse Flag] = 1 and [order Entry Warehouse Code]&lt;&gt;'' and uni_company_name=@CompanyName)
--in (305,306,307,308)
and inv_yesterday.uni_mkting_unit=@MarketUnit
group by uni_mkting_unit, warehouse_number
union
--NAV shipment field #2, same date as posting date.
select 
	[Actual BackOffice Unit] BackOffice_Unit, 
	[Order Entry Warehouse Code] WH,
	0 ITX_Begin,
	sum(Quantity) NAV_Shipment,
	0 ITX_END,
	0 NAV_Sales,
	0 NAV_Adjustment
from UNINAV.PROD_NAVISION.DBO.item_ledger_entry_nc_vw a
join UNINAV.PROD_NAVISION.DBO.location_nc_vw b
on a.uni_company_name = b.uni_company_name
and a.[Location Code] = b.[Code]
join UNINAV.PROD_NAVISION.DBO.company_information_nc_vw c
on b.uni_company_name = c.uni_company_name
where [Order Entry Warehouse Code] &lt;&gt; ''
and a.uni_company_name = @CompanyName --remove for final query
and a.[Entry Type] = 1 --sales
and a.[Document Type] = 1 --sales shipment
and b.[DSC Warehouse Flag] = 1 --dsc's only
and cast(a.[Posting Date] as date) = cast(getdate() as date) --'14-MAY-2018'--should be yesterday
group by [Actual BackOffice Unit], [Order Entry Warehouse Code]
union
--ITX End, field #3
select 
	uni_mkting_unit BackOffice_Unit,
	warehouse_number WH,
	0 ITX_Begin,
	0 NAV_Shipment,
	sum(quantity_on_hand) ITX_END,
	0 NAV_Sales,
	0 NAV_Adjustment
from dw.dbo.inv
where cast(warehouse_number as varchar) in 
(select [order Entry Warehouse Code] from UNINAV.PROD_NAVISION.DBO.location_nc_vw where [DSC Warehouse Flag] = 1 and [order Entry Warehouse Code]&lt;&gt;'' and uni_company_name=@CompanyName)
--in (305,306,307,308)
and inv.uni_mkting_unit=@MarketUnit
group by uni_mkting_unit, warehouse_number
union
select 
	[Market Number], 
	[Warehouse ID],
	0 ITX_Begin,
	0 NAV_Shipment,
	0 ITX_END, 
	sum([Transaction Quantity]) NAV_Sales,
	0 NAV_Adjustment
from UNINAV.PROD_NAVISION.DBO.stg_depl_inv_depl_itrx_dk_nc_vw
where 1=1
and [Market Number] = @MarketUnit and [Warehouse ID] in 
(select [order Entry Warehouse Code] from UNINAV.PROD_NAVISION.DBO.location_nc_vw where [DSC Warehouse Flag] = 1 and [order Entry Warehouse Code]&lt;&gt;'' and uni_company_name=@CompanyName)
--(305,307,308) --remove for final query
and cast([Transaction Date] as date) = cast(getdate()  as date) --'14-MAY-2018' --should be yesterday
group by [Market Number], [Warehouse ID]
union
select 
    [Actual BackOffice Unit] BackOffice_Unit,
	[Order Entry Warehouse Code] WH,
	0 ITX_Begin,
	0 NAV_Shipment,
	0 ITX_END,
	0 NAV_Sales,
	sum(Quantity) NAV_Adjustment
from UNINAV.PROD_NAVISION.DBO.item_ledger_entry_nc_vw a
join UNINAV.PROD_NAVISION.DBO.location_nc_vw b
on a.uni_company_name = b.uni_company_name
and a.[Location Code] = b.[Code]
join UNINAV.PROD_NAVISION.DBO.company_information_nc_vw c
on b.uni_company_name = c.uni_company_name
where [Order Entry Warehouse Code] &lt;&gt; ''
and a.uni_company_name = @CompanyName --remove for final query
and a.[Entry Type] in (2,3) --Positive adjustments and Negative adjustments
and b.[DSC Warehouse Flag] = 1 --dsc's only
and cast(a.[Posting Date] as date) = cast(getdate()  as date)--should be yesterday
group by [Actual BackOffice Unit], [Order Entry Warehouse Code]
) a
group by a.BackOffice_Unit,a.WH</CommandText>
      </Query>
