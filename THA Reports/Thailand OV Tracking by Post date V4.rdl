  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostDate">
            <Value>=Parameters!PostDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DST.MDIST AS TOP_MSPON,
  DST.ROMAN_NAME_1 AS TOP_MSPON_NAME,
  ODH.PV_DATE_DATE AS PV_DATE,
  ODH.POST_DATE,
  SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*PRICE_2 ELSE PRICE_2 END) AS OWN_PV,
  0 AS DOWNLINE_PV
FROM
  DW.dbo.DST
  left outer join DW.dbo.ODH on (DST.DIST_ID=ODH.DIST_ID AND ODH.STATUS IN (2,3,4,5) 
  AND ODH.POST_DATE between DATEADD(month, DATEDIFF(month, 0, @PostDate), 0) and @PostDate) --@prompt
WHERE
(
   DST.MDIST in 
   ('66-2667166','66-2696966','66-2757766','66-2765166','66-2808766','66-2962166','66-2965766','66-2965866','66-2996566',
'66-3011266','66-3053866','66-3065766','66-3072666','66-3121366','66-3138266','66-3141266','66-3148066','66-3341366',
'66-3393766','66-5710366','66-5776966','66-5801766','66-8444566','66-8455966','66-95136066','66-95208466','66-95386966',
'81-95387166','66-95652666','66-95763666','66-96250466','66-96320266','66-96590666','66-96601366','66-96788066','66-96901666',
'66-96911366','66-96936066','66-97807566','66-97839466','66-98014266','66-100280466','66-100711566','66-100919566',
'66-102506566','66-105640066','66-106679566','66-107245566','66-109999266','66-112287566','66-112399566','66-115091866',
'66-116456466','66-122339366','66-123276566','66-132163166','66-135655966','66-142287666','66-172026066','66-174504466',
'66-174576366','66-189838066','66-190474366','66-231135466','84-435813584','81-95387166','66-123275966','84-435813584')
   AND DST.UNI_MKTING_UNIT in ('66','81','84')
  )
GROUP BY  
  DST.MDIST,
  DST.ROMAN_NAME_1,
  ODH.PV_DATE_DATE,
  ODH.POST_DATE
  HAVING  SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*PRICE_2 ELSE PRICE_2 END) IS NOT NULL
UNION ALL
--The query is for TOP SPONSOR DOWNLINE PV
SELECT
  GENIE.TOP_MSPON,
  TOPMSPON_NAME.ROMAN_NAME_1,
  ODH.PV_DATE_DATE AS PV_Month,
  ODH.POST_DATE,
  0 AS OWN_PV,
  SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*PRICE_2 ELSE PRICE_2 END) AS DOWNLINE_PV
FROM
  DW.dbo.GENIE inner join DW.dbo.DST  TOPMSPON_NAME on ( GENIE.TOP_MSPON=TOPMSPON_NAME.MDIST  )
  inner join DW.dbo.DST on ( GENIE.DIST_ID=DST.DIST_ID  )
  left outer join DW.dbo.ODH on (DST.DIST_ID=ODH.DIST_ID AND ODH.STATUS IN (2,3,4,5) 
  AND ODH.POST_DATE between DATEADD(month, DATEDIFF(month, 0, @PostDate), 0) and @PostDate ) --@prompt 
WHERE
(
   GENIE.TOP_MSPON in 
   ('66-2667166','66-2696966','66-2757766','66-2765166','66-2808766','66-2962166','66-2965766','66-2965866','66-2996566',
'66-3011266','66-3053866','66-3065766','66-3072666','66-3121366','66-3138266','66-3141266','66-3148066','66-3341366',
'66-3393766','66-5710366','66-5776966','66-5801766','66-8444566','66-8455966','66-95136066','66-95208466','66-95386966',
'81-95387166','66-95652666','66-95763666','66-96250466','66-96320266','66-96590666','66-96601366','66-96788066','66-96901666',
'66-96911366','66-96936066','66-97807566','66-97839466','66-98014266','66-100280466','66-100711566','66-100919566',
'66-102506566','66-105640066','66-106679566','66-107245566','66-109999266','66-112287566','66-112399566','66-115091866',
'66-116456466','66-122339366','66-123276566','66-132163166','66-135655966','66-142287666','66-172026066','66-174504466',
'66-174576366','66-189838066','66-190474366','66-231135466','84-435813584','81-95387166','66-123275966','84-435813584') 
   AND SUBSTRING(GENIE.TOP_MSPON,1,2) in ('66','81','84')
  )
GROUP BY  
  GENIE.TOP_MSPON,
  TOPMSPON_NAME.ROMAN_NAME_1,
  ODH.PV_DATE_DATE,
  ODH.POST_DATE
  HAVING  SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*PRICE_2 ELSE PRICE_2 END) IS NOT NULL</CommandText>
