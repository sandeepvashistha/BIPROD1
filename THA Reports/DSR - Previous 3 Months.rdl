  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PVDate">
            <Value>=Parameters!PVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MktingUnit">
            <Value>=Parameters!MktingUnit.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT PV_DATE
, UNI_MKTING_UNIT 
, COUNTRY_CODE 
, ODH_TYPE
, ORDER_SUB_TOTAL
, MORDER
, ENTRY_DATE
, POST_DATE
, WAREHOUSE
, TOT_INVCE_AMOUNT
, PV_AMOUNT
, PAYMENT_TOTAL
, CASE 
	WHEN ODH_TYPE = 'C' THEN 
		CASE WHEN PAYMENT_TOTAL &gt; ORDER_SUB_TOTAL THEN 
			CASE WHEN PAYMENT_TOTAL &gt; TOT_INVCE_AMOUNT THEN -1*PAYMENT_TOTAL ELSE -1*TOT_INVCE_AMOUNT END 
		END 
	ELSE 
		CASE WHEN PAYMENT_TOTAL &gt; ORDER_SUB_TOTAL THEN 
			CASE WHEN PAYMENT_TOTAL &gt; TOT_INVCE_AMOUNT THEN PAYMENT_TOTAL ELSE TOT_INVCE_AMOUNT END 
		END
  END as LARGEST_NET_PV 
, CASE WHEN ODH_TYPE = 'C' THEN -1*PV_AMOUNT ELSE PV_AMOUNT END as NET_PV 
FROM 
(
SELECT
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) as pv_date ,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.UNI_MKTING_UNIT, 
  (CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) as ORDER_SUB_TOTAL ,
  DW.dbo.ODH.MORDER,
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.ODH.POST_DATE,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.ODH.TOT_INVCE_AMOUNT,
  sum(DW.dbo.ODH.PRICE_2) as PV_AMOUNT, 
  sum(CONVERT(FLOAT,RTRIM(LTRIM(DW.dbo.ODP.AMOUNT_RECD)))) as PAYMENT_TOTAL 
FROM
  DW.dbo.ODH, 
  DW.dbo.ODP
WHERE
	ODH.MORDER = ODP.MORDER 
AND 
  (
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (
    DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 91  )
    OR
    DW.dbo.ODH.COUNTRY_CODE  IN  ( 'IND'  )
   )
   AND
   (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103))  = @PVDate
  )
GROUP BY
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)), 
  DW.dbo.ODH.ODH_TYPE, 
  (CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END), 
  DW.dbo.ODH.MORDER, 
  DW.dbo.ODH.ENTRY_DATE, 
  DW.dbo.ODH.POST_DATE, 
  DW.dbo.ODH.WAREHOUSE, 
  DW.dbo.ODH.TOT_INVCE_AMOUNT, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.UNI_MKTING_UNIT
) a
WHERE a.uni_mkting_unit IN (@MktingUnit) 
AND a.country_code IN (@CountryCode)</CommandText>
      </Query>
