  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>a494162a-ce1d-49f4-bc2d-956140507e90</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select 
  DW.dbo.ODH.Country_Code as order_country_code,
  ODH.pv_date,
  Sum(DW.dbo.ODH.Price_1 / isnull(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) as sales_usd,
  DW.dbo.DST.COUNTRY_CODE as dist_country_code, 
SUM(DW.dbo.ODH.Price_2) as pv_amount
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  and ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE = DW.dbo.ODH.ENTRY_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' 
  )
  Where
  (
  CONVERT(DATETIME,'1/'+ ODH.PV_DATE,103) &gt;= DATEADD(Month, Datediff(Month, 0, DATEADD(m, -1, GETDATE())), 0)
  AND
  DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
  AND
  DW.dbo.ODH.ODH_TYPE  &lt;&gt;  'C'

  )
GROUP BY
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.DST.COUNTRY_CODE,
    ODH.pv_date</CommandText>
      </Query>
      <Fields>
        <Field Name="order_country_code">
          <DataField>order_country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pv_date">
          <DataField>pv_date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sales_usd">
          <DataField>sales_usd</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="dist_country_code">
          <DataField>dist_country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pv_amount">
          <DataField>pv_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
