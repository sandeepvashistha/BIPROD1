  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>281c7ef0-bd14-4098-b799-6ef9cd82a34e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
	tops.dist_id top_id,
	genie.dist_id,
	dst.home_country,
	dst.entry_date,
	bwt.vol1,
	genie.group_level
from 
	dw.dbo.genie
	inner join dw.dbo.dst tops on tops.dist_id=substring(genie.top_mspon,4,20)
	inner join dw.dbo.bwt on bwt.dist_id=genie.dist_id 
	inner join dw.dbo.dst on dst.dist_id=bwt.dist_id and bwt.pv_date_date=DATEADD(MONTH, DATEDIFF(MONTH, 0, dst.entry_date), 0)
where 
	tops.dist_id=@BANumber --117801466
	and bwt.pv_date_date=@CommissionMonth --'2019-07-01'
	and genie.group_level&lt;&gt;100</CommandText>
      </Query>
    </DataSet>
  </DataSets>
