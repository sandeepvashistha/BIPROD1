<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>5647f8a6-d31e-4991-86eb-ee271f593831</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Month">
            <Value>=Parameters!PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103) As PV_Date,
  DW.dbo.ODH.COUNTRY_CODE,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.OTY.DESCRIPTION,
  Sum(DW.dbo.ODH.Price_1 / isnull(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) as Amount_USD,
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) as Amount_Local,
  sum(DW.dbo.ODH.PRICE_2) As Total_PV
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  INNER JOIN DW.dbo.OTY ON ( DW.dbo.OTY.MKTING_UNIT = DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.OTY.ORDER_TYPE = DW.dbo.ODH.ODH_TYPE )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' 
  )
WHERE
  (
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'LAO'  )
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) &gt;= @PV_Month
  )
GROUP BY
  DW.dbo.ODH.PV_DATE, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.OTY.DESCRIPTION</CommandText>
      </Query>
