    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.PV_DATE,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.DST.COUNTRY_CODE,
  DW.dbo.DST.UNI_MKTING_UNIT,
  sum(DW.dbo.ODH.price_1 / isnull(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) as sales_usd,
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) as sales_local
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' 
  )
WHERE
  (
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  &gt;=  @StartDate
   AND
   DW.dbo.DST.UNI_MKTING_UNIT  IN  ( 1, 2, 3, 49, 58, 60, 62, 63, 65, 66, 81, 82, 85, 88, 84, 91, 97  )
  )
GROUP BY
  DW.dbo.ODH.PV_DATE, 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.DST.UNI_MKTING_UNIT</CommandText>
      </Query>
      <Fields>
