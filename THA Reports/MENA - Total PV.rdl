  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8074df3c-3ffc-4697-933c-f61a0962a5e8</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_DATE">
            <Value>=Parameters!PV_DATE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  CONVERT(DATETIME,'1/'+PV_DATE,103) as pv_date,COUNTRY_CODE,SUM(PV) as TOTAL_PV
FROM  (SELECT O.PV_DATE,O.HOME_COUNTRY, D.COUNTRY_CODE,SUM(O.PV_AMOUNT) AS PV
      FROM dbo.ODH O INNER JOIN dbo.DST D
      ON O.DIST_ID = D.DIST_ID
      WHERE  CONVERT(DATETIME,'1/'+O.PV_DATE,103) = @PV_DATE
      AND O.STATUS IN (2,3,4,5)
      AND O.ODH_TYPE &lt;&gt; 'C'
      AND D.UNI_MKTING_UNIT = '97'
      GROUP BY O.PV_DATE,O.HOME_COUNTRY,D.COUNTRY_CODE,O.DIST_ID
      HAVING SUM(PV_AMOUNT) &gt;= 1) o
GROUP BY CONVERT(DATETIME,'1/'+PV_DATE,103),COUNTRY_CODE
ORDER BY TOTAL_PV desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PV_DATE">
          <DataField>pv_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="COUNTRY_CODE">
          <DataField>COUNTRY_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TOTAL_PV">
          <DataField>TOTAL_PV</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
