  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 1/count(1) cnt
from dw.dbo.odh
where dist_id = 108357166
and cast(record_date as date) = cast(GETDATE() as date) --this is what we want (only yesterday)
--and cast(record_date as date) &gt; getdate() -14 --this is for testing (last two weeks)
and [status] &lt;&gt; 9 and odh_type&lt;&gt;'C'</CommandText>
      </Query>
      <Fields>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_2">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select uni_mkting_unit, order_number, dist_id, entry_date, entry_time, [status],odh_type, comment_3,country_code
from dw.dbo.odh
