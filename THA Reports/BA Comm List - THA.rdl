    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b31f0419-afd7-4519-a717-65d9f56685df</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Earnings_Amount">
            <Value>=Parameters!Earnings_Amount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.BWT_CURRENT.DIST_ID,
  DW.dbo.DST.ROMAN_NAME_1,
  DW.dbo.DST.EMAIL_ADDR
FROM
  DW.dbo.BWT_CURRENT,
  DW.dbo.DST,
  DW.dbo.ERN  ERN_Current
WHERE
  ( DW.dbo.DST.MDIST=DW.dbo.BWT_CURRENT.MDIST  )
  AND  ( DW.dbo.BWT_Current.PV_DATE_DATE = ERN_Current.PV_DATE_DATE and DW.dbo.BWT_Current.MDIST=ERN_Current.MDIST )
  AND  
  (
   CONVERT(dateTIME, RTRIM(DW.dbo.BWT_CURRENT.PV_DATE)+'01')  Between @Start_PV_Month and @End_PV_Month
   AND
   DW.dbo.BWT_CURRENT.MARKETING_UNIT  IN  ( 66  )
   AND
   DW.dbo.BWT_CURRENT.COUNTRY_CODE  IN  ( 'THA'  )
  )
GROUP BY
  DW.dbo.BWT_CURRENT.DIST_ID, 
  DW.dbo.DST.ROMAN_NAME_1, 
  DW.dbo.DST.EMAIL_ADDR
HAVING
  sum(ERN_Current.EARNINGS_AMOUNT)  &gt;=  @Earnings_Amount</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ROMAN_NAME_1">
          <DataField>ROMAN_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EMAIL_ADDR">
          <DataField>EMAIL_ADDR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
