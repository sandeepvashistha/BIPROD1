  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPORDA</DataSourceName>
        <CommandText>select 
	odh.order_number
	,odh.dist_id
	,dst.dist_status
	,odh.[status]
	,odh.price_lvl
	,odh.entry_date
	,odh.ship_to_name
	,odh.ship_to_addr_1
	,odh.ship_to_phone
	,odh.ship_via_desc
	,odh.tot_invce_amount
	,odh.home_country
	,cast(getdate() as date) yesterday
	,odp.pre_auth_transit Auth_Code
	,odh.country_code
	,stuff( (SELECT ';'+odl.item_code+','+odl.item_name_1+','+cast(odl.qty_ordered as varchar)+' Unit'
               FROM dw.dbo.odl
               WHERE odl.morder=odh.morder--y.morder
               ORDER BY odl.item_code
			   FOR XML PATH(''), TYPE).value('.', 'varchar(max)')
			    ,1,1,'') AS [Item_Details]
from 
	dw.dbo.odh 
	inner join dw.dbo.dst on (dst.dist_id=odh.dist_id)
	left outer join dw.dbo.odp on odp.morder=odh.morder and odp.payment_number=1
where 
	1=1 and (odh.comment_3 like 'express%' or odh.comment_2 like 'express%')
	and odh.UNI_MKTING_UNIT = 65 and odh.[status] in (2,3,4,5)
	--and odh.order_number in (416061561,416061560,416061566,416061573)
	and cast(odh.entry_date as date) = cast(getdate() as date)   --yesterday</CommandText>
      </Query>
      <Fields>
