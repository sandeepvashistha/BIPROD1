        </Field>
        <Field Name="Blocked">
          <DataField>Blocked</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="On_Hand_Quantity">
          <DataField>On-Hand Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="On_hand_value___USD">
          <DataField>On-hand value - USD</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
