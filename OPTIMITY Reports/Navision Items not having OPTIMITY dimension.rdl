  <DataSources>
    <DataSource Name="UniNav">
      <DataSourceReference>UniNav</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>fef0010c-2683-4669-aaf6-a424f6494e00</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <CommandText>SELECT	i.[uni_company_name] AS [Company], 		
		i.[No_] AS [Item No], 	
		i.[Description], 	
		--CASE i.[Item Type] 	
		--	WHEN 2 THEN 'Finished good' 
		--	WHEN 5 THEN 'Raw Material'
		--	WHEN 8 THEN 'Semifinished' 
		--END AS [Item Type], 	
		g.OptionString as [Item Type],	
		CASE WHEN [Item Status]=0 THEN 'Active'	
			WHEN [Item Status]=1 THEN 'Discontinue'
			WHEN [Item Status]=2 THEN 'Inactive'
			WHEN [Item Status]=3 THEN 'Obsolete'
			WHEN [Item Status]=4 THEN 'Phantom'
			WHEN [Item Status]=5 THEN 'PreRelease'
			WHEN [Item Status]=6 THEN 'Reference' 
		END AS [Item Status],	
		CASE  i.[Blocked] 	
			WHEN 0 THEN 'No' 
			WHEN 1 THEN 'Yes' 
		END AS [Blocked],	
		cast(SUM(ile.[Remaining Quantity]) as numeric(20,0)) AS [On-Hand Quantity],	
		cast(SUM(ile.[Remaining Quantity] * i.[Standard Cost] *  COALESCE(er.[Exchange_Rate],1)) as numeric(24,0)) AS [On-hand value - USD] 	
FROM Item_nc_vw i			
LEFT JOIN Default_Dimension_nc_vw d			
ON d.uni_company_name = i.uni_company_name AND d.[No_] = i.[No_] and d.[Dimension Code] = 'OPTIMITY' and D.[Table ID] = 27			
LEFT JOIN ( select Distinct(FieldInteger),(OptionString) from option_strings_nc_vw  where TableName='Item' and FieldName='Item Type') g 			
ON i.[item type]=g.[FieldInteger]			
LEFT JOIN Item_Ledger_Entry_nc_vw ile			
ON ile.uni_company_name = i.uni_company_name and ile.[Item No_] = i.[No_] and ile.[Remaining Quantity] &gt; 0			
LEFT JOIN			
(			
	select a.uni_company_name, a.[Starting Date], b.[LCY Code] as from_currency, a.[Currency Code] as to_currency, [Exchange Rate Amount]/([Relational Exch_ Rate Amount]) as exchange_rate		
	from Currency_Exchange_Rate_nc_vw a		
	join general_ledger_setup_nc_vw b		
	on a.uni_company_name = b.uni_company_name		
	union		
	select c.uni_company_name, DATEADD(mm, DATEDIFF(mm, 0, GETDATE()), 0) as [Starting Date], c.[LCY Code] as from_currency, c.[LCY Code] as to_currency, 1 as exchange_rate		
	from general_ledger_setup_nc_vw c		
) er 			
ON er.[to_currency]='USD' and er.uni_company_name=i.uni_company_name and er.[Starting Date] = (select max([Starting Date]) from Currency_Exchange_Rate_nc_vw where [Currency Code]='USD' and uni_company_name = i.uni_company_name)			
WHERE i.[Item Type] IN (2,5,8) -- Finished Goods, Raw Material and Semifinished			
AND i.[Item Status] IN (0,1,5) -- Active, Discontinue and PreRelease			
And i.[uni_company_name] NOT IN (select company from dm_optimity_exclusions)			
AND D.[Dimension Code] IS  NULL			
GROUP BY i.uni_company_name, i.[No_], i.[Description], g.OptionString, [Item Status], i.Blocked			</CommandText>
      </Query>
      <Fields>
        <Field Name="Company">
          <DataField>Company</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No">
          <DataField>Item No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
