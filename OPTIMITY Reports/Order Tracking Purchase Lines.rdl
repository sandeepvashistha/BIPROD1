  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <CommandText>SELECT	h.[Uni_company_name],
	h.[No_] as [PO No],
	replace (convert(varchar, h.[Order Date] , 106), ' ', '-') as [PO Date],
	h.[Pay-to Name] as [Vendor],
	h.[Purchaser Code] + '-' + sp.Name as [Purchaser],
	l.[Line No_] as [PO Line],
	l.[No_] as [Item No],
	l.[Description] as [Item Description],
	l.[Location Code] as [Location],
	format(l.[Quantity],'#.##') as [PO Ordered Quantity],
	format(l.[Outstanding Quantity],'#.##') as [PO Outstanding Quantity],
	l.[Unit of Measure],
	replace (convert(varchar, l.[Planned Receipt Date] , 106), ' ', '-') as [Planned Receipt Date]
FROM prod_navision.dbo.Purchase_Header_nc_vw h 	
INNER JOIN prod_navision.dbo.Purchase_Line_nc_vw l ON l.uni_company_name = h.uni_company_name AND l.[Document No_] = h.[No_]	
INNER JOIN prod_navision.dbo.Item_nc_vw i ON i.uni_company_name = l.uni_company_name AND l.[No_] = i.[No_]	
LEFT JOIN prod_navision.dbo.Salesperson_Purchaser_nc_vw sp on sp.uni_company_name = h.uni_company_name and sp.Code = h.[Purchaser Code]	
WHERE h.uni_company_name in ('Performance Labs', 'MLB Exports', 'USA','UII Corp') AND h.[Completed] = 0 -- not complete only	
AND h.[Document Type] = 1 -- PO's only, removed credit memos, etc.	
AND l.[Outstanding Quantity] &gt; 0 AND l.[Type] = 2 -- excludes GL accounts	
AND l.[Prod_ Order No_] = '' -- excludes subcon PO	
AND i.[Item Type] in (2, 8, 5) -- FG, SF and RM only 	
AND [Vendor Order No_] not in (select [No_] from prod_navision.dbo.Purchase_Header_nc_vw where uni_company_name = 'MLB Exports')	
Order by h.uni_company_name, h.No_, l.[Line No_]	</CommandText>
        <Timeout>9</Timeout>
      </Query>
