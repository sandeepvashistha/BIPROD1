  <DataSets>
    <DataSet Name="CountryDesc">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
   country_code,
   country_name,
   country_code+' - '+ country_name descri 
from 
   dw.dbo.dm_location_manager 
where 
   --backoffice_mkting_unit=49
   country_name is not null 
order by 
   country_code</CommandText>
      </Query>
      <Fields>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="country_name">
          <DataField>country_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="descri">
          <DataField>descri</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_Decrease">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Date">
            <Value>=Parameters!PV_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Market">
            <Value>=Parameters!Market.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select b.*,bwt12.pv_date_date,bwt12.vol3
from
(
select d.*
from
(
select
c.dist_id,c.roman_name_1,c.home_country,c.country_code,
c.current_pv_date,c.last_pv_date,c.current_vol3,c.last_vol3,c.diff
,RANK() OVER(PARTITION BY c.country_code ORDER BY c.diff,c.dist_id) as rnk
from
(
select bwt.dist_id,dst.roman_name_1,dst.home_country,dst.country_code,
CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE) current_pv_date
,CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE) last_pv_date
,sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end) current_vol3
,sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end) last_vol3
,(sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end)-
sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end)) diff
from dw.dbo.bwt inner join dw.dbo.dst on bwt.dist_id=dst.dist_id
where 
--dst.uni_mkting_unit=49
dst.uni_mkting_unit=@Market
and 
dst.country_code in (@CountryCode)
and 
cast(bwt.pv_date_date as date) between CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE) and
CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)
group by bwt.dist_id,dst.roman_name_1,dst.home_country,dst.country_code--,a.current_pv_date,a.last_pv_date
) c
) d
where d.rnk&lt;=10
) b
left outer join dw.dbo.bwt bwt12 on (bwt12.dist_id=b.dist_id 
and CAST(bwt12.pv_date_date AS DATE) between CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, b.current_pv_date)-11, 0) AS DATE) and CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, b.current_pv_date), 0) AS DATE))</CommandText>
      </Query>
      <Fields>
        <Field Name="dist_id">
          <DataField>dist_id</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="roman_name_1">
          <DataField>roman_name_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="home_country">
          <DataField>home_country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="current_pv_date">
          <DataField>current_pv_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="last_pv_date">
          <DataField>last_pv_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="current_vol3">
          <DataField>current_vol3</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="last_vol3">
          <DataField>last_vol3</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="diff">
          <DataField>diff</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="rnk">
          <DataField>rnk</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="pv_date_date">
          <DataField>pv_date_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="vol3">
          <DataField>vol3</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_Increase">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Date">
            <Value>=Parameters!PV_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Market">
            <Value>=Parameters!Market.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select b.*,bwt12.pv_date_date,bwt12.vol3
from
(
select d.*
from
(
select
c.dist_id,c.roman_name_1,c.home_country,c.country_code,
c.current_pv_date,c.last_pv_date,c.current_vol3,c.last_vol3,c.diff
,RANK() OVER(PARTITION BY c.country_code ORDER BY c.diff desc,c.dist_id) as rnk
from
(
select bwt.dist_id,dst.roman_name_1,dst.home_country,dst.country_code,
CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE) current_pv_date
,CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE) last_pv_date
,sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end) current_vol3
,sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end) last_vol3
,(sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end)-
sum(case when CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE)=bwt.pv_date_date then bwt.vol3 else 0 end)) diff
from dw.dbo.bwt inner join dw.dbo.dst on bwt.dist_id=dst.dist_id
where 
--dst.uni_mkting_unit=49
dst.uni_mkting_unit=@Market 
and 
dst.country_code in (@CountryCode)
and 
cast(bwt.pv_date_date as date) between CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date))-1, 0) AS DATE) and
CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, CONVERT(date, @PV_Date)), 0) AS DATE)
group by bwt.dist_id,dst.roman_name_1,dst.home_country,dst.country_code--,a.current_pv_date,a.last_pv_date
) c
) d
where d.rnk&lt;=10
) b
left outer join dw.dbo.bwt bwt12 on (bwt12.dist_id=b.dist_id 
and CAST(bwt12.pv_date_date AS DATE) between CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, b.current_pv_date)-11, 0) AS DATE) and CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, b.current_pv_date), 0) AS DATE))
order by b.country_code,b.rnk,b.dist_id,bwt12.pv_date_date</CommandText>
      </Query>
      <Fields>
        <Field Name="dist_id">
          <DataField>dist_id</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="roman_name_1">
          <DataField>roman_name_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="home_country">
