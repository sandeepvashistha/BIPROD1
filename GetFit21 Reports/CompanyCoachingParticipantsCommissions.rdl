  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>Exigo_Repo</DataSourceName>
        <CommandText>SELECT od.ItemCode
, o.CustomerID
, o.OrderDate
, c.LastName
, c.FirstName
, c.Email
, c.SponsorID
, s.LastName as SponsorLastName
, s.FirstName as SponsorFirstName
, s.Email as SponsorEmail
, o.OrderId
, o.Other11
, LEFT(o.Notes, CHARINDEX(',',o.Notes)-1) as ChallengeStartDate
FROM EXIGO_REPO.dbo.Orders o 
INNER JOIN OrderDetails od ON od.OrderID = o.OrderId 
INNER JOIN Customers c ON c.CustomerId = o.CustomerID
INNER JOIN Customers s ON s.CustomerID = c.SponsorID
WHERe od.ItemCode = '28980' </CommandText>
      </Query>
