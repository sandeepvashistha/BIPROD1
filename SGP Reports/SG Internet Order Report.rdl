    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>44cfa597-6011-44c2-8316-3952a77051d1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartEntryDate">
            <Value>=Parameters!StartEntryDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndEntryDate">
            <Value>=Parameters!EndEntryDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ITD.ITEM_NAME_1,
  SUM(DW.dbo.ODL.QTY_ORDERED) as QTY_ORDERED
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER )
  INNER JOIN DW.dbo.ODP ON ( DW.dbo.ODP.MORDER = DW.dbo.ODH.MORDER )
  LEFT OUTER JOIN DW.dbo.ITD ON ( DW.dbo.ODL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS = DW.dbo.ITD.MITEM )
WHERE
  ( ( ( DW.dbo.ODL.MKTING_UNIT=91  )  AND  ( DW.dbo.ODH.UNI_MKTING_UNIT=91  )  )
  OR  ( ( DW.dbo.ODL.MKTING_UNIT IN (60,62,63,65,66,81,82,85,88,84)  )  AND  ( DW.dbo.ODH.UNI_MKTING_UNIT  IN (60,62,63,65,66,81,82,85,88,84)  )  )
  )  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 65  )
   AND
   DW.dbo.ODH.ENTRY_DATE  BETWEEN  @StartEntryDate AND @EndEntryDate
   AND
   DW.dbo.ODH.COUNTRY_CODE  IN  ('SGP')  
   AND
   DW.dbo.ODH.ENTRY_INIT  IN  ( '*ms'  )
   AND
   DW.dbo.ODP.RECEIPT_TYPE  IN  ( 13  )
   AND
   DW.dbo.ODH.STATUS  &lt;&gt;  9
  )
GROUP BY DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ITD.ITEM_NAME_1</CommandText>
      </Query>
      <Fields>
        <Field Name="ITEM_CODE">
          <DataField>ITEM_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ITEM_NAME_1">
          <DataField>ITEM_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTY_ORDERED">
          <DataField>QTY_ORDERED</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
