<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>ce9c0a34-2c79-40ae-8b93-7f137979f27e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Month">
            <Value>=Parameters!PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) As PV_Month,
  DW.dbo.DST.COUNTRY_CODE As Dist_Country,
  DW.dbo.ODH.COUNTRY_CODE AS ORDER_COUNTRY,
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.ODH.RELEASED_DATE,
  DW.dbo.ODL.ORDER_NUMBER,
  DW.dbo.ODL.morder,
  DW.dbo.ODL.MITEM,
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ITD.ITEM_NAME_1,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.ODH.SHIP_TO_COUNTRY,
  sum(DW.dbo.ODL.QTY_ORDERED) as Total_Qty,
  sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_1) AS AMOUNT_LOCAL,
  Sum(DW.dbo.ODL.QTY_ORDERED*DW.dbo.ODL.price_1 / isnull(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) AS AMOUNT_USD
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID )
  INNER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.MORDER=DW.dbo.ODL.MORDER )
  LEFT OUTER JOIN DW.dbo.ITD ON ( DW.dbo.ODL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS = DW.dbo.ITD.MITEM )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' 
  )
WHERE
  (
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  =  @PV_Month
   AND
   DW.dbo.ODL.MITEM  NOT IN  ( '66-23426','66-24736','66-24740','66-25385','66-26763','66-26765','66-26767','66-26769','66-26959','66-27569','66-27960','66-27962','66-27966','66-27972','66-27976','66-27978','66-27984','65-24452','66-29464','66-29480','01-17744','01-24455','01-24458','01-24460','01-27800','01-26580','65-24347','01-6151','49-24002','66-23433','66-27974','66-27968','01-22165','01-27801','01-27028','01-27029','01-27030','01-24456','85-24503','85-19192','62-29099','62-26556','85-24312','85-26661','65-29536','01-24457','01-19591','01-29340' )
   AND
   DW.dbo.ODH.COUNTRY_CODE  IN  ( 'SGP'  )
  )
GROUP BY
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)),
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.ODH.ENTRY_DATE, 
  DW.dbo.ODH.RELEASED_DATE, 
  DW.dbo.ODL.ORDER_NUMBER, 
  DW.dbo.ODL.morder, 
  DW.dbo.ODL.MITEM, 
  DW.dbo.ODL.ITEM_CODE, 
  DW.dbo.ITD.ITEM_NAME_1, 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.ODH.SHIP_TO_COUNTRY</CommandText>
      </Query>
