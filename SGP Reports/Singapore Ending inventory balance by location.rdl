    <DataSet Name="LocationCode">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <CommandText>select code,(code+' - '+name) code_description 
from prod_navision.dbo.location_nc_vw 
where uni_company_name='Singapore'
order by code;</CommandText>
      </Query>
      <Fields>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="code_description">
          <DataField>code_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="NonPLVendor">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <CommandText>select distinct a.[Vendor No_] 
from prod_navision.dbo.Purchase_Price_nc_vw a
inner join prod_navision.dbo.purchases_and_payables_setup_nc_vw b
on a.uni_company_name = b.uni_company_name and a.[Vendor No_] &lt;&gt; b.[PL Vendor]
inner join prod_navision.dbo.purchases_and_payables_setup_nc_vw u
on a.uni_company_name = u.uni_company_name and a.[Vendor No_] &lt;&gt; u.[US Vendor]
where a.uni_company_name='Singapore' and
a.[Vendor No_]&lt;&gt;u.[US Vendor] and a.[Vendor No_]&lt;&gt;b.[PL Vendor]
order by a.[Vendor No_];</CommandText>
      </Query>
      <Fields>
        <Field Name="Vendor_No_">
          <DataField>Vendor No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
