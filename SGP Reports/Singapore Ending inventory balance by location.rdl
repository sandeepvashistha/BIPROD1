  <DataSets>
    <DataSet Name="Main_Query">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostingMonth">
            <Value>=Parameters!PostingMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LocationCode">
            <Value>=Parameters!LocationCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NonPLVendor">
            <Value>=Parameters!NonPLVendor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
LedgerEntry.[uni_company_name],LedgerEntry.[Location Code]
, LedgerEntry.[Item No_]
,LedgerEntry.[Item Name]
--,cur.currency_code [Standard Cost Currency Code]
,cast(LedgerEntry.[Standard Cost] as numeric(20,4)) [Standard Cost]
,LedgerEntry.[Sellable Item]
,isnull(PL.[Currency Code],'') [Currency Code]
,isnull(PL.[PL Vendor],'')[PL Vendor]
,isnull(cast(PL.[PL Purchase Transfer Price] as numeric(20,4)),0) [PL Transfer Price]
,isnull(PL.[US Vendor],'') [US Vendor]
,isnull(cast(PL.[US Purchase Transfer Price] as numeric(20,4)),0) [US Transfer Price]
,isnull(PL.[NON-PL Vendor],'') [NON-PL Vendor]
,isnull(cast(PL.[NON-PL Purchase Transfer Price] as numeric(20,4)),0) [NOn-PL Transfer Price]
,cast(LedgerEntry.[Total till Posting Month] as numeric(20,0)) [Total till Posting Month]
,LedgerEntry.Last_Month
,cast(LedgerEntry.[Total Last_Month] as numeric(20,0)) [Total Last_Month]
,LedgerEntry.Last_2Month
,cast(LedgerEntry.[Total Last_2Month] as numeric(20,0)) [Total Last_2Month]
,LedgerEntry.Last_3Month
,cast(LedgerEntry.[Total Last_3Month] as numeric(20,0)) [Total Last_3Month]
from
(
select b.[uni_company_name],b.[Location Code] as [Location Code]
, b.[Item No_] as [Item No_]
,ItemNV.[Description] AS [Item Name]
,ItemNV.[Standard Cost]
,(case ItemNV.[OE Transactable] when 0 then 'No' when 1 then 'Yes' end) as [Sellable Item]
, sum(isnull(b.[quantity],0)) as [Total till Posting Month]
,EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-1, 0)) Last_Month
, sum(case when cast(b.[Posting Date] as date)&lt;= EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-1, 0))
then isnull(b.[quantity],0) else 0 end) as [Total Last_Month]
,EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-2, 0)) Last_2Month
, sum(case when cast(b.[Posting Date] as date)&lt;= EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-2, 0))
then isnull(b.[quantity],0) else 0 end) as [Total Last_2Month]
,EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-3, 0)) Last_3Month
, sum(case when cast(b.[Posting Date] as date)&lt;= EOMONTH(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PostingMonth)-3, 0))
then isnull(b.[quantity],0) else 0 end) as [Total Last_3Month]
from [Prod_Navision].dbo.Item_Ledger_Entry_nc_vw b
INNER JOIN [Prod_Navision].dbo.Item_nc_vw ItemNV ON (b.[Item No_]=ItemNV.No_  AND  b.uni_company_name=ItemNV.uni_company_name)
where 
b.[uni_company_name] in ('Singapore') 
and cast(DATEADD(MONTH, DATEDIFF(MONTH, 0, b.[Posting Date]), 0) as date) &lt;= @PostingMonth
and b.[Location Code] in (@LocationCode)
--and b.[Item No_]='24403'
group by
b.[uni_company_name],b.[Item No_],b.[Location Code],ItemNV.[Description],
(case ItemNV.[OE Transactable] when 0 then 'No' when 1 then 'Yes' end),ItemNV.[Standard Cost]
) LedgerEntry
left outer join
(
select 
	a.uni_company_name
	, a.[Item No_]
	, (case when a.[Currency Code] is null or a.[Currency Code]='' then cur.currency_code else a.[Currency Code] end) [Currency Code]
	, MAX(isnull(b.[PL Vendor],'')) as [PL Vendor]
	, MAX(isnull((case when b.[PL Vendor] is not null then a.[Direct Unit Cost] else null end),null)) as [PL Purchase Transfer Price]
	, MAX(isnull(u.[US Vendor],'')) as [US Vendor]
	, MAX(isnull((case when u.[US Vendor] is not null then a.[Direct Unit Cost] else null end),null)) as [US Purchase Transfer Price]
	, MAX(case when a.[Vendor No_] =@NonPLVendor then a.[Vendor No_] else null end) as [NON-PL Vendor]
	, MAX(isnull((case when a.[Vendor No_] =@NonPLVendor then a.[Direct Unit Cost] else null end),null)) as [NON-PL Purchase Transfer Price]
from prod_navision.dbo.Purchase_Price_nc_vw a
left outer join prod_navision.dbo.purchases_and_payables_setup_nc_vw b
on a.uni_company_name = b.uni_company_name and a.[Vendor No_] = b.[PL Vendor]
left outer join prod_navision.dbo.purchases_and_payables_setup_nc_vw u
on a.uni_company_name = u.uni_company_name and a.[Vendor No_] = u.[US Vendor]
left outer join
(
select distinct d.dw_navision_market_name,d.country_code,d.currency_code,c.[country_region code],c.uni_company_name 
from [sqlproda\sqla].dw.dbo.dm_location_manager d inner join prod_navision.dbo.company_information_nc_vw c
on c.uni_company_name COLLATE DATABASE_DEFAULT=d.dw_navision_market_name and c.[country_region code]=d.country_code
) cur on cur.uni_company_name=a.uni_company_name
group by
a.uni_company_name, a.[Item No_], (case when a.[Currency Code] is null or a.[Currency Code]='' then cur.currency_code else a.[Currency Code] end)
) PL on LedgerEntry.uni_company_name=PL.uni_company_name and LedgerEntry.[Item No_]=PL.[Item No_]
order by LedgerEntry.uni_company_name,LedgerEntry.[Item No_]</CommandText>
      </Query>
