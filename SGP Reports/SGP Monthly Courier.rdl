    <DataSet Name="Detail">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
	odh.currency_code,
	odh.order_number,
	odh.invoice_number,
	odh.entry_date as order_entry_date,
	odh.pv_date_date,
	odh.warehouse,
	(cast(odh.order_source as varchar)+' - '+ods.[description]) as source_desc,
	odh.total_weight as [Order Weight],
	odh.price_1 as [Amount (incl. GST)],
	odh.tot_invce_amount as [Total Invoice Amount],
	odh.freight_amount as [Freight (incl. GST)]
from dw.dbo.odh left outer join dw.dbo.ods on (odh.uni_mkting_unit=ods.mkting_unit and odh.order_source=ods.ods_number)
where odh.uni_mkting_unit=65 and odh.entry_date&gt;=DATEADD(YEAR, DATEDIFF(YEAR, 0,DATEADD(YEAR, 0, GETDATE())), 0)
and odh.odh_type&lt;&gt;'C' and odh.status in (2,3,4,5) and odh.order_source in (101,900,901,903,905,906)
order by odh.currency_code,source_desc,[Order Weight],order_entry_date</CommandText>
      </Query>
