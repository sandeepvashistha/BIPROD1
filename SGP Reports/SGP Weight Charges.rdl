  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
	odh.pv_year,cast(odh.order_source as varchar)+' - '+ods.description source_desc,odh.order_source,ods.description,
	(
	case when odh.total_weight&lt;=3 then '1.Below or Equal to 3K' 
	when odh.total_weight&gt;3 and odh.total_weight&lt;=4 then '2.Above 3KG to 4KG'
	when odh.total_weight&gt;4 and odh.total_weight&lt;=7 then '3.Above 4KG to 7KG'
	when odh.total_weight&gt;7 and odh.total_weight&lt;=10 then '4.Above 7KG to 10KG'
	when odh.total_weight&gt;10 then '5.Above 10KG'
	end
	) [Order Weight],
	count(odh.order_number) [Order Count],sum(odh.price_1) [Amount (incl. GST)],
	sum(odh.tot_invce_amount) [Total Invoice Amount],sum(odh.freight_amount) [Freight (incl. GST)]
from dw.dbo.odh left outer join dw.dbo.ods on (odh.uni_mkting_unit=ods.mkting_unit and odh.order_source=ods.ods_number)
where odh.uni_mkting_unit=65 and odh.pv_year&gt;='2015'
and odh.odh_type&lt;&gt;'C' and odh.status in (2,3,4,5) and odh.order_source in (101,900,901,903,905,906)
group by 
	odh.pv_year,cast(odh.order_source as varchar)+' - '+ods.description,odh.order_source,ods.description,
	(
	case when total_weight&lt;=3 then '1.Below or Equal to 3K' 
	when total_weight&gt;3 and total_weight&lt;=4 then '2.Above 3KG to 4KG'
	when total_weight&gt;4 and total_weight&lt;=7 then '3.Above 4KG to 7KG'
	when total_weight&gt;7 and total_weight&lt;=10 then '4.Above 7KG to 10KG'
	when total_weight&gt;10 then '5.Above 10KG'
	end
	);</CommandText>
      </Query>
