    <DataSet Name="Query_2">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 
	cast(odh.order_source as varchar)+' - '+ods.description [source_desc],order_source ,ship_via_desc,pv_year,
	(
	case when odh.total_weight&lt;=3 then '1.Below or Equal to 3K' 
	when odh.total_weight&gt;3 and odh.total_weight&lt;=4 then '2.Above 3KG to 4KG'
	when odh.total_weight&gt;4 and odh.total_weight&lt;=7 then '3.Above 4KG to 7KG'
	when odh.total_weight&gt;7 and odh.total_weight&lt;=10 then '4.Above 7KG to 10KG'
	when odh.total_weight&gt;10 then '5.Above 10KG'
	end
	) [Order Weight],
	count(order_number) [Order Count],sum(price_1) [Amount incl GST],sum(tot_invce_amount) [Total Invoice Amount],sum(freight_amount) [Freight incl GST]
from dw.dbo.odh 
	 left outer join dw.dbo.ods on (odh.uni_mkting_unit=ods.mkting_unit and odh.order_source=ods.ods_number)
where uni_mkting_unit=65 and pv_year&gt;='2015'
and odh_type&lt;&gt;'C' and status in (2,3,4,5) and order_source&lt;&gt;100 
and price_1&gt;=350 
and total_weight&lt;=3 and isnull(freight_amount,0)&lt;=0 and ship_via_code in (2,3)
group by 
	cast(odh.order_source as varchar)+' - '+ods.description,order_source,ship_via_desc,pv_year,
	(
	case when odh.total_weight&lt;=3 then '1.Below or Equal to 3K' 
	when odh.total_weight&gt;3 and odh.total_weight&lt;=4 then '2.Above 3KG to 4KG'
	when odh.total_weight&gt;4 and odh.total_weight&lt;=7 then '3.Above 4KG to 7KG'
	when odh.total_weight&gt;7 and odh.total_weight&lt;=10 then '4.Above 7KG to 10KG'
	when odh.total_weight&gt;10 then '5.Above 10KG'
	end
	)</CommandText>
      </Query>
