        </Field>
        <Field Name="Amount__incl__GST_">
          <DataField>Amount (incl. GST)</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Invoice_Amount">
          <DataField>Total Invoice Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
