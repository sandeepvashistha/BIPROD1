  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Order_Creation_Month">
            <Value>=Parameters!Start_Order_Creation_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Order_Creation_Month">
            <Value>=Parameters!End_Order_Creation_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Item_Code">
            <Value>=Parameters!Item_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  distinct DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODL.ITEM_NAME_1,
  DW.dbo.ODL.PRICE_1,
  --trunc(DW.dbo.ODL.CREATION_DATE,'MM')
  DATEADD(mm, DATEDIFF(mm,0,DW.dbo.ODL.CREATION_DATE),0) As Order_Creation_Month
   --DW.dbo.ODL.QTY_ORDERED,
  -- DW.dbo.ODL.QTY_SHIPPED
FROM
  DW.dbo.ODL,
  DW.dbo.ODH,
  DW.dbo.IPL
WHERE
  ( DW.dbo.ODH.MORDER=DW.dbo.ODL.MORDER  )
and (DW.dbo.ODL.PRICE_LEVEL = DW.dbo.IPL.PRICE_LEVEL and DW.dbo.ODL.MKTING_UNIT = DW.dbo.IPL.MKTING_UNIT)
  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  65
   AND
   DW.dbo.ODH.COUNTRY_CODE  =  'SGP'
   AND
   DATEADD(mm, DATEDIFF(mm,0,DW.dbo.ODL.CREATION_DATE),0)  BETWEEN  @Start_Order_Creation_Month  AND  @End_Order_Creation_Month
   AND
   DW.dbo.ODL.ITEM_CODE  IN  (@Item_Code)
   AND
   DW.dbo.ODH.DISTRIBUTOR_STATUS  IN  ( 'A','B'  )
  )</CommandText>
      </Query>
      <Fields>
        <Field Name="ITEM_CODE">
          <DataField>ITEM_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ITEM_NAME_1">
          <DataField>ITEM_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PRICE_1">
          <DataField>PRICE_1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Order_Creation_Month">
          <DataField>Order_Creation_Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
