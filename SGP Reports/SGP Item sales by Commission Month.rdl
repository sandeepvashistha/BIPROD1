  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dates">
            <Value>=Parameters!Dates.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartPVDate">
            <Value>=Parameters!StartPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT (CASE WHEN 1=@Dates THEN ODH.PV_DATE_DATE ELSE CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.INVOICE_PRINT_DATE), 0) AS DATE) END) PV_DATE_DATE,ODL.ITEM_CODE [Code],ITD.ITEM_NAME_1 [Product Name],IPL.PRICE_3 [WP (SGD)],--ODH.ORDER_SOURCE,ODL.FLAG_2,
sum(CASE WHEN (ODH.ORDER_SOURCE=915 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='AUS')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [AU NFR],
sum(CASE WHEN (ODH.ORDER_SOURCE=913 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='IDN')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [IDN NFR],
sum(CASE WHEN (ODH.CURRENCY_CODE  IN ('USD','JPN','KORN') AND ODH.SHIP_TO_COUNTRY='JPN' AND ODH.ORDER_SOURCE  NOT IN  (912,913,914,915,916,918)) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [JPN NFR],
sum(CASE WHEN (ODH.ORDER_SOURCE=912 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='MYS')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [MY NFR],
sum(CASE WHEN (ODH.ORDER_SOURCE=914 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='PHL')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [PH NFR],
sum(CASE WHEN (ODH.ORDER_SOURCE=916 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='NZL')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END)  [NZ NFR],
sum(CASE WHEN (ODH.ORDER_SOURCE=918 OR (ODH.ORDER_SOURCE=904 AND ODH.COUNTRY_CODE='KOR')) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END)  [KR NFR],
sum(CASE WHEN (ODH.CURRENCY_CODE = 'BGN' AND ODH.SHIP_TO_COUNTRY='BGD' AND ODH.ORDER_SOURCE  NOT IN  (912,913,914,915,916,918)) THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END)  [BD NFR],
sum(CASE WHEN ODH.COUNTRY_CODE='SGP' AND ODH.SHIP_TO_COUNTRY = 'SGP' THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [Walk In/Ushop],
sum(CASE WHEN ODH.COUNTRY_CODE='SGP' AND ODH.SHIP_TO_COUNTRY NOT IN ('SGP','JPN') AND ODH.CURRENCY_CODE  NOT IN ('USD','JPN','KORN') THEN (CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ELSE 0 END) [Advance],
sum(CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) [Total]
FROM
	DW.DBO.ODH INNER JOIN DW.DBO.ODL ON (ODH.MORDER=ODL.MORDER)
	--NNER JOIN DW.DBO.DST ON (DST.DIST_ID=ODH.DIST_ID)
	LEFT OUTER JOIN DW.DBO.ITD ON (ODL.MITEM COLLATE DATABASE_DEFAULT=ITD.MITEM COLLATE DATABASE_DEFAULT)
	LEFT OUTER JOIN DW.DBO.ODS ON (ODS.MKTING_UNIT=ODH.UNI_MKTING_UNIT AND ODS.ODS_NUMBER=ODH.ORDER_SOURCE)
	LEFT OUTER JOIN DW.DBO.IPL ON (ODL.MITEM=IPL.MITEM AND IPL.PRICE_LEVEL=1 AND IPL.COUNTRY_CODE='SGP')
WHERE
	((1=@Dates AND ODH.PV_DATE_DATE between @StartPVDate and @EndPVDate) OR (2=@Dates AND CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.INVOICE_PRINT_DATE), 0) AS DATE) between @StartPVDate and @EndPVDate))
	AND ODH.UNI_MKTING_UNIT = 65 AND ODH.STATUS  IN  (2,3,4,5)
	--AND ODH.COUNTRY_CODE in ('AUS','AUSN','IDNN','MYSN','PHLN','JPNN','SGP','JPN','USA','NZLN')
	/*AND 
	(ODH.ORDER_SOURCE  IN  (912,913,914,915,916,904,918)
	OR (ODH.CURRENCY_CODE  IN ('USD','JPN','KORN') AND ODH.SHIP_TO_COUNTRY='JPN')
	OR ODH.COUNTRY_CODE='SGP'
	)*/
	AND (ODL.FLAG_2 = 'M' OR ODL.FLAG_2  Is Null)
	AND ODH.COUNTRY_CODE&lt;&gt;'BRN'
	--AND ODL.ITEM_CODE='30405' --AND ODH.SHIP_TO_COUNTRY &lt;&gt; 'SGP'--ODH.ORDER_SOURCE=906
	--AND ODH.ODH_TYPE='I'
GROUP BY
	(CASE WHEN 1=@Dates THEN ODH.PV_DATE_DATE ELSE CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.INVOICE_PRINT_DATE), 0) AS DATE) END),ODL.ITEM_CODE,ITD.ITEM_NAME_1,IPL.PRICE_3</CommandText>
      </Query>
