        </Field>
        <Field Name="Walk_In_Ushop">
          <DataField>Walk In/Ushop</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Advance">
          <DataField>Advance</DataField>
