  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>0c859169-6b5b-446c-908d-9186edb2165a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostDateStart">
            <Value>=Parameters!PostDateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PostDateEnd">
            <Value>=Parameters!PostDateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	ODL.ITEM_CODE,
	ITD.ITEM_NAME_1,
	ODH.ODH_TYPE,
	SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) ORDERED_QUANTITY,
	SUM(CASE WHEN ODH.ODH_TYPE='C' THEN -1*(ODL.QTY_ORDERED*ODL.PRICE_1) ELSE (ODL.QTY_ORDERED*ODL.PRICE_1) END) ORDERED_SALES_AMOUNT,
	DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.POST_DATE), 0) POST_DATE
FROM
	DW.DBO.ODH ODH INNER JOIN DW.DBO.ODL ODL ON (ODH.MORDER=ODL.MORDER)
	LEFT OUTER JOIN DW.DBO.ITD ITD ON (ODL.MITEM COLLATE DATABASE_DEFAULT=ITD.MITEM COLLATE DATABASE_DEFAULT)
WHERE
	ODH.UNI_MKTING_UNIT  =  82 AND ODH.STATUS  BETWEEN  2  AND  5
	AND ODH.RELEASED_DATE  Is Not Null
	AND ODH.POST_DATE  BETWEEN  @PostDateStart AND @PostDateEnd
GROUP BY
	ODL.ITEM_CODE,
	ITD.ITEM_NAME_1,
	ODH.ODH_TYPE,
	DATEADD(MONTH, DATEDIFF(MONTH, 0, ODH.POST_DATE), 0)</CommandText>
      </Query>
      <Fields>
