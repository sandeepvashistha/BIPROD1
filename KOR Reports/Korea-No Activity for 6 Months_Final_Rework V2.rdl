  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>9ec3007f-0c9c-485b-a03e-9f6a59b08482</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT 
  T.DIST_ID AS BA_NUMBER,
  T.REAL_NATIVE_NAME_1 AS NATIVE_NAME_1,
  T.ENTRY_DATE,
  T.DIST_STATUS AS DISTRIBUTOR_STATUS_CODE,
  MAX(T.order_entry_date) AS MAX_ENTRY_DATE
  FROM (
SELECT
  DW.dbo.DST.DIST_ID,
  DW.dbo.DST.REAL_NATIVE_NAME_1,
  DW.dbo.DST.ENTRY_DATE,
  DW.dbo.DST.DIST_STATUS,
  ISNULL(DW.dbo.ODH.ENTRY_DATE,'1999-01-01') as order_entry_date
FROM
  DW.dbo.DST 
  LEFT OUTER JOIN DW.dbo.ODH ON
  (DW.dbo.DST.dist_id=DW.dbo.ODH.dist_id AND DW.dbo.ODH.STATUS &lt;&gt; 9 AND DW.dbo.ODH.ODH_TYPE &lt;&gt; 'C')
WHERE
   DW.dbo.DST.UNI_MKTING_UNIT  =  82
   AND   DW.dbo.DST.DIST_ID NOT IN (15745082,117097282,102117582,102298382,101466882)
   AND   DW.dbo.DST.DIST_STATUS  IN  ( 'A','B')
   AND   DW.dbo.DST.ENTRY_DATE  &lt;  Dateadd(Month, Datediff(Month, 0, DATEADD(m, -5, GETDATE())), 0)
   AND ISNULL(DW.dbo.DST.real_native_name_1,'A') NOT LIKE '%AFP%'
UNION ALL
select  
d.DIST_ID,
  d.REAL_NATIVE_NAME_1,
  d.ENTRY_DATE,
  d.DIST_STATUS,
   ISNULL(o.ENTRY_DATE,'1999-01-01') as order_entry_date
from DW.dbo.DST d 
inner join DW.dbo.DST DD on dd.sponsor_dist_id= d.dist_id and dd.dist_status='H'
inner join DW.dbo.ODH O on o.dist_id=dd.dist_id
where d.uni_mkting_unit=82 and d.DIST_STATUS  IN  ( 'A','B')
--and d.mdist='82-142838682'
AND D.ENTRY_DATE  &lt;  Dateadd(Month, Datediff(Month, 0, DATEADD(m, -5, GETDATE())), 0)
AND O.STATUS &lt;&gt; 9 AND O.ODH_TYPE &lt;&gt; 'C'
AND ISNULL(d.real_native_name_1,'A') NOT LIKE '%AFP%' AND d.DIST_ID NOT IN (15745082,117097282,102117582,102298382)
) T
GROUP BY 
T.DIST_ID,
  T.REAL_NATIVE_NAME_1,
  T.ENTRY_DATE,
  T.DIST_STATUS
HAVING MAX(T.order_entry_date) &lt; Dateadd(Month, Datediff(Month, 0, DATEADD(m, -6, GETDATE())), 0);</CommandText>
      </Query>
      <Fields>
        <Field Name="BA_NUMBER">
          <DataField>BA_NUMBER</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NATIVE_NAME_1">
          <DataField>NATIVE_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ENTRY_DATE">
          <DataField>ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DISTRIBUTOR_STATUS_CODE">
          <DataField>DISTRIBUTOR_STATUS_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MAX_ENTRY_DATE">
          <DataField>MAX_ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="R_Status_Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  DW.dbo.DST.DIST_ID AS BA_NUMBER,
  ISNULL(DW.dbo.DST.REAL_NATIVE_NAME_1,DW.dbo.DST.NATIVE_NAME_1) AS NATIVE_NAME_1,
  DW.dbo.DST.ENTRY_DATE AS DISTRIBUTOR_ENTRY_DATE,
  DW.dbo.DST.DIST_STATUS AS DISTRIBUTOR_STATUS_CODE
FROM
  DW.dbo.DST
WHERE
  ( 
  ( ( (DW.dbo.DST.UNI_MKTING_UNIT = 81 OR DW.dbo.DST.HFLAG_2 = 'X')  )  )
  OR  ( ( (DW.dbo.DST.UNI_MKTING_UNIT = 82 OR DW.dbo.DST.HFLAG_2 = 'X')  )  )
  )  AND  
  (
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'KOR'  )
   AND
   DW.dbo.DST.DIST_STATUS  IN  ( 'R'  )
  )</CommandText>
      </Query>
      <Fields>
        <Field Name="BA_NUMBER">
          <DataField>BA_NUMBER</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NATIVE_NAME_1">
          <DataField>NATIVE_NAME_1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DISTRIBUTOR_ENTRY_DATE">
          <DataField>DISTRIBUTOR_ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DISTRIBUTOR_STATUS_CODE">
          <DataField>DISTRIBUTOR_STATUS_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
