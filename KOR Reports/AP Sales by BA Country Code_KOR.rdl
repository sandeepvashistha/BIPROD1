  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Month">
            <Value>=Parameters!PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) As PV_Month,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.DST.COUNTRY_CODE As Dist_Country,
  DW.dbo.ODH.COUNTRY_CODE As Order_Country,
  DW.dbo.DST.UNI_MKTING_UNIT,
  sum(DW.dbo.ODH.price_1 / isnull(DW.dbo.VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) AS AMOUNT_USD,
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) AS AMOUNT_LOCAL
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID )
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( DW.dbo.VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
  AND ( DW.dbo.ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
  And 
  DW.dbo.VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' 
  )
WHERE
  (
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  &gt;=  @PV_Month  --@Prompt
   AND
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'KOR'  )
  )
GROUP BY
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)),
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.DST.UNI_MKTING_UNIT</CommandText>
      </Query>
