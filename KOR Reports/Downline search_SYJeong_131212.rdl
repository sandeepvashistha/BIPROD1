  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SponsorBANumber">
            <Value>=Parameters!SponsorBANumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PVDate">
            <Value>=Parameters!PVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	DISTINCT
	SAPDW_GENIE2.SPONSOR_DIST_ID,
	GENIE.DIST_NAME,
	substring(GENIE.MDIST,4,20) MDIST,
	CONVERT(DATETIME,LTRIM(RTRIM(BWT.PV_DATE))+'01',103) AS PV_DATE,
	GENIE.GROUP_LEVEL
FROM
	DW.DBO.GENIE
	INNER JOIN DW.DBO.DST  SAPDW_GENIE2 ON (GENIE.TOP_MSPON=SAPDW_GENIE2.MDIST)
	INNER JOIN DW.DBO.BWT BWT ON (GENIE.MDIST=BWT.MDIST)
WHERE
	SAPDW_GENIE2.SPONSOR_DIST_ID  IN  (@SponsorBANumber) 
	AND (CONVERT(DATETIME,LTRIM(RTRIM(BWT.PV_DATE))+'01',103))  = @PVDate
	AND substring(SAPDW_GENIE2.MSPON,1,2)='82'</CommandText>
      </Query>
      <Fields>
        <Field Name="SPONSOR_DIST_ID">
          <DataField>SPONSOR_DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DIST_NAME">
          <DataField>DIST_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MDIST">
          <DataField>MDIST</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PV_DATE">
          <DataField>PV_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="GROUP_LEVEL">
          <DataField>GROUP_LEVEL</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
