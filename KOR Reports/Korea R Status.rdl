  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT m.dist_id, m.entry_date, m.dist_status, m.max_date as Order_Month
FROM
(SELECT t.dist_id, real_native_name_1,t.entry_date, t.dist_status, t.order_date, DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0) as current_mth, t.max_date, 
 CASE WHEN t.order_date IS NOT NULL 
  THEN DATEDIFF(MM,DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0),t.order_date) 
    ELSE DATEDIFF(MM,DATEADD(month, DATEDIFF(month, 0, GETDATE()), 0),DATEADD(month, DATEDIFF(month, 0, t.entry_date), 0)) 
END as mths
FROM 
(
SELECT
  DW.dbo.DST.DIST_ID,
  DW.dbo.DST.REAL_NATIVE_NAME_1,
  DW.dbo.DST.ENTRY_DATE,
  DW.dbo.DST.DIST_STATUS,
  max(DW.dbo.ODH.ENTRY_DATE) as max_date,
  DATEADD(month, DATEDIFF(month, 0, MAX(ODH.ENTRY_DATE)), 0) as order_date
FROM
  DW.dbo.DST
  LEFT OUTER JOIN DW.dbo.ODH ON DW.dbo.DST.dist_id=DW.dbo.ODH.dist_id
WHERE
   DW.dbo.DST.UNI_MKTING_UNIT  =  82
   AND
   DW.dbo.DST.DIST_ID NOT IN (15745082,12345678)
   AND
   DW.dbo.DST.DIST_STATUS  IN  ( 'A','B'  )
   AND   DW.dbo.DST.ENTRY_DATE  &lt;  DATEADD(month,-6,GETDATE())
   --AND DST.DIST_ID = 4312982

 
     
 
GROUP BY
  DW.dbo.DST.DIST_ID, 
  DW.dbo.DST.REAL_NATIVE_NAME_1, 
  DW.dbo.DST.ENTRY_DATE, 
  DW.dbo.DST.DIST_STATUS
) t 
) m
WHERE (m.mths &gt;= 7 OR m.max_date IS NULL)
AND m.real_native_name_1 NOT LIKE '%AFP%'

ORDER BY m.entry_date DESC

</CommandText>
      </Query>
      <Fields>
        <Field Name="dist_id">
          <DataField>dist_id</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="entry_date">
          <DataField>entry_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="dist_status">
          <DataField>dist_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Order_Month">
          <DataField>Order_Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
