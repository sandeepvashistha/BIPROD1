<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>6f59d4d1-ea4a-4069-b6ed-a740f5d262db</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostDateStart">
            <Value>=Parameters!PostDateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PostDateEnd">
            <Value>=Parameters!PostDateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	ODP.DIST_ID,
	ODP.ORDER_NUMBER,
	ODP.RECEIPT_TYPE,
	RTY.DESCRIPTION,
	ODP.POST_DATE,
	count(distinct(ODP.PAYMENT_NUMBER)) PAYMENT_COUNT
FROM
	DW.dbo.ODH ODH
	INNER JOIN DW.dbo.ODP ODP ON (ODP.MORDER=ODH.MORDER)
	INNER JOIN DW.dbo.RTY RTY ON (RTY.MKTING_UNIT=ODP.MKTING_UNIT and RTY.RECEIPT_TYPE = ODP.RECEIPT_TYPE)
WHERE
	ODH.UNI_MKTING_UNIT=82 AND ODH.STATUS  BETWEEN  2  AND  5
	AND ODP.RECEIPT_TYPE  IN  (3,7,10,11,12,13,14,15)
	AND ODH.ODH_TYPE  NOT IN  ( 'C'  )
	AND ODP.POST_DATE  BETWEEN  @PostDateStart  AND  @PostDateEnd
GROUP BY
	ODP.DIST_ID, 
	ODP.ORDER_NUMBER, 
	ODP.RECEIPT_TYPE, 
	RTY.DESCRIPTION, 
	ODP.POST_DATE</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ORDER_NUMBER">
          <DataField>ORDER_NUMBER</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="RECEIPT_TYPE">
          <DataField>RECEIPT_TYPE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="POST_DATE">
          <DataField>POST_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PAYMENT_COUNT">
          <DataField>PAYMENT_COUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
