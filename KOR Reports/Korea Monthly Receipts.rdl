  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PostingStartMonth">
            <Value>=Parameters!PostingStartMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PostingEndMonth">
            <Value>=Parameters!PostingEndMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	a.[Posting Date] as [Posting Date],
	a.[Document No_] as [Receipt Number],
	a.[No_] as [Item Number],
	a.[Description],
	a.[Buy-from Vendor No_],
	b.[Name] as [Buy-from Vendor Name],
	a.[Quantity],
	c.[Last Direct Cost],
	a.[Quantity]*c.[Last Direct Cost] as [Value],
	c.[Standard Cost],
	c.[Overhead Rate],
	c.[Standard Material Cost]
FROM purch_rcpt_line_nc_vw a
LEFT OUTER JOIN vendor_nc_vw b
       ON (a.uni_company_name = b.uni_company_name AND a.[Buy-from Vendor No_] = b.[No_])
LEFT OUTER JOIN item_nc_vw c
       ON (a.uni_company_name = c.uni_company_name AND a.[No_] = c.[No_])
where 
	a.[Type] = 2 AND a.uni_company_name = 'Korea' AND a.[Quantity] &lt;&gt; 0
	--AND a.[Posting Date] &gt;= DateAdd(month, -3,CONVERT(DATETIME, CONVERT(DATE, GETDATE()))) removed and added filters
	AND DATEADD(month, DATEDIFF(month, 0, a.[Posting Date]), 0) &gt;= @PostingStartMonth
	AND DATEADD(month, DATEDIFF(month, 0, a.[Posting Date]), 0) &lt;= @PostingEndMonth</CommandText>
      </Query>
