    <DataSet Name="Details_Query">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartPostingDate">
            <Value>=Parameters!StartPostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPostingDate">
            <Value>=Parameters!EndPostingDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
ve.[Posting Date],
(case ve.[Item Ledger Entry Type] 
when 0 then 'Purchase'
when 1 then 'Sale'
when 2 then 'Positive Adjmnt.'
when 3 then 'Negative Adjmnt.'
when 4 then 'Transfer'
when 5 then 'Consumption'
when 6 then 'Output'
when 7 then ''
end) as [Item Ledger Entry Type]
,
cast(sum(ve.[Invoiced Quantity]) as numeric(24,0)) [Invoiced Quantity],ve.[Location Code],ve.[Item No_],item_nc_vw.[Description] [Item Name],
ve.[Reason Code],ac.[Inventory Adjmt_ Account],cast(item_nc_vw.[Standard Cost] as numeric(24,6)) [Standard Cost],
cast((item_nc_vw.[Standard Cost]*sum(ve.[Invoiced Quantity])) as numeric(24,6)) [Line Cost],ve.[User ID],
ve.[Document No_],ve.[Description] [Document Description]
from prod_navision.dbo.value_entry_nc_vw ve
left outer join
(
select a.uni_company_name,a.Code as [Reason Code], a.[Gen_ Prod_ Posting Group], b.[Inventory Adjmt_ Account]
from prod_navision.dbo.reason_code_nc_vw a
left outer join prod_navision.dbo.General_Posting_Setup_nc_vw b
on a.uni_company_name = b.uni_company_name
and a.[Gen_ Prod_ Posting Group] = b.[Gen_ Prod_ Posting Group]
where a.uni_company_name = 'Korea'
and isnull(b.[Gen_ Bus_ Posting Group],'') = ''
) ac on ac.uni_company_name=ve.uni_company_name  and ac.[Reason Code]=ve.[Reason Code]
left outer join prod_navision.dbo.item_nc_vw on item_nc_vw.uni_company_name=ve.uni_company_name and item_nc_vw.[No_] =ve.[Item No_]
where ve.uni_company_name='Korea' and ve.[Item Ledger Entry Type] in (2,3) and ve.[Entry Type] = 0 --Direct cost
and ve.[Source Code] = '' and ve.[Posting Date] between @StartPostingDate and @EndPostingDate --'2019-01-01' and '2019-01-10'
group by
ve.[Posting Date],(case ve.[Item Ledger Entry Type] 
when 0 then 'Purchase'
when 1 then 'Sale'
when 2 then 'Positive Adjmnt.'
when 3 then 'Negative Adjmnt.'
when 4 then 'Transfer'
when 5 then 'Consumption'
when 6 then 'Output'
when 7 then ''
end),
ve.[Location Code],ve.[Item No_],item_nc_vw.[Description],ve.[Reason Code],ac.[Inventory Adjmt_ Account],
item_nc_vw.[Standard Cost],ve.[User ID],ve.[Document No_],ve.[Description]
having sum(ve.[Invoiced Quantity])&gt;0
order by ve.[Posting Date],ve.[Location Code],ve.[Item No_]</CommandText>
      </Query>
      <Fields>
        <Field Name="Posting_Date">
          <DataField>Posting Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Ledger_Entry_Type">
          <DataField>Item Ledger Entry Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Invoiced_Quantity">
          <DataField>Invoiced Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Location_Code">
          <DataField>Location Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No_">
          <DataField>Item No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Name">
          <DataField>Item Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
