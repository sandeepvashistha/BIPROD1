  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Month">
            <Value>=Parameters!PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.DST.COUNTRY_CODE,
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) As PV_Month,
  DW.dbo.OTY.DESCRIPTION,
  sum(DW.dbo.ODH.PRICE_2) As Total_PV
FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.OTY
WHERE
  ( DW.dbo.OTY.MKTING_UNIT=DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.OTY.ORDER_TYPE=DW.dbo.ODH.ODH_TYPE  )
  AND  ( DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID  )
  AND  
  (
   (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) &gt;= @PV_Month
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 97  )
  )
GROUP BY
  DW.dbo.ODH.ENTRY_DATE, 
  DW.dbo.DST.COUNTRY_CODE, 
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)), 
  DW.dbo.OTY.DESCRIPTION</CommandText>
      </Query>
      <Fields>
        <Field Name="ENTRY_DATE">
          <DataField>ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="COUNTRY_CODE">
          <DataField>COUNTRY_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PV_Month">
          <DataField>PV_Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_PV">
          <DataField>Total_PV</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
