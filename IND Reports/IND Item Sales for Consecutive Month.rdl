    <DataSet Name="Item_Code">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select Distinct Item_Code from ODL
where Mkting_unit = '91'
and Item_Code in ('28390','28387','28396','30694','28393','27643','30697','28955','29605','29965','28993','30938','30939','30608','30607','29179','29189','29193','29183','29185','29181','29187','29191','29728','30605','30767','30768','29485')
Order By Item_Code</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_Code">
          <DataField>Item_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
