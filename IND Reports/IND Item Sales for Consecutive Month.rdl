  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Item_Code">
            <Value>=Parameters!Item_Code.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH Months AS 
    (
        SELECT DISTINCT 
             dst.dist_id,
             dst.roman_name_1,
             odl.item_code,
             odl.item_name_1,
             odh.PV_Period,
             Grp = DateDiff(Month, '20000101', CONVERT(DATE,'1/'+odh.PV_DATE,103))
         FROM  DW.dbo.DST 
         INNER JOIN DW.DBO.ODH ON (DST.DIST_ID = ODH.DIST_ID)
         INNER JOIN dw.dbo.odl on (odh.morder = odl.morder)
         WHERE
                --DST.DIST_ID IN ('108882901','110167201','114271501','112447601','108641101')
                --AND 
                --ODL.ITEM_CODE = '28387'
                ODL.ITEM_CODE = @Item_Code
                AND   
                ODH.Status in (2,3,4,5)
                AND 
                --CONVERT(DATETIME,'1/'+odh.PV_DATE,103) BETWEEN  '01-JAN-2017'  AND  '01-OCT-2017'
                CONVERT(DATETIME,'1/'+odh.PV_DATE,103) BETWEEN @Start_PV_Month and @End_PV_Month
     ), 
Anchors AS 
    (
         SELECT
              m.dist_id,
              m.roman_name_1,
              m.item_code,
              m.item_name_1,
              Ind = M.Grp + X.Offset
          FROM  Months M
          CROSS JOIN ( SELECT -1 UNION ALL SELECT 0 UNION ALL SELECT 1 ) X (Offset)
          GROUP BY
               M.dist_id,
               m.roman_name_1,
               m.item_code,
               m.item_name_1,
               M.Grp + X.Offset
           HAVING
                Count(*) = 3
      )
SELECT DISTINCT
            dst.dist_id,
            dst.roman_name_1,
            CONVERT(DATE,'1/'+odh.PV_DATE,103) pv_date,
            --odh.entry_date,
            odh.order_number,
            odh.PV_Period,
            odl.item_code,
            odl.item_name_1,--,Grp = DateDiff(Month, '20000101', CONVERT(DATE,'1/'+odh.PV_DATE,103))
             Sum(odl.Qty_Ordered) As Qty
FROM
             DW.dbo.DST
             INNER JOIN DW.DBO.ODH ON (DST.DIST_ID=ODH.DIST_ID)
             INNER JOIN dw.dbo.odl ON (odh.morder=odl.morder)
WHERE 
             --DST.DIST_ID IN ('108882901','110167201','114271501','112447601','108641101')
             --and   
             ODH.Status in (2,3,4,5)
             AND 
             --ODL.ITEM_CODE = '28387'
             ODL.ITEM_CODE = @Item_Code
             AND
             --CONVERT(DATETIME,'1/'+odh.PV_DATE,103) BETWEEN  '01-JAN-2017'  AND  '01-OCT-2017'
             CONVERT(DATETIME,'1/'+odh.PV_DATE,103) BETWEEN @Start_PV_Month and @End_PV_Month
              AND
              EXISTS ( SELECT 1 FROM Anchors A WHERE dst.dist_id = A.dist_id )
Group by
                   dst.dist_id,
	dst.roman_name_1,
	CONVERT(DATE,'1/'+odh.PV_DATE,103),
	--odh.entry_date,
	odh.order_number,
	odh.PV_Period,
	odl.item_code,
                   odl.item_name_1--,Grp = DateDiff(Month, '20000101', CONVERT(DATE,'1/'+odh.PV_DATE,103))
	--Sum(odl.Qty_Ordered) As Qty
ORDER BY 
                   dst.dist_id,
	CONVERT(DATE,'1/'+odh.PV_DATE,103)
	--odh.entry_date</CommandText>
      </Query>
