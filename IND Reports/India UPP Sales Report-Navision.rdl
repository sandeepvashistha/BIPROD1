  <DataSets>
    <DataSet Name="IND_UPP_Sales">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Posting_Date">
            <Value>=Parameters!Start_Posting_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Posting_Date">
            <Value>=Parameters!End_Posting_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
[dbo].[Customer_nc_vw].[no_],
[dbo].[Customer_nc_vw].[name],
[dbo].[Customer_nc_vw].[contact],
[dbo].[Customer_nc_vw].[county],
ISNULL(round(CUST_LEDGER.MONTH_AMOUNT,0),0) AMT,
round(((ISNULL(round(CUST_LEDGER.MONTH_AMOUNT,0),0)/(sum(CUST_LEDGER.MONTH_AMOUNT) over()))*100),2) AMT_PERCENT
from [dbo].[Customer_nc_vw]
left outer join
(
select 
[dbo].[Cust_Ledger_Entry_nc_vw].[uni_company_name],
[dbo].[Cust_Ledger_Entry_nc_vw].[customer no_],
round(sum([dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[amount]),0) MONTH_AMOUNT 
from 
[dbo].[Cust_Ledger_Entry_nc_vw]
inner join [dbo].[Detailed_Cust_Ledg_Entry_nc_vw] on 
([dbo].[Cust_Ledger_Entry_nc_vw].[uni_company_name]=[dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[uni_company_name]
and [dbo].[Cust_Ledger_Entry_nc_vw].[entry no_]=[dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[Cust_ Ledger Entry No_]
and [dbo].[Cust_Ledger_Entry_nc_vw].[document type]=[dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[document type])
where 
[dbo].[Cust_Ledger_Entry_nc_vw].[uni_company_name]='India'
--and [dbo].[Cust_Ledger_Entry_nc_vw].[document no_]='PS1001139'
and ([dbo].[Cust_Ledger_Entry_nc_vw].[posting date] between @Start_Posting_Date  AND  @End_Posting_Date) 
and [dbo].[Cust_Ledger_Entry_nc_vw].[document type]=2 and [dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[entry type]=1
and [dbo].[Detailed_Cust_Ledg_Entry_nc_vw].[document type]=2
group by [dbo].[Cust_Ledger_Entry_nc_vw].[uni_company_name],[dbo].[Cust_Ledger_Entry_nc_vw].[customer no_]
) CUST_LEDGER on ([dbo].[Customer_nc_vw].[uni_company_name] = CUST_LEDGER.[uni_company_name] and
[dbo].[Customer_nc_vw].[no_]=CUST_LEDGER.[customer no_])
where [dbo].[Customer_nc_vw].[uni_company_name]='India' and [dbo].[Customer_nc_vw].[no_] not like 'CU%'
order by AMT DESC,[dbo].[Customer_nc_vw].[county],[dbo].[Customer_nc_vw].[no_]</CommandText>
      </Query>
