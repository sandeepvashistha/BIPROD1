<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>d2aa0542-ccde-46e2-b83b-0840276fcc8d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartPVDate">
            <Value>=Parameters!StartPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)) as pv_date,
  DW.dbo.ODH.ODH_TYPE,
  (CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) as sales_local,
  DW.dbo.ODH.MORDER,
  DW.dbo.ODH.ENTRY_DATE,
  DW.dbo.ODH.POST_DATE,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.ODH.TOT_INVCE_AMOUNT,
  sum(DW.dbo.ODH.PRICE_2) as pv_amount,
  sum(CONVERT(FLOAT,RTRIM(LTRIM(DW.dbo.ODP.AMOUNT_RECD)))) as amount_recieved
FROM
  DW.dbo.ODH, 
  DW.dbo.ODP
WHERE
	ODH.MORDER = ODP.MORDER
	AND 
  (
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   (
    DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 91  )
    OR
    DW.dbo.ODH.COUNTRY_CODE  IN  ( 'IND'  )
   )
   AND
   (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103))  &gt;=  @StartPVDate 
   AND 
    (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103))  &lt;=  @EndPVDate
  )
GROUP BY
  (CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)), 
  DW.dbo.ODH.ODH_TYPE, 
  (CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END), 
  DW.dbo.ODH.MORDER, 
  DW.dbo.ODH.ENTRY_DATE, 
  DW.dbo.ODH.POST_DATE, 
  DW.dbo.ODH.WAREHOUSE, 
  DW.dbo.ODH.TOT_INVCE_AMOUNT</CommandText>
      </Query>
