  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select 
   DW.dbo.DST.Dist_ID, 
   DW.dbo.DST.Roman_Name_1, 
   DW.dbo.DST.Dist_Status, 
   DW.dbo.DST.Entry_Date,
   DW.dbo.DST.Addr, 
   DW.dbo.DST.Addr_2, 
   DW.dbo.DST.Addr_3, 
   COALESCE(DW.dbo.DST.Home_Phone, DW.dbo.DST.Work_Phone, DW.dbo.DST.Cell_Phone) As Contact_Number,
   DW.dbo.DST.Email_Addr,
   DW.dbo.DST.Sponsor_Dist_ID, 
   SAPDW_SPONSOR.ROMAN_NAME_1 as Sponsor_Name, 
   DW.dbo.DST.Enroller_ID,
   SAPDW_DST_ENROLLER.ROMAN_NAME_1 as Enroller_Name,
   DW.dbo.ODH.Order_Number,
   DW.dbo.ODH.Entry_Date As Order_Date,
   DW.dbo.ODH.Status,
   DW.dbo.ODH.Odh_Type,
   DW.dbo.ODH.Ship_To_Addr_1,
   DW.dbo.ODH.Ship_To_Addr_2,
   DW.dbo.ODH.Ship_to_Addr_3,
   DW.dbo.ODL.Item_Code,
   DW.dbo.ODL.Item_Name_1,
   DW.dbo.ODL.Qty_Ordered,
   DW.dbo.ODH.Warehouse
from 
   DW.dbo.DST
   LEFT OUTER JOIN DW.dbo.DST  SAPDW_SPONSOR ON ( DW.dbo.DST.SPONSOR_DIST_ID = SAPDW_SPONSOR.DIST_ID )
   LEFT OUTER JOIN DW.dbo.DST  SAPDW_DST_ENROLLER ON ( SAPDW_DST_ENROLLER.DIST_ID = DW.dbo.DST.ENROLLER_ID )
   LEFT OUTER JOIN DW.dbo.ODH ON ( DW.dbo.DST.Dist_ID = Dw.dbo.ODH.Dist_ID and DW.dbo.DST.Uni_Mkting_Unit = Dw.dbo.ODH.Uni_Mkting_Unit And DW.dbo.ODH.Status in (2,3,4,5))
   LEFT OUTER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.Morder = DW.dbo.ODL.Morder)
Where 
   DW.dbo.DST.Uni_Mkting_Unit = '91'
   And
   DW.dbo.DST.Country_Code = 'IND'
   /*And
   DW.dbo.DST.Addr is Null*/
   And
   DW.dbo.DST.Dist_Status = 'C'
   And
   DW.dbo.DST.Entry_Date &gt;= '01-Jan-2020'
   And
   (
   DW.dbo.DST.Sponsor_Dist_Id not in ('108639101','2')
   Or
   DW.dbo.DST.Enroller_ID not in ('108639101','2')
   )
   And
   DW.dbo.DST.Roman_Name_1 &lt;&gt; 'koziara, howard'
   And
   DW.dbo.ODH.Order_Source in ('100','900','901','906','909')
   </CommandText>
      </Query>
