  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select
         odh.order_number
       , odh.post_date
       , odp.entry_date as cc_entry_date --because of the how infotrax works, this should equal post_date in 99% of cases
       , odp.entry_time as cc_entry_time
       , odh.entry_init
       , odh.dist_id
       , dst.roman_name_1 as distributor_name
       , odh.ship_to_country --maybe filter for just japan
       , odh.warehouse --maybe filter for just certain warehouses
       , odp.payment_number --this is the payment line
       , a.payment_count as num_pay_lines_in_order --this is how many total payment lines in the order
       , odp.receipt_type
       , rty.[description] as payment_description
       , amount_recd as payment_line_amount
       , odh.tot_invce_amount as total_order_amount --note this might be confusing, since summing this column will give bad data
	   , case cc_type
                    when 'B' then 'JCB'
                    when 'A' then 'AMEX'
                    when 'V' then 'Visa'
                    when 'M' then 'Mastercard'
                    else 'Other'
                    end as orig_cc_type
       , cc_type2 as updated_cc_type
       , cc_name
from dw.dbo.odh
join dw.dbo.odp
on odh.morder = odp.morder
join dw.dbo.dst
on odh.dist_id = dst.dist_id
left outer join dw.dbo.rty
on odp.mkting_unit = rty.mkting_unit
and odp.receipt_type= rty.receipt_type
join
(
       select morder, count(*) as payment_count
       from dw.dbo.odp where mkting_unit=81
       group by morder
) a
on odh.morder = a.morder
where odh.uni_mkting_unit = 81
and cast(odh.post_date as date) = cast(getdate() as date)
and odp.receipt_type in (3,21)
order by odp.entry_date, odp.entry_time</CommandText>
      </Query>
