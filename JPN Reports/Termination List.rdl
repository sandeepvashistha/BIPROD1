  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  DST.DIST_ID,
  ISNULL(DST.REAL_NATIVE_NAME_1,DST.NATIVE_NAME_1) as Native_Name,
  DST.ENTRY_DATE,
  DST.ENTRY_PV_DATE_DATE,
  DST.TERMINATE_DATE,
  DST.DIST_STATUS
FROM
  DW.dbo.DST
WHERE
   DST.UNI_MKTING_UNIT = 81 AND DST.COUNTRY_CODE = 'JPN'
   AND DST.DIST_STATUS='T'
   AND (CAST(DST.TERMINATE_DATE AS DATE) &gt;= CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE())-1, 0) AS DATE)
   OR (DST.TERMINATE_DATE IS NULL AND CAST(DST.ENTRY_DATE AS DATE) &gt;= CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE())-1, 0) AS DATE)))</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Native_Name">
          <DataField>Native_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ENTRY_DATE">
          <DataField>ENTRY_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ENTRY_PV_DATE_DATE">
          <DataField>ENTRY_PV_DATE_DATE</DataField>
