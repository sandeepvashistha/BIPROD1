    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT ODH_LIVE.ORDER_NUMBER [Order #],ODH_LIVE.DIST_ID [Distributor#],ODH_LIVE.PV_DATE [Comission Period],ODH_LIVE.BCH_NUM [Batch #],
ODH_LIVE.ENTRY_INIT [Entered],'' Blank1,ODH_LIVE.RELEASED_INIT [Released],'' Blank2,OSP.PACKAGE_SHIP_DATE [shipped date (Blank=Today)],
ODH_LIVE.SHIP_TO_NAME [Name],'' Blank3,ODH_LIVE.SHIP_TO_ADDR_1 [Zip Code],ODH_LIVE.SHIP_TO_ADDR_2 [Ship to Adress 1],
ODH_LIVE.SHIP_TO_ADDR_3 [Ship to Adress 2],ODH_LIVE.SHIP_TO_ADDR_4 [Ship to Adress 3],ODH_LIVE.SHIP_TO_PHONE [Phone#],
ODH_LIVE.EMAIL_ADDRESS [mail adress],ODH_LIVE.PRICE_1 [sub total of orderd items],ODH_LIVE.PRICE_2 [Total PV],
ODH_LIVE.SALES_TAX [amount of tax],ODH_LIVE.FREIGHT_AMOUNT [Shipping fee],ODH_LIVE.TOT_INVCE_AMOUNT [Total amount],'11111' Barcode1,'A11111A' Barcode2,
DST.REAL_NATIVE_NAME_1 [Bill to Name],DST.ADDR [Bill to Zip Code],DST.ADDR_2 [Bill to Adress 1],DST.ADDR_3 [Bill to Adress 2],
DST.ADDR_4 [Bill to Adress 3],DST.HOME_PHONE [Bill to Phone Number],DST.WORK_PHONE [Bill to Phone Number 2],
OSH.SHIPMENT_NUMBER [Shipmnet#],1 [Number of ctn],'NEEDS TO CHECK' [Carton#],OSP.BOX_ID [Carton size],
'none' Blank4,'none' Blank5,ODH_LIVE.DELIVERY_TIME [Time Zone],ODL_LIVE.ORDER_LINE [Line number],ODL_LIVE.ITEM_CODE [Item Code],
ODL_LIVE.ITEM_NAME_1 [Item Name],ODL_LIVE.QTY_ORDERED [Order Qty],'' Blank6,ODL_LIVE.QTY_SHIPPED [Shipped Qty],
ODL_LIVE.QTY_BO_FILLED [Backorder qty],ODL_LIVE.TAXABLE_PRICE [amount total w/O tax],ODL_LIVE.SALES_PRICE [amount total w tax],
ODL_LIVE.PRICE_2 PV,'none' [Pick Location]
FROM
 DW.DBO.ODH_LIVE
 INNER JOIN DW.DBO.ODL_LIVE ON ODH_LIVE.MORDER=ODL_LIVE.MORDER
 INNER JOIN DW.DBO.ITD ON ITD.MITEM COLLATE DATABASE_DEFAULT=ODL_LIVE.MITEM
 LEFT OUTER JOIN DW.DBO.DST ON ODH_LIVE.DIST_ID=DST.DIST_ID
 LEFT OUTER JOIN DW.DBO.OSH ON ODH_LIVE.MORDER=OSH.MORDER
 LEFT OUTER JOIN DW.DBO.OSP ON OSH.MORDER=OSP.MORDER
WHERE ODH_LIVE.UNI_MKTING_UNIT=81 AND ODH_LIVE.STATUS IN (1,2,3,4,5) AND (ODL_LIVE.FLAG_2 IS NULL OR ODL_LIVE.FLAG_2='C')
AND ODH_LIVE.ODH_TYPE&lt;&gt;'C' AND ITD.SALES_CATEGORY&lt;&gt;'S'
AND ODH_LIVE.INVOICE_PRINT_DATE BETWEEN @BeginInvoiceDate AND @EndInvoiceDate
--AND ODH_LIVE.ORDER_NUMBER=416722393
ORDER BY ODH_LIVE.ORDER_NUMBER,ODL_LIVE.ORDER_LINE DESC</CommandText>
      </Query>
    </DataSet>
  </DataSets>
