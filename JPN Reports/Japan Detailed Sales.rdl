  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BeginInvoiceDate">
            <Value>=Parameters!BeginInvoiceDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndInvoiceDate">
            <Value>=Parameters!EndInvoiceDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select odh.uni_mkting_unit,odh.country_code,odh.order_number,odh.status,odh.odh_type,oty.description odh_type_desc,
odh.entry_date,odh.invoice_print_date,odh.pv_date_date,odl.item_code,itd.item_name_1,
sum(case when odh.odh_type in ('C','R') then -1*odl.qty_ordered else odl.qty_ordered end) qty_ordered,
sum(case when odh.odh_type in ('C','R') then -1*odl.qty_shipped else odl.qty_shipped end) qty_shipped,
sum(case when odh.odh_type in ('C','R') then -1*odl.qty_bo_filled else odl.qty_bo_filled end) qty_bo_filled,
sum(case when odh.odh_type in ('C','R') then -1*odl.price_1 else odl.price_1 end) sales_price,
sum(case when odh.odh_type in ('C','R') then -1*(odl.price_3*odl.qty_ordered) else (odl.price_3*odl.qty_ordered) end) total_amount_before_tax,
sum(case when odh.odh_type in ('C','R') then -1*(odl.unit_sales_tax*odl.qty_ordered) else (odl.unit_sales_tax*odl.qty_ordered) end) total_sales_tax,
sum(case when odh.odh_type in ('C','R') then -1*(odl.price_1*odl.qty_ordered) else (odl.price_1*odl.qty_ordered) end) total_amount
from
dw.dbo.odh inner join dw.dbo.odl on odh.morder=odl.morder
left outer join dw.dbo.itd on odl.mkting_unit=itd.uni_mkting_unit and odl.item_code=itd.item_code
left outer join dw.dbo.oty on oty.mkting_unit=odh.uni_mkting_unit and oty.order_type=odh.odh_type
where
odh.uni_mkting_unit=81 and odh.country_code='JPN' and odh.status in (2,3,4,5) and (odl.flag_2 is null or odl.flag_2='C')
and odh.invoice_print_date between @BeginInvoiceDate and @EndInvoiceDate
group by
odh.uni_mkting_unit,odh.country_code,odh.order_number,odh.status,odh.odh_type,oty.description,
odh.entry_date,odh.invoice_print_date,odh.pv_date_date,odl.item_code,itd.item_name_1
order by odh.invoice_print_date,odh.order_number,odl.item_code</CommandText>
      </Query>
