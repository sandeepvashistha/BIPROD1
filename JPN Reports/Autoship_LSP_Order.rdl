    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>58808b11-5da4-47e9-b13c-6e4b962908fe</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Date">
            <Value>=Parameters!PV_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.MORDER,
  DW.dbo.ODL.ITEM_CODE,
  sum(DW.dbo.ODL.QTY_ORDERED) as Total_Qty
FROM
  DW.dbo.ODH,
  DW.dbo.ODL
WHERE
  ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER  )
  AND
  DW.dbo.ODH.Country_Code = 'JPN'
  And  
  (
   DW.dbo.ODL.MKTING_UNIT  =  81
   AND
   DW.dbo.ODL.ITEM_CODE  IN  ( '22252','22253','24435','24436'  )
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  =  @PV_Date
   AND
   DW.dbo.ODH.ORDER_SOURCE  =  903
  )
GROUP BY
  DW.dbo.ODH.MORDER, 
  DW.dbo.ODL.ITEM_CODE</CommandText>
      </Query>
      <Fields>
        <Field Name="MORDER">
          <DataField>MORDER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ITEM_CODE">
          <DataField>ITEM_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Qty">
          <DataField>Total_Qty</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
