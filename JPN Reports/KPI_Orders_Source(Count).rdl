  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>ec957741-7130-428a-9ade-6e2f82d9e342</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Date">
            <Value>=Parameters!Start_PV_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Date">
            <Value>=Parameters!End_PV_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  count (distinct(DW.dbo.ODH.MORDER)) as Order_Count,
  DW.dbo.ODS.DESCRIPTION,
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103)) as PV_Date
FROM
  DW.dbo.ODH,
  DW.dbo.ODS
WHERE
  ( DW.dbo.ODH.UNI_MKTING_UNIT = DW.dbo.ODS.MKTING_UNIT and DW.dbo.ODH.ORDER_SOURCE = DW.dbo.ODS.ODS_NUMBER  )
  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  81
   AND
   DW.dbo.ODH.STATUS  IN  ( 2,3,4,5 )
   AND
   DW.dbo.ODH.ODH_TYPE  &lt;&gt;  'C'
   AND
   (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))  BETWEEN @Start_PV_Date  AND  @End_PV_Date
  )
GROUP BY
  DW.dbo.ODS.DESCRIPTION, 
  (CONVERT(DATETIME,'1/'+DW.dbo.ODH.PV_DATE,103))</CommandText>
      </Query>
      <Fields>
        <Field Name="Order_Count">
          <DataField>Order_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PV_Date">
          <DataField>PV_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
