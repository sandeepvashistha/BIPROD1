  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b7f95d21-b5bf-4012-ab6e-fc2e1f192049</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT 
	odh.ORDER_NUMBER, 
	odl.ITEM_CODE, 
	Sum(odl.QTY_SHIPPED) AS [QTY_SHIPPEDの合計], 
	Sum(odl.QTY_BO_FILLED) AS [QTY_BACK_ORDERの合計]
FROM dw.dbo.odh_live odh 
inner join dw.dbo.odl_live odl on odh.morder=odl.morder
WHERE odh.UNI_MKTING_UNIT=81 and odh.status in (2,3,4,5) AND odh.odh_type&lt;&gt;'C' --AND tbl_osl.ORDER_NUMBER=416084007
AND (ODL.FLAG_2 IS NULL OR ODL.FLAG_2='C')
--and odh.ENTRY_DATE&gt;='2019-08-01'
GROUP BY odh.ORDER_NUMBER, odl.ITEM_CODE
HAVING (Sum(odl.QTY_BO_FILLED)&lt;&gt;0 and Sum(odl.QTY_BO_FILLED)&gt;Sum(odl.QTY_SHIPPED))
ORDER BY odh.ORDER_NUMBER,odl.ITEM_CODE;</CommandText>
      </Query>
      <Fields>
        <Field Name="ORDER_NUMBER">
          <DataField>ORDER_NUMBER</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ITEM_CODE">
          <DataField>ITEM_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QTY_SHIPPEDの合計">
          <DataField>QTY_SHIPPEDの合計</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="QTY_BACK_ORDERの合計">
          <DataField>QTY_BACK_ORDERの合計</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
