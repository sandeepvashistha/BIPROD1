    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  count (distinct(DW.dbo.DST.mdist)) as Dist_Count,
  DW.dbo.DST.DIST_STATUS
FROM
  DW.dbo.DST
WHERE
  ( (DW.dbo.DST.UNI_MKTING_UNIT = 81 OR DW.dbo.DST.HFLAG_2 = 'X')  )
  AND 
  DW.dbo.DST.Country_Code = 'JPN'
  And 
  (
   DW.dbo.DST.UNI_MKTING_UNIT  =  81
   AND
   DW.dbo.DST.DIST_STATUS  &lt;&gt;  'T'
  )
GROUP BY
  DW.dbo.DST.DIST_STATUS</CommandText>
      </Query>
      <Fields>
        <Field Name="Dist_Count">
          <DataField>Dist_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DIST_STATUS">
          <DataField>DIST_STATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
