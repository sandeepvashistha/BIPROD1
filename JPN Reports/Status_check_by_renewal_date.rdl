  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Next_Renew_Date">
            <Value>=Parameters!Next_Renew_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.DST.DIST_ID,
  ISNULL(DW.dbo.DST.REAL_NATIVE_NAME_1,DW.dbo.DST.NATIVE_NAME_1) as Native_Name,
  DW.dbo.DST.DIST_STATUS,
  DW.dbo.DST.ENTRY_DATE,
  MAX(ISNULL(DW.dbo.RNW.NEXT_RENEW_DATE,DW.dbo.DST.NEXT_RENEW_DATE)) as Next_Renew_Date
FROM
  DW.dbo.DST
  LEFT OUTER JOIN DW.dbo.RNW ON ( DW.dbo.DST.MDIST = DW.dbo.RNW.MDIST )
WHERE
  ( (DW.dbo.DST.UNI_MKTING_UNIT = 81 OR DW.dbo.DST.HFLAG_2 = 'X') )
  AND  
  (
   DW.dbo.DST.UNI_MKTING_UNIT  =  81
   And 
   DW.dbo.DST.Country_Code = 'JPN'
   AND
   DW.dbo.DST.DIST_STATUS  NOT IN  ( 'T','H','E','C','R'  )
  )
GROUP BY
  DW.dbo.DST.DIST_ID, 
  ISNULL(DW.dbo.DST.REAL_NATIVE_NAME_1,DW.dbo.DST.NATIVE_NAME_1), 
  DW.dbo.DST.DIST_STATUS, 
  DW.dbo.DST.ENTRY_DATE
HAVING
  MAX(ISNULL(DW.dbo.RNW.NEXT_RENEW_DATE,DW.dbo.DST.NEXT_RENEW_DATE))  &lt;=  @Next_Renew_Date</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
