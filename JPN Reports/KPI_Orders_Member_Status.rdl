  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>97881634-fcd4-4eac-b403-eced59eb61b4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Ship_Verified_Month">
            <Value>=Parameters!Start_Ship_Verified_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Ship_Verified_Month">
            <Value>=Parameters!End_Ship_Verified_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.DISTRIBUTOR_STATUS,
  count (distinct(DW.dbo.DST.mdist)) as Dist_count,
  --trunc(DW.dbo.ODH.VERIFY_DATE,'MM')
  DATEADD(mm, DATEDIFF(mm,0,DW.dbo.ODH.VERIFY_DATE),0) as Ship_Verified_Month
FROM
  DW.dbo.ODH,
  DW.dbo.DST
WHERE
  ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID and DW.dbo.DST.UNI_MKTING_UNIT = DW.dbo.ODH.UNI_MKTING_UNIT )
  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  81
   AND
   DW.dbo.ODH.Country_Code = 'JPN'
   And
   DW.dbo.ODH.ODH_TYPE  &lt;&gt;  'C'
   AND
   DW.dbo.ODH.STATUS In (1,2,3,4,5)
   AND
   DATEADD(mm, DATEDIFF(mm,0,DW.dbo.ODH.VERIFY_DATE),0)  BETWEEN @Start_Ship_Verified_Month  AND  @End_Ship_Verified_Month
   )
GROUP BY
  DW.dbo.ODH.DISTRIBUTOR_STATUS, 
  DATEADD(mm, DATEDIFF(mm,0,DW.dbo.ODH.VERIFY_DATE),0)</CommandText>
      </Query>
      <Fields>
        <Field Name="DISTRIBUTOR_STATUS">
          <DataField>DISTRIBUTOR_STATUS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Dist_count">
          <DataField>Dist_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Ship_Verified_Month">
          <DataField>Ship_Verified_Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
