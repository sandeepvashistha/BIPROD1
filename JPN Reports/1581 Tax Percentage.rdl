  <DataSources>
    <DataSource Name="ProdNavision">
      <DataSourceReference>ProdNavision</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b9b15cf5-6563-4010-bec1-79eef215ba48</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BeginPostingDate">
            <Value>=Parameters!BeginPostingDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPostingDate">
            <Value>=Parameters!EndPostingDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	a.[Posting Date],
	a.[Document No_],
	--a.[Type],
	a.[Buy-from Vendor No_],
	sum(case when a.[No_]&lt;&gt;1581 then a.[Amount] else 0 end) [Total of non-1581 accounts],
	a.[Individual amounts (non-1581)],
	sum(case when a.[No_]=1581 then a.[Amount] else 0 end) [1581 amount],
	cast(
		(case when	sum(case when a.[No_]&lt;&gt;1581 then a.[Amount] else 0 end)=0 then 0 else
					(sum(case when a.[No_]=1581 then a.[Amount] else 0 end)/sum(case when a.[No_]&lt;&gt;1581 then a.[Amount] else 0 end)) 
		end) as numeric(24,3)
		) [1581 percentage],
	max(a.[Entry No]) [Entry No]
from
	(
	select 
		pil.[Document No_],
		pil.[Type],pil.[No_],
		pil.[Buy-from Vendor No_],
		pil.[Posting Date],cast(pil.[Amount] as numeric(24,4)) [Amount],
		stuff( (SELECT ';'+format(pil1.[Amount],'##########.##')
				   FROM prod_navision.dbo.[Purch_Inv_line_nc_vw] pil1
				   WHERE pil1.uni_company_name='Japan' and pil1.[document no_]=pil.[document no_] 
				   and pil1.[Buy-from Vendor No_]=pil.[Buy-from Vendor No_] and pil1.[No_]&lt;&gt;1581
				   ORDER BY pil1.[Posting Date],pil1.[No_]
				   FOR XML PATH(''), TYPE).value('.', 'varchar(max)')
					,1,1,'') AS [Individual amounts (non-1581)],
		(select max(gl.[Entry No_]) as [Entry No]
		from prod_navision.dbo.g_l_entry_nc_vw gl
		where gl.uni_company_name = pil.uni_company_name and gl.[G_L Account No_] = pil.[No_]
        and gl.[Document No_]=pil.[Document No_]and	gl.[G_L Account No_] = '1581' and gl.[Document Type] = 2
        ) [Entry No]
	from	prod_navision.dbo.[Purch_Inv_line_nc_vw] pil
	where	uni_company_name='Japan' and pil.[Type]&lt;&gt;0 --and [document no_] in ('PI1008616','PI1008614') 
			and pil.[Posting Date] between @BeginPostingDate and @EndPostingDate
			--and pil.[Posting Date] between '2015-02-01' and '2015-02-28'
	) a
group by
	a.[Posting Date],
	a.[Document No_],
	--a.[Type],
	a.[Buy-from Vendor No_],
	a.[Individual amounts (non-1581)];</CommandText>
      </Query>
