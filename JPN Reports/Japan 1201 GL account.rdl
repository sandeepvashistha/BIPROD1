<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ProdNavision">
      <DataSourceReference>ProdNavision</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8e542f64-e866-40c8-b1d3-bc376d56eb18</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Summary">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select uni_company_name,[G_L Account No_],[Document No_], min([Posting Date]) as postmin, max([Posting Date]) as postmax, count(*) as num_trans, sum([Amount]) as net1201
, max(case when len([Document No_]) = 9 and left([Document No_],1) = '4' then 'InfoTrax' else 'Manual' end) as entry_type
from prod_navision.dbo.g_l_entry_nc_vw
where uni_company_name = 'Japan'
and [G_L Account No_] = '1201'
--and [Journal batch name] in ('ORDERS','PAYMENT')
group by uni_company_name,[G_L Account No_],[Document No_]
having sum([Amount]) &lt;&gt; 0
order by 6, 2</CommandText>
      </Query>
    </DataSet>
  </DataSets>
