    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PV_Date">
            <Value>=Parameters!PV_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.BWT.DIST_ID,
  DW.dbo.BWT.OVOL1,
  CONVERT(DATETIME,LTRIM(RTRIM(DW.dbo.BWT.PV_DATE))+'01',103) as PV_Date
FROM
  DW.dbo.BWT
WHERE
   DW.dbo.BWT.Country_Code = 'JPN'
   and
  (
   DW.dbo.BWT.MARKETING_UNIT  =  81
   AND
   CONVERT(DATETIME,LTRIM(RTRIM(DW.dbo.BWT.PV_DATE))+'01',103)  =  @PV_Date
  )</CommandText>
      </Query>
      <Fields>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OVOL1">
          <DataField>OVOL1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PV_Date">
          <DataField>PV_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
