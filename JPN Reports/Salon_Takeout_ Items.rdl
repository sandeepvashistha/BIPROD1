  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Verify_Date">
            <Value>=Parameters!Start_Verify_Date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Verify_Date">
            <Value>=Parameters!End_Verify_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODL.ITEM_CODE,
  Sum(DW.dbo.ODL.QTY_SHIPPED) As Total_Qty,
  Sum(DW.dbo.ODL.QTY_SHIPPED*DW.dbo.ODL.price_1) As Total_Amount,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.ITD.ITEM_NAME_1,
  DATEADD(mm, DATEDIFF(m,0,DW.dbo.ODH.VERIFY_DATE),0) As Ship_Verified_Month
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.ODL ON ( DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER )
  LEFT OUTER JOIN DW.dbo.ITD ON ( DW.dbo.ODL.MITEM COLLATE SQL_Latin1_General_CP1_CI_AS = DW.dbo.ITD.MITEM )
WHERE
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  81
   And
   DW.dbo.ODH.Country_Code = 'JPN'
   AND
   --DW.dbo.ODH.VERIFY_DATE  BETWEEN  '19-APRIL-2017'  AND  '24-APRIL-2017'   ----@Prompt
   DW.dbo.ODH.VERIFY_DATE  BETWEEN  @Start_Verify_Date  AND  @End_Verify_Date  
   AND
   DW.dbo.ODH.ODH_TYPE  IN  ( 'R','D','I','E'  )
   AND
   DW.dbo.ODL.ITEM_CODE  NOT IN  ( '19257','19258','19261','0100','19259','17109','19260','15811'  )
   AND
   DW.dbo.ODH.WAREHOUSE  =  2
  )
Group by
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODH.WAREHOUSE,
  DW.dbo.ITD.ITEM_NAME_1,
  DATEADD(mm, DATEDIFF(m,0,DW.dbo.ODH.VERIFY_DATE),0)</CommandText>
      </Query>
