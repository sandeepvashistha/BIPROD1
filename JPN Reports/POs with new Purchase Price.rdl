  <DataSources>
    <DataSource Name="ProdNavision">
      <DataSourceReference>ProdNavision</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>73a29bbe-0c8d-47e3-9731-be251391077f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 
       a.No_ as [PO No_]
       , a.[Buy-from Vendor No_]
       , a.[Buy-from Vendor Name]
       , a.[Document Date]
       , b.No_ as [Item No_]
       , d.[Description] as [Item Description]
       , cast(d.[Standard Cost] as numeric(20,5)) [Standard Cost]
       , cast(b.[Direct Unit Cost] as numeric(20,5)) as [PO Direct Unit Cost]
       , a.[Currency Code]
       , cast(a.[Currency Factor] as numeric(20,11)) [Currency Factor]
       , case when a.[Currency Code] &lt;&gt; '' then b.[Direct Unit Cost]/a.[Currency Factor] end as [PO Direct Unit Cost in LCY]
       , b.Quantity
	   ,case d.[Replenishment System] when 0 then 'Purchase' when 1 then 'Prod. Order' when 2 then 'Transfer' end as [Replenishment System]
	   ,row_number() over(partition by b.No_ order by a.[Document Date] desc,a.No_ desc) as rn
from Purchase_Header_nc_vw a
join Purchase_Line_nc_vw b
on a.uni_company_name = b.uni_company_name
and a.[Document Type] = b.[Document Type]
and a.No_ = b.[Document No_]
join Vendor_nc_vw c
on a.uni_company_name = c.uni_company_name
and a.[Buy-from Vendor No_] = c.No_
join Item_nc_vw d
on b.uni_company_name = d.uni_company_name
and b.No_ = d.No_
where a.uni_company_name = 'Performance Labs'
and [Document Date] &gt;= getdate()-14 --last 14 days
and c.[Vendor Type] &lt;&gt; 2 --exclude unicity vendors
--and a.No_='200091'
and b.[Type] = 2 --items only
and d.[Standard Cost] &lt;&gt; b.[Direct Unit Cost] --standard cost on item should be different than cost on PO</CommandText>
      </Query>
