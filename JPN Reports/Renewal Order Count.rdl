  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>94df4aba-2d8d-4517-94d9-b51f5ce3556a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Released_Month">
            <Value>=Parameters!Start_Released_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Released_Month">
            <Value>=Parameters!End_Released_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DATEADD(mm, DATEDIFF(m,0,DW.dbo.ODH.RELEASED_DATE),0) as Released_Month,
  DW.dbo.ODH.DIST_ID,
  DW.dbo.ODH.Order_number,
  count (distinct(DW.dbo.ODH.MORDER)) as Order_Count
FROM
  DW.dbo.ODH,
  DW.dbo.ODL
WHERE
  ( DW.dbo.ODH.MORDER=DW.dbo.ODL.MORDER  )
  AND  
  (
   DW.dbo.ODH.UNI_MKTING_UNIT  =  81
   And
   DW.dbo.ODH.Country_Code = 'JPN'
   AND
   DW.dbo.ODL.ITEM_CODE  =  '0100'
   AND
   DATEADD(mm, DATEDIFF(m,0,DW.dbo.ODH.RELEASED_DATE),0)  BETWEEN  @Start_Released_Month  AND @End_Released_Month
  )
GROUP BY
  DATEADD(mm, DATEDIFF(m,0,DW.dbo.ODH.RELEASED_DATE),0), 
  DW.dbo.ODH.DIST_ID, 
  DW.dbo.ODH.Order_number</CommandText>
      </Query>
      <Fields>
        <Field Name="Released_Month">
          <DataField>Released_Month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DIST_ID">
          <DataField>DIST_ID</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Order_number">
          <DataField>Order_number</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Order_Count">
          <DataField>Order_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
