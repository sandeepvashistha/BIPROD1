  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PvDateBeg">
            <Value>=Parameters!PvDateBeg.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PvDateEnd">
            <Value>=Parameters!PvDateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ds."Pv Date",ds."Item Code", ITD.ITEM_NAME_1 as Description, SUM(ds."Shipped Amount Local") as Retail, SUM(ds."Pv Amount") as Pv
FROM DW.dbo.DM_SHIPPED_SALES_PACKS ds 
LEFT OUTER JOIN DW.dbo.ITD ON ITD.ITEM_CODE = ds."Item Code"
	AND ITD.UNI_MKTING_UNIT = ds.[Uni Mkting Unit]

WHERE ds."Uni Mkting Unit" = 62 
AND ds."PV Date" &gt;= @PvDateBeg
AND ds."PV Date" &lt;= @PvDateEnd
AND ds."RMA" &lt;&gt; 'C'
AND ds.[Parent/Child] &lt;&gt; 'Child'
GROUP BY ds."Pv Date",ds."Item Code", ITD.ITEM_NAME_1
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Pv_Date">
          <DataField>Pv Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Code">
