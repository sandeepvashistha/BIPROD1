<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>46d62473-002d-446e-a9aa-d74443d7a77b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103) as pv_date,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.DST.COUNTRY_CODE as dst_country_code,
  DW.dbo.ODH.COUNTRY_CODE as odh_country_code,
  DW.dbo.DST.UNI_MKTING_UNIT,
  sum(DW.dbo.ODH.price_1 /  ISNULL(VW_NAV_CURRENCY_ALL_DSR_USD.Exchange_Rate_Amount,1)) as sales_usd, 
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) sales_local
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID
  LEFT OUTER JOIN VW_NAV_CURRENCY_ALL_DSR  VW_NAV_CURRENCY_ALL_DSR_USD ON VW_NAV_CURRENCY_ALL_DSR_USD.TO_CURRENCY=DW.dbo.ODH.CURRENCY_CODE 
and DW.dbo.ODH.ENTRY_DATE between VW_NAV_CURRENCY_ALL_DSR_USD.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR_USD.END_DATE
  AND  VW_NAV_CURRENCY_ALL_DSR_USD.from_currency='USD' 
WHERE
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)  &gt;=  '2017-01-01'  --this is a prompt
   AND
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'IDN'  )

GROUP BY
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103), 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.DST.UNI_MKTING_UNIT</CommandText>
      </Query>
      <Fields>
        <Field Name="pv_date">
          <DataField>pv_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ODH_TYPE">
          <DataField>ODH_TYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dst_country_code">
          <DataField>dst_country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="odh_country_code">
          <DataField>odh_country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UNI_MKTING_UNIT">
          <DataField>UNI_MKTING_UNIT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sales_usd">
          <DataField>sales_usd</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="sales_local">
          <DataField>sales_local</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
