  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemCode">
            <Value>=Parameters!ItemCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BeginPVDate">
            <Value>=Parameters!BeginPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	odh.dist_id,
	dst.roman_name_1,
	odh.order_number,
	odh.odh_type,
	odh.pv_date_date,
	odl.item_code,
	itd.item_name_1,
	sum(case when odh.odh_type='C' then -1*odl.qty_ordered else odl.qty_ordered end) ordered_qty,
	sum(case when odh.odh_type='C' then -1*odl.qty_ordered*odl.price_2 else odl.qty_ordered*odl.price_2 end) PV
from
	dw.dbo.odh
	inner join dw.dbo.odl on odh.morder=odl.morder
	inner join dw.dbo.dst on dst.dist_id=odh.dist_id
	left outer join dw.dbo.itd on itd.uni_mkting_unit=odl.mkting_unit and itd.item_code=odl.item_code
where
	odh.uni_mkting_unit=62 and odh.status in (2,3,4,5)
	and (odl.flag_2='M' or odl.flag_2 is null)
	and odl.item_code=@ItemCode --31076
	and CAST(odh.pv_date_date AS DATE) between CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, @BeginPVDate), 0) AS DATE) and CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, @EndPVDate), 0) AS DATE) --'2020-01-01' and '2020-03-01'
group by
	odh.dist_id,
	dst.roman_name_1,
	odh.order_number,
	odh.odh_type,
	odh.pv_date_date,
	odl.item_code,
	itd.item_name_1</CommandText>
      </Query>
