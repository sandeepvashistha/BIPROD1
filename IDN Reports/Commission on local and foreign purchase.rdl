  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BeginPVDate">
            <Value>=Parameters!BeginPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPVDate">
            <Value>=Parameters!EndPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select btr.dist_id as paid_to_dist, dst.roman_name_1 as paid_to_nm ,dst.country_code as paid_to_homecountry, btr.target_country,btr.pv_date_date,  
btr.bonus_type, btr.paid_level,btr.BTR_LEVEL,
btr.sold_dist_id, btr.sold_name,sd.country_code as sold_to_homecountry, btr.source_country,
substring((case when (dst.country_code = 'IDN' and btr.target_country = 'IDN' and btr.source_country = 'IDN' )
                                   then 'Local Volume'
                when (dst.country_code = 'IDN' and sd.country_code = 'IDN' and btr.target_country = 'IDN' and btr.source_country &lt;&gt; 'IDN' )
                                   then 'Foreign Volume' 
                when (dst.country_code = 'IDN' and sd.country_code &lt;&gt; 'IDN' and btr.target_country = 'IDN' and btr.source_country &lt;&gt; 'IDN' )
                                   then 'Foreign Downlines Volume' 
                        else 'N/A' end) ,0,50) [Paid From],
btr_percent,conversion_rate, sum(converted_bonus) as Com_LCY, sum(amount_1) as PV
from dw.dbo.btr 
join dw.dbo.dst on btr.dist_id = dst.dist_id
join dw.dbo.dst sd on btr.sold_dist_id = sd.dist_id
where btr.pv_date_date between @BeginPVDate and @EndPVDate
and ( (btr.SOURCE_COUNTRY = 'IDN' and btr.TARGET_COUNTRY = 'IDN') OR
       --(btr.SOURCE_COUNTRY = 'IDN' and btr.TARGET_COUNTRY &lt;&gt; 'IDN') OR
      (btr.SOURCE_COUNTRY &lt;&gt; 'IDN' and btr.TARGET_COUNTRY = 'IDN'))
group by btr.dist_id, dst.roman_name_1, dst.country_code,btr.target_country, btr.pv_date_date, btr.bonus_type, btr.paid_level,
btr.sold_dist_id, btr.sold_name, sd.country_code, btr.source_country,btr.BTR_LEVEL,
btr_percent,conversion_rate
having sum(bonus) &gt; 0 OR sum(converted_bonus) &gt; 0</CommandText>
      </Query>
