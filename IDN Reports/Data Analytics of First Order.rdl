  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportDate">
            <Value>=Parameters!ReportDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BeginEntryPVDate">
            <Value>=Parameters!BeginEntryPVDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndEntryPVDate">
            <Value>=Parameters!EndEntryPVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dst.dist_id ID,dst.roman_name_1 Name,dst.dist_status [Dist Status],dst.entry_date [Dist Entry Date]
,dst.entry_pv_date_date [Dist Entry PV Date],fi.ENTRY_DATE [First Order Entry Date],fi.order_number [First Order Number]
,fi.price_2 [First Order PV],fi.ODH_TYPE [First Order Type],cast(@ReportDate as date) [Report Date], datediff(day,dst.entry_date,isnull(fi.ENTRY_DATE,cast(@ReportDate as date))) [# Days]
from dw.dbo.dst 
left outer join dw.dbo.dm_first_order_data fod on fod.dist_id=dst.dist_id
left outer join 
(
SELECT ODH.MORDER,DST.MDIST,ODH.UNI_MKTING_UNIT,DST.DIST_ID,ODH.ORDER_NUMBER,ODH.ENTRY_DATE,
ODH.PRICE_2,ODH.ODH_TYPE
FROM
(select A.DIST_ID,A.R_DATE,MIN(ODH.ORDER_NUMBER) AS O_NUMBER
from DW.DBO.ODH ODH INNER JOIN DW.DBO.DST ON DST.DIST_ID=ODH.DIST_ID
INNER JOIN
(select ODH.DIST_ID,cast(MIN(ODH.ENTRY_DATE) as date) R_DATE
FROM DW.DBO.ODH ODH INNER JOIN DW.DBO.DST ON DST.DIST_ID=ODH.DIST_ID
LEFT OUTER JOIN (select ODH.UNI_MKTING_UNIT,(case when isnumeric(substring(DW.dbo.udf_GetNumeric(ODH.COMMENT_1),1,15))=1 then cast(substring(DW.dbo.udf_GetNumeric(ODH.COMMENT_1),1,15) as numeric) else 0 end) C_ORDER
     from DW.DBO.ODH ODH INNER JOIN DW.DBO.DST ON DST.DIST_ID=ODH.DIST_ID
     where ODH.ODH_TYPE='C' and ODH.STATUS IN (2,3,4,5) AND DST.UNI_MKTING_UNIT=62 AND ODH.PV_DATE_DATE&gt;=@BeginEntryPVDate
    ) ODH_CANCELLED ON (ODH.UNI_MKTING_UNIT=ODH_CANCELLED.UNI_MKTING_UNIT AND ODH.ORDER_NUMBER=ODH_CANCELLED.C_ORDER)
WHERE ODH.STATUS IN (2,3,4,5) AND ODH.ODH_TYPE &lt;&gt; 'C' AND DST.UNI_MKTING_UNIT=62 AND ODH.PV_DATE_DATE&gt;=@BeginEntryPVDate
AND ODH.PRICE_2&gt;0 AND ODH.DIST_ID IS NOT NULL AND ODH_CANCELLED.C_ORDER IS NULL
GROUP BY
 ODH.DIST_ID
) AS A ON A.DIST_ID=ODH.DIST_ID AND ODH.ENTRY_DATE=A.R_DATE
--LEFT OUTER JOIN (select ODH.UNI_MKTING_UNIT,substring(DW.dbo.udf_GetNumeric(ODH.COMMENT_1),1,15) C_ORDER
--     from DW.DBO.ODH ODH INNER JOIN DW.DBO.DST ON DST.DIST_ID=ODH.DIST_ID
--     where ODH.ODH_TYPE='C' and ODH.STATUS IN (2,3,4,5) AND DST.UNI_MKTING_UNIT=62 AND ODH.PV_DATE_DATE&gt;='2019-04-01'
--    ) ODH_CANCELLED ON (ODH.UNI_MKTING_UNIT=ODH_CANCELLED.UNI_MKTING_UNIT AND cast(ODH.ORDER_NUMBER as varchar)=ODH_CANCELLED.C_ORDER)
WHERE ODH.STATUS IN (2,3,4,5) AND ODH.ODH_TYPE &lt;&gt; 'C' AND DST.UNI_MKTING_UNIT=62 AND ODH.PV_DATE_DATE&gt;=@BeginEntryPVDate
AND ODH.PRICE_2&gt;0 AND ODH.DIST_ID IS NOT NULL --AND ODH_CANCELLED.C_ORDER IS NULL
GROUP BY
 A.DIST_ID,A.R_DATE
) Query
inner join DW.DBO.DST DST ON (DST.DIST_ID=Query.DIST_ID)
inner join DW.DBO.ODH ODH ON (ODH.ORDER_NUMBER=Query.O_NUMBER AND ODH.DIST_ID=Query.DIST_ID AND ODH.STATUS IN (2,3,4,5))
) fi on fi.dist_id=dst.dist_id
where dst.uni_mkting_unit=62 and dst.entry_pv_date_date between @BeginEntryPVDate and @EndEntryPVDate;</CommandText>
      </Query>
