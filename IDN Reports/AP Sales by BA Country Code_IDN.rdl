  <DataSets>
    <DataSet Name="BISQL01">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103) as pv_date,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.DST.COUNTRY_CODE as Dist_Country,
  DW.dbo.ODH.COUNTRY_CODE as Order_Country,
  DW.dbo.DST.UNI_MKTING_UNIT,
  sum(DW.dbo.ODH.price_1 /  ISNULL(VW_NAV_CURRENCY_ALL_DSR_USD.Exchange_Rate_Amount,1)) as sales_usd, 
  sum(CASE WHEN (DW.dbo.ODH.UNI_MKTING_UNIT=84 AND DW.dbo.ODH.PRICE_1=DW.dbo.ODH.TOT_INVCE_AMOUNT) THEN (DW.dbo.ODH.PRICE_1-DW.dbo.ODH.SALES_TAX) ELSE DW.dbo.ODH.PRICE_1 END) sales_local
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON DW.dbo.DST.DIST_ID=DW.dbo.ODH.DIST_ID
  LEFT OUTER JOIN VW_NAV_CURRENCY_ALL_DSR  VW_NAV_CURRENCY_ALL_DSR_USD ON ( VW_NAV_CURRENCY_ALL_DSR_USD.TO_CURRENCY = DW.dbo.ODH.CURRENCY_CODE 
and DW.dbo.ODH.ENTRY_DATE between VW_NAV_CURRENCY_ALL_DSR_USD.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR_USD.END_DATE
  AND  VW_NAV_CURRENCY_ALL_DSR_USD.from_currency='USD' )
WHERE
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)  &gt;=  @Start_PV_Month
   AND
   DW.dbo.DST.COUNTRY_CODE  IN  ( 'IDN'  )
GROUP BY
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103), 
  DW.dbo.ODH.ODH_TYPE, 
  DW.dbo.DST.COUNTRY_CODE, 
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.DST.UNI_MKTING_UNIT</CommandText>
      </Query>
