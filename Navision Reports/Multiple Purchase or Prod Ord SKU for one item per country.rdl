  <DataSets>
    <DataSet Name="Query_Multiple_Purchase">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 
uni_company_name, [Item No_]
--, case [Replenishment System] when 0 then 'Purchase' when 1 then 'Prod. Order' when 2 then 'Transfer' end as [Replenishment System]
, count(*) as non_trans_rp_count
from Stockkeeping_Unit_nc_vw
where [Replenishment System] in (0,1)
group by uni_company_name, [Item No_]
having count(*) &gt;=2
order by uni_company_name, [Item No_]
</CommandText>
      </Query>
      <Fields>
        <Field Name="uni_company_name">
          <DataField>uni_company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No_">
          <DataField>Item No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="non_trans_rp_count">
          <DataField>non_trans_rp_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_Multiple_Purchase_Different_lead_times">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select uni_company_name, [Item No_], count(*) as diff_lead_times
from 
(
       select 
       uni_company_name, [Item No_], [Lead Time Calculation]
       --, case [Replenishment System] when 0 then 'Purchase' when 1 then 'Prod. Order' when 2 then 'Transfer' end as [Replenishment System]
       , count(*) as ltcount
       from Stockkeeping_Unit_nc_vw
       where [Replenishment System] in (0,1)
       group by uni_company_name, [Item No_], [Lead Time Calculation]
) a
group by uni_company_name, [Item No_]
having count(*) &gt;=2
order by uni_company_name, [Item No_]
</CommandText>
      </Query>
      <Fields>
        <Field Name="uni_company_name">
          <DataField>uni_company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No_">
          <DataField>Item No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="diff_lead_times">
          <DataField>diff_lead_times</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_Multiple_Purchase_Exclude_Blank">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select uni_company_name, [Item No_], count(*) as diff_lead_times
from 
(
       select 
       uni_company_name, [Item No_], [Lead Time Calculation]
       --, case [Replenishment System] when 0 then 'Purchase' when 1 then 'Prod. Order' when 2 then 'Transfer' end as [Replenishment System]
       , count(*) as ltcount
       from Stockkeeping_Unit_nc_vw
       where [Replenishment System] in (0,1)
       and [Lead Time Calculation] &lt;&gt; ''
       group by uni_company_name, [Item No_], [Lead Time Calculation]
) a
group by uni_company_name, [Item No_]
having count(*) &gt;=2
order by uni_company_name, [Item No_]</CommandText>
      </Query>
      <Fields>
        <Field Name="uni_company_name">
          <DataField>uni_company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_No_">
          <DataField>Item No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="diff_lead_times">
          <DataField>diff_lead_times</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Count_ZeroRecords">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 1/count(b.diff_lead_times) cnt
from
(
select uni_company_name, [Item No_], count(*) as diff_lead_times
from 
(
       select 
       uni_company_name, [Item No_], [Lead Time Calculation]
       --, case [Replenishment System] when 0 then 'Purchase' when 1 then 'Prod. Order' when 2 then 'Transfer' end as [Replenishment System]
       , count(*) as ltcount
       from Stockkeeping_Unit_nc_vw
       where [Replenishment System] in (0,1)
       and [Lead Time Calculation] &lt;&gt; ''
       group by uni_company_name, [Item No_], [Lead Time Calculation]
) a
group by uni_company_name, [Item No_]
having count(*) &gt;=2
)b</CommandText>
      </Query>
      <Fields>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
