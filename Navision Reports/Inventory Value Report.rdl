    <DataSet Name="Query2">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select c.uni_company_name,c.[Item No_], c.[Country_Region Code], c.three_month_forecast, 
isnull(c.three_month_forecast/3,0) as average_monthly_forecast
,v.[description],(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end) [Standard Cost],
(c.three_month_forecast*(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end)) [Three Month COGS Forecast],
(c.three_month_forecast*(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end))/3 [Average Monthly COGS Forecast]

from
(
       select a.uni_company_name, a.[Item No_], b.[Country_Region Code], sum([Forecast Quantity]) as three_month_forecast
       from production_forecast_entry_nc_vw a
       join company_information_nc_vw b
       on a.uni_company_name = b.uni_company_name
       where 1=1
             and [Forecast Date] between DATEADD(month, DATEDIFF(month, 0, getdate()), 0) 
			 and DATEadd(MONTH,datediff(month,0,getdate())+3,0)-1 --This needs to be fixed so that it dynamically pulls the first day of the current month and the last day of 2 months from now (3 months total)
             and [Production Forecast Name] = 'MERCIA'
       group by a.uni_company_name, a.[Item No_], b.[Country_Region Code]
) c
left outer join item_nc_vw v on (c.uni_company_name=v.uni_company_name and c.[Item No_]=v.no_)</CommandText>
      </Query>
