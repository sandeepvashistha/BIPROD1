    <DataSet Name="Query4">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select sum(c.quantity*d.USD_standard) as transfer_order_quantity
from
(
       select a.uni_company_name, a.[Item No_], a.[Location Code], sum([Remaining Quantity]) as quantity
       from item_ledger_entry_nc_vw a
       join location_nc_vw b
       on a.uni_company_name = b.uni_company_name
       and a.[Location Code] = b.[Code]
       where a.[Open] = 1
       and b.[Use as In-Transit] = 1
       group by a.uni_company_name, a.[Item No_], a.[Location Code]
) c
join currency_all_items_nc_vw d
on c.uni_company_name = d.uni_company_name
and c.[Item No_] = d.[No_]

</CommandText>
      </Query>
      <Fields>
        <Field Name="transfer_order_quantity">
          <DataField>transfer_order_quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
