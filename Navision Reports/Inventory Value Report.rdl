  <DataSets>
    <DataSet Name="Query1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 
	te.uni_company_name --
    ,te.[Item No_]--
    ,te.[Description]--
    ,te.[Location Code]--
    ,te.[Code]--
    ,te.[Bin Code]--
    ,cast(te.on_hand as numeric(20,0)) as on_hand
    ,cast(te.unposted_qty as numeric(20,0)) as unposted_qty
    ,cast(te.[Standard Cost] as numeric(20,10)) as [Standard Cost]
    ,cast(te.exp_standard_cost as numeric(20,10)) as exp_standard_cost
    ,te.currency--
    ,cast(te.infotrax_sellable_qty as numeric(20,0)) as infotrax_sellable_qty
    ,cast(te.planning_immediately_sellable_inventory_qty as numeric(20,0)) as planning_immediately_sellable_inventory_qty
    ,cast(te.valued_inventory_qty as numeric(20,0)) as valued_inventory_qty
    ,te.valued_inventory_value
    ,te.[OE Transactable]
    ,te.[Non-Nettable]
    ,te.[DSC Warehouse Flag]
    ,te.[Use for Inventory Sync]
    ,te.[Order Entry Warehouse Code]
    ,te.planning_inventory_value
    ,te.planning_inventory_value_usd
    ,te.[Country_Region Code]
    ,te.exchange_rate_amount
	,te.sellable_worldwide
	,cast(te.planning_qty as numeric(20,0)) as planning_qty
    ,cast(te.non_planning_qty as numeric(20,0)) as non_planning_qty
    ,te.non_planning_value_usd
	,te.planning_value_usd
	,cast(isnull(forecast.three_month_forecast,0.00) as numeric(20,0)) as three_month_forecast
	,cast(isnull(forecast.average_monthly_forecast,0.00) as numeric(20,0)) as average_monthly_forecast
	,cast(isnull(forecast.[Forecast Standard Cost],0) as numeric(20,10)) as [Forecast Standard Cost]
	,cast(isnull(forecast.[Three Month COGS Forecast],0) as numeric(20,0)) as [Three Month COGS Forecast]
	,cast(isnull(forecast.[Average Monthly COGS Forecast],0) as numeric(20,0)) as [Average Monthly COGS Forecast]
from (
select 
    b.uni_company_name --
    , b.[Item No_]--
    , c.[Description]--
    , b.[Location Code]--
    , d.[Code]--
    , b.[Bin Code]--
    , b.on_hand--
    , isnull(g.unposted_qty,0) as unposted_qty--
    , c.[Standard Cost]--
    , case d.[DSC Warehouse Flag] when 0 then c.[Standard Cost] when 1 then 0 end as exp_standard_cost--
    , h.[LCY Code] as currency--
    , case when e.[Use for Inventory Sync] = 1 and c.[OE Transactable] =1 and d.[Order Entry Warehouse Code] &lt;&gt; '' then b.on_hand - isnull(g.unposted_qty,0) else 0 end as infotrax_sellable_qty
    , case when e.[Use for Inventory Sync] = 1 and c.[OE Transactable] =1 and d.[Non-Nettable] = 0 and d.[DSC Warehouse Flag] = 0 then b.on_hand - isnull(g.unposted_qty,0) else 0 end as planning_immediately_sellable_inventory_qty
    , case when d.[DSC Warehouse Flag] = 0 then b.on_hand else 0 end as valued_inventory_qty
    , case when d.[DSC Warehouse Flag] = 0 then b.on_hand * c.[Standard Cost] else 0 end as valued_inventory_value
    , case c.[OE Transactable] when 0 then 'No' when 1 then 'Yes' end as [OE Transactable]
    , case d.[Non-Nettable] when 0 then 'No' when 1 then 'Yes' end as [Non-Nettable]
    , case d.[DSC Warehouse Flag] when 0 then 'No' when 1 then 'Yes' end as [DSC Warehouse Flag]
    , case e.[Use for Inventory Sync] when 0 then 'No' when 1 then 'Yes' end as [Use for Inventory Sync]
    , d.[Order Entry Warehouse Code]
    , case when e.[Use for Inventory Sync] = 1 and c.[OE Transactable] =1 and d.[Non-Nettable] = 0 and d.[DSC Warehouse Flag] = 0 then ((b.on_hand - isnull(g.unposted_qty,0))* c.[Standard Cost]) else 0 end as planning_inventory_value
    , case when e.[Use for Inventory Sync] = 1 and c.[OE Transactable] =1 and d.[Non-Nettable] = 0 and d.[DSC Warehouse Flag] = 0 then ((b.on_hand - isnull(g.unposted_qty,0))* c.[Standard Cost])*(isnull(i.exchange_rate_amount,1)) else 0 end as planning_inventory_value_usd
    ,isnull(d.[Country_Region Code],' ') as [Country_Region Code]
    ,i.exchange_rate_amount
	, case isnull(m.No_,'') when '' then 'No' else 'Yes' end as sellable_worldwide
	, case when isnull(m.No_,'') &lt;&gt; '' and d.[Non-Nettable] = 0 and d.[DSC Warehouse Flag] = 0 then b.on_hand - isnull(g.unposted_qty,0) else 0 end as planning_qty
    , case when d.[DSC Warehouse Flag] = 1 then 0 when (isnull(m.No_,'') = '' or d.[Non-Nettable] = 1) then b.on_hand + isnull(g.unposted_qty,0) else isnull(g.unposted_qty,0) end as non_planning_qty --this excludes DSC inventory. planning_qty + non_planning_qty should total the total inventory less DSCs
    , ((case when d.[DSC Warehouse Flag] = 1 then 0 when (isnull(m.No_,'') = '' or d.[Non-Nettable] = 1) then b.on_hand + isnull(g.unposted_qty,0) else isnull(g.unposted_qty,0) end)* c.[Standard Cost]*(isnull(i.exchange_rate_amount,1))) as non_planning_value_usd
	, ((case when isnull(m.No_,'') &lt;&gt; '' and d.[Non-Nettable] = 0 and d.[DSC Warehouse Flag] = 0 then b.on_hand - isnull(g.unposted_qty,0) else 0 end)* c.[Standard Cost]*isnull(i.exchange_rate_amount,1)) as planning_value_usd
from
(
    select a.uni_company_name, a.[Item No_], a.[Location Code], a.[Bin Code], sum(a.[Qty_ (Base)]) as on_hand
     from warehouse_entry_nc_vw a
	 --where a.uni_company_name='Algeria'--a.[Item No_]='25881' and 
    group by a.uni_company_name, a.[Item No_], a.[Location Code], a.[Bin Code]
    having sum(a.[Qty_ (Base)]) &lt;&gt; 0
) as b
join item_nc_vw c
on 1=1
    and b.uni_company_name = c.uni_company_name
    and b.[Item No_] = c.[No_]
join location_nc_vw d
on 1=1
    and b.uni_company_name = d.uni_company_name
    and b.[Location Code] = d.[Code]
join bin_nc_vw e
on 1=1
    and b.uni_company_name = e.uni_company_name
    and b.[Location Code] = e.[Location Code]
    and b.[Bin Code] = e.[Code]
left outer join
(
       select l.No_
       from item_nc_vw l
       where l.[OE Transactable] = 1
       group by l.No_
) m
on 1=1
       and b.[Item No_] = m.[No_]
left outer join
(
    select f.uni_company_name, f.[Item No_], f.[Location Code], f.[Bin Code], sum(f.[Quantity (Base)]) unposted_qty
    from item_journal_line_nc_vw f
    where 1=1
        and f.[Journal Batch Name] = 'INV DEPL'
        and f.[Item No_] &lt;&gt; ''
    group by f.uni_company_name, f.[Item No_], f.[Location Code], f.[Bin Code] 
) g
on 1=1
    and b.uni_company_name = g.uni_company_name
    and b.[Item No_] = g.[Item No_]
    and b.[Location Code] = g.[Location Code]
    and b.[Bin Code] = g.[Bin Code]
join general_ledger_setup_nc_vw h
on 1=1
    and b.uni_company_name = h.uni_company_name
left outer join
(
select c.from_currency, c.to_currency, c.[Starting Date],c.exchange_rate_amount
from currency_all_nc_vw c
join
(
       select a.from_currency, a.to_currency, max(a.[Starting Date]) as latest_date
       from currency_all_nc_vw a
       where a.[Starting Date] &lt;= (getdate() + 1) --Filter out mistakes, which there aren't any currently. Plus today is tomorrow in most places at Unicity.
       group by a.from_currency, a.to_currency
) b
on 1=1
       and c.from_currency = b.from_currency
       and c.to_currency = b.to_currency
       and c.[Starting Date] = b.latest_date
                   and b.to_currency='USD'
) i on h.[LCY Code]=i.from_currency
) te 
left outer join
(
select c.uni_company_name,c.[Item No_], c.[Country_Region Code], c.three_month_forecast, 
isnull(c.three_month_forecast/3,0) as average_monthly_forecast
,v.[description],(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end) [Forecast Standard Cost],
(c.three_month_forecast*(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end)) [Three Month COGS Forecast],
(c.three_month_forecast*(CASE WHEN v.[Standard Cost] is null then 0 else v.[Standard Cost] end))/3 [Average Monthly COGS Forecast]

from
(
       select a.uni_company_name, a.[Item No_], b.[Country_Region Code], sum([Forecast Quantity]) as three_month_forecast
       from production_forecast_entry_nc_vw a
       join company_information_nc_vw b
       on a.uni_company_name = b.uni_company_name
       where 1=1
             and [Forecast Date] between DATEADD(month, DATEDIFF(month, 0, getdate()), 0) 
			 and DATEadd(MONTH,datediff(month,0,getdate())+3,0)-1 --This needs to be fixed so that it dynamically pulls the first day of the current month and the last day of 2 months from now (3 months total)
             and [Production Forecast Name] = 'MERCIA'
			 --and a.uni_company_name='Philippines'
			 --and a.[Item No_]='25881'
       group by a.uni_company_name, a.[Item No_], b.[Country_Region Code]
) c
left outer join item_nc_vw v on (c.uni_company_name=v.uni_company_name and c.[Item No_]=v.no_)
) forecast on (te.uni_company_name=forecast.uni_company_name and te.[Item No_]=forecast.[Item No_] and te.[Country_Region Code]=forecast.[Country_Region Code])
where 1=1</CommandText>
      </Query>
