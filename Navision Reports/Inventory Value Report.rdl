    <DataSet Name="Query3">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select 
	case	when DATEADD(wk, DATEDIFF(wk, 5, '1/1/' + left(a.yearweek,4)) + (right(a.yearweek,2)-1), 5)&gt; (getdate() + 1) 
			then (getdate() + 1) -- Last Day of Week crosses  getdate() + 1
			else DATEADD(wk, DATEDIFF(wk, 5, '1/1/' + left(a.yearweek,4)) + (right(a.yearweek,2)-1), 5) -- Last Day of Week i.e. Saturday
	end AS EndOfWeek,
	sum(a.usd)
from
	(
		select 
			   case 
					when cast((cast(YEAR(b.[Posting Date]) as varchar)+right(replicate('0',2) + cast(DATEPART(week,b.[Posting Date]) as varchar),2)) as numeric)
			   &lt;=cast((cast(YEAR(Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar)+right(replicate('0',2) + cast(DATEPART(week, Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar),2)) as numeric)	
					then cast((cast(YEAR(Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar)+right(replicate('0',2) + cast(DATEPART(week, Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar),2)) as numeric)
					else 
					cast((cast(YEAR(b.[Posting Date]) as varchar)+right(replicate('0',2) + cast(DATEPART(week,b.[Posting Date]) as varchar),2)) as numeric)
			   end as yearweek
			   ,sum(b.on_hand * c.[Standard Cost] * i.exchange_rate_amount) as usd
		from
			(
				   select a.uni_company_name, a.[Item No_], a.[Location Code],a.[Posting Date], sum(a.[Quantity]) as on_hand
				   from item_ledger_entry_nc_vw a
				   where [Posting Date] &lt;= (getdate() + 1)
				   group by a.uni_company_name, a.[Item No_], a.[Location Code],a.[Posting Date]
			) as b
			join item_nc_vw c
			on 1=1
				and b.uni_company_name = c.uni_company_name
				and b.[Item No_] = c.[No_]
			join location_nc_vw d
			on 1=1
				and b.uni_company_name = d.uni_company_name
				and b.[Location Code] = d.[Code]
			join general_ledger_setup_nc_vw h
			on 1=1
				and b.uni_company_name = h.uni_company_name
			left outer join
			(
				   select c.from_currency, c.to_currency, c.[Starting Date],c.exchange_rate_amount
				   from currency_all_nc_vw c
				   join
				   (
						 select a.from_currency, a.to_currency, max(a.[Starting Date]) as latest_date
						 from currency_all_nc_vw a
						 where a.[Starting Date] &lt;= (getdate() + 1) --Filter out mistakes, which there aren't any currently. Plus today is tomorrow in most places at Unicity.
						 group by a.from_currency, a.to_currency
				   ) b
				   on 1=1
						 and c.from_currency = b.from_currency
						 and c.to_currency = b.to_currency
						 and c.[Starting Date] = b.latest_date
						 and b.to_currency='USD'
			) i on h.[LCY Code]=i.from_currency
		where 1=1
		and d.[DSC Warehouse Flag] = 0
		group by
		case when cast((cast(YEAR(b.[Posting Date]) as varchar)+right(replicate('0',2) + cast(DATEPART(week,b.[Posting Date]) as varchar),2)) as numeric)
			   &lt;=cast((cast(YEAR(Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar)+right(replicate('0',2) + cast(DATEPART(week, Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar),2)) as numeric) then
			   cast((cast(YEAR(Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar)+right(replicate('0',2) + cast(DATEPART(week, Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, current_timestamp)), 0)) as varchar),2)) as numeric)
			   else cast((cast(YEAR(b.[Posting Date]) as varchar)+right(replicate('0',2) + cast(DATEPART(week,b.[Posting Date]) as varchar),2)) as numeric)
			   end
   ) as a
   group by
   case	when DATEADD(wk, DATEDIFF(wk, 5, '1/1/' + left(a.yearweek,4)) + (right(a.yearweek,2)-1), 5)&gt; (getdate() + 1) 
			then (getdate() + 1) -- Last Day of Week crosses  getdate() + 1
			else DATEADD(wk, DATEDIFF(wk, 5, '1/1/' + left(a.yearweek,4)) + (right(a.yearweek,2)-1), 5) -- Last Day of Week i.e. Saturday
	end



</CommandText>
      </Query>
      <Fields>
        <Field Name="EndOfWeek">
          <DataField>EndOfWeek</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
