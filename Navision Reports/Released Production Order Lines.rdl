  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>ProdNavision</DataSourceName>
        <CommandText>select
       a.uni_company_name
       ,[Prod_ Order No_]
       ,[Line No_]
       ,[Item No_]
       ,B.[Description]
       ,B.[Description 2]
       ,A.[Location Code]
       ,A.[Bin Code]
       ,cast(A.[Quantity] as numeric(15)) [Quantity]
       ,cast(A.[Finished Quantity] as numeric(15)) [Finished Quantity]
       ,cast(A.[Remaining Quantity] as numeric(15)) [Remaining Quantity]
       ,A.[Due Date]
       ,A.[Starting Date]
       ,A.[Ending Date]
       ,[Production BOM No_]
       ,A.[Routing No_]
       ,A.[Inventory Posting Group]
       ,[Routing Reference No_]
	   ,cast(A.[Unit Cost] as numeric(20,5)) [Unit Cost]
	   ,cast(A.[Cost Amount] as numeric(20,5)) [Cost Amount]
       ,[Unit of Measure Code]
       ,[Created By]
       ,[Assigned User ID]
from prod_navision.dbo.prod_order_line_nc_vw A
inner join prod_navision.dbo.production_order_nc_vw B on 1=1 
       and a.uni_company_name = b.uni_company_name
       and A.[Prod_ Order No_]= B.[No_]
where 1=1
       and b.[Status] = 3 --Released status
       and a.[Item No_] &lt;&gt; '' --Ignores blank/notes lines</CommandText>
      </Query>
