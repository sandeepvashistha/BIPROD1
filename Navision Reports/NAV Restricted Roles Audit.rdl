  <DataSources>
    <DataSource Name="UniNav">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=sqlprodb\sqlb;Initial Catalog=Prod_Asia</ConnectString>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>45d290cc-e713-4c27-9aa5-4b91f1244bae</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>UniNav</DataSourceName>
        <CommandText>select 
	a.[Role ID],
	(case when (a.[Company Name] is null or a.[Company Name]='') then 'All Markets' else a.[Company Name] end) [Company Name],
	b.name
from 
	prod_asia.dbo.[Windows Access Control] a
	inner join sys.server_principals b on SID_BINARY(a.[Login SID]) = b.[sid]
where a.[Role ID] in ('UNI-COMMISSIONS','SUPER','UNI-GL PERIODS','UNI-GL SETUP','UNI-HR','UNI-PAYABLES PAYMENT','UNI-PAYROLL','UNI-STANDARDCOST','UNI-TRANSFERPRICE','UNI-VENDOR','UNI-ITEM MAINT','UNI-LOCATION','UNI-OPS SETUP','UNI-CURRENCY')</CommandText>
      </Query>
      <Fields>
        <Field Name="Role_ID">
          <DataField>Role ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Company_Name">
          <DataField>Company Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
