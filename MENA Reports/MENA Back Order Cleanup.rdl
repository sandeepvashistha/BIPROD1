  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CountryCode">
            <Value>=Parameters!CountryCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Back order report for all countries in the 97 spoke and Europe/Africa
select odh.country_code, bck.ship_to_country, bck.uni_mkting_unit, bck.order_number, bck.line_number
, bck.warehouse_number, bck.record_date, bck.item_code, itd.item_name_1, bck.qty_ordered, bck.qty_shipped 
,bck.qty_ordered - bck.qty_shipped as qty_back_order
from dw.dbo.bck
join dw.dbo.odh on bck.morder = odh.morder
left outer join dw.dbo.itd on itd.mitem = bck.mitem collate database_default
where 1=1 
       and (odh.uni_mkting_unit = 97
             or bck.ship_to_country in ('MAR','DZA')
             ) and odh.country_code = @CountryCode</CommandText>
      </Query>
