    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_Post_Month">
            <Value>=Parameters!Start_Post_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_Post_Month">
            <Value>=Parameters!End_Post_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dist_Country">
            <Value>=Parameters!Dist_Country.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DST.DIST_ID,
  DST.ROMAN_NAME_1,
  DST.Uni_Mkting_unit As Dist_Market,
  DST.COUNTRY_CODE As Dist_Country,
  ODH.ORDER_NUMBER,
  ODH.Uni_Mkting_unit As Order_Market,
  ODH.Country_Code As Order_Country,
  ODH.ODH_Type,
  OTY.[Description] as Order_Type_Desc,
  ODH.PV_DATE_DATE as Post_Month,
  ODL.ITEM_CODE,
  ITD.ITEM_NAME_1,
  sum(CASE WHEN ODH.ODH_Type='C' THEN -1*ODL.QTY_ORDERED ELSE ODL.QTY_ORDERED END) as Qty_Orderd,
  Sum(CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_1*ODL.QTY_ORDERED) ELSE (ODL.PRICE_1*ODL.QTY_ORDERED) END) as Amount,
  sum((CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_1*ODL.QTY_ORDERED) ELSE (ODL.PRICE_1*ODL.QTY_ORDERED) END) /  ISNULL(VW_NAV_CURRENCY_ALL_DSR.Exchange_Rate_Amount,1)) As Amount_USD,
Sum(CASE WHEN DST.Uni_Mkting_unit = ODH.Uni_Mkting_unit 
		 THEN (CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_1*ODL.QTY_ORDERED) ELSE (ODL.PRICE_1*ODL.QTY_ORDERED) END) ELSE 0 END) as Local_Amount,
Sum(CASE WHEN DST.Uni_Mkting_unit &lt;&gt; ODH.Uni_Mkting_unit 
		 THEN (CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_1*ODL.QTY_ORDERED) ELSE (ODL.PRICE_1*ODL.QTY_ORDERED) END) ELSE 0 END) as Global_Amount,
Sum(CASE WHEN DST.Uni_Mkting_unit = ODH.Uni_Mkting_unit 
		 THEN (CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_2*ODL.QTY_ORDERED) ELSE (ODL.PRICE_2*ODL.QTY_ORDERED) END) ELSE 0 END) as Local_PV,
Sum(CASE WHEN DST.Uni_Mkting_unit &lt;&gt; ODH.Uni_Mkting_unit 
		 THEN (CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_2*ODL.QTY_ORDERED) ELSE (ODL.PRICE_2*ODL.QTY_ORDERED) END) ELSE 0 END) as Global_PV,
Sum(CASE WHEN ODH.ODH_Type='C' THEN -1*(ODL.PRICE_2*ODL.QTY_ORDERED) ELSE (ODL.PRICE_2*ODL.QTY_ORDERED) END) as Total_PV
FROM
  DW.dbo.ODH
  INNER JOIN DW.dbo.DST ON ( DST.DIST_ID = ODH.DIST_ID  )
  INNER JOIN DW.DBO.ODL ON (ODL.MKTING_UNIT=ODH.UNI_MKTING_UNIT AND ODL.ORDER_NUMBER=ODH.ORDER_NUMBER)
  LEFT OUTER JOIN DW.DBO.ITD ON (ODL.MKTING_UNIT=ITD.UNI_MKTING_UNIT AND ODL.ITEM_CODE=ITD.ITEM_CODE)
  LEFT OUTER JOIN DW.DBO.OTY ON (ODH.UNI_MKTING_UNIT=OTY.MKTING_UNIT AND ODH.ODH_TYPE=OTY.ORDER_TYPE)
  LEFT OUTER JOIN DW.dbo.VW_NAV_CURRENCY_ALL_DSR ON ( VW_NAV_CURRENCY_ALL_DSR.to_CURRENCY = ODH.CURRENCY_CODE 
    AND ( ODH.ENTRY_DATE BETWEEN VW_NAV_CURRENCY_ALL_DSR.STARTING_DATE and VW_NAV_CURRENCY_ALL_DSR.END_DATE )
    And VW_NAV_CURRENCY_ALL_DSR.From_currency = 'USD' )
WHERE
  (
   ODH.PV_DATE_DATE Between @Start_Post_Month and @End_Post_Month
   AND ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND ODH.UNI_MKTING_UNIT  IN  ( 97  )
   AND (ODL.FLAG_2 IS NULL OR ODL.FLAG_2='M')
   and ODH.COUNTRY_CODE in (@Dist_Country)
  )
GROUP BY
  DST.DIST_ID,
  DST.ROMAN_NAME_1,
  DST.Uni_Mkting_unit, 
  DST.COUNTRY_CODE,
  ODH.ORDER_NUMBER,
  ODH.Uni_Mkting_unit,
  ODH.COUNTRY_CODE,
  ODH.ODH_Type,
  OTY.[Description], 
  ODH.PV_DATE_DATE,
  ODL.ITEM_CODE,
  ITD.ITEM_NAME_1</CommandText>
      </Query>
