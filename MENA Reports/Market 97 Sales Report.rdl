  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start_PV_Month">
            <Value>=Parameters!Start_PV_Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End_PV_Month">
            <Value>=Parameters!End_PV_Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.ODH.UNI_MKTING_UNIT,
  DW.dbo.ODH.POST_DATE,
  DW.dbo.DST.COUNTRY_CODE as Dist_Country,
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103) as PV_Month,
  DW.dbo.ODH.COUNTRY_CODE as ORDER_COUNTRY,
  DW.dbo.OTY.DESCRIPTION,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODL.ITEM_NAME_1,
  --DW.dbo.ODL.Flag_2,
  --DW.dbo.ODL.Price_1,
  --DW.dbo.ODL.Price_2,
  Sum(DW.dbo.ODL.QTY_ORDERED) as Qty_Ordered,
  --Sum(DW.dbo.ODL.PRICE_2*DW.dbo.ODL.QTY_ORDERED) as PV_Amount,
  Sum(DW.dbo.ODL.PRICE_1*DW.dbo.ODL.QTY_ORDERED) as Amount
FROM
  DW.dbo.ODH,
  DW.dbo.DST,
  DW.dbo.OTY,
  DW.dbo.ODL
WHERE
  DW.dbo.ODH.MORDER = DW.dbo.ODL.MORDER
  AND
  ( DW.dbo.OTY.MKTING_UNIT = DW.dbo.ODH.UNI_MKTING_UNIT and DW.dbo.OTY.ORDER_TYPE = DW.dbo.ODH.ODH_TYPE )
  AND  ( DW.dbo.DST.DIST_ID = DW.dbo.ODH.DIST_ID )
  AND  
  (
   CONVERT(DATETIME,'1/'+ODH.PV_DATE,103)  Between @Start_PV_Month and @End_PV_Month
   AND
   DW.dbo.ODH.STATUS  IN  ( 2, 3, 4, 5  )
   AND
   DW.dbo.ODH.UNI_MKTING_UNIT  IN  ( 97  )
   AND
   (
    DW.dbo.ODL.Flag_2 = 'M'
	OR 
	DW.dbo.ODL.Flag_2 is null
   )
  )
GROUP BY
  DW.dbo.ODH.UNI_MKTING_UNIT,
  DW.dbo.ODH.POST_DATE, 
  DW.dbo.DST.COUNTRY_CODE, 
  CONVERT(DATETIME,'1/'+ODH.PV_DATE,103),
  DW.dbo.ODH.COUNTRY_CODE, 
  DW.dbo.OTY.DESCRIPTION,
  DW.dbo.ODH.ODH_TYPE,
  DW.dbo.ODL.ITEM_CODE,
  DW.dbo.ODL.ITEM_NAME_1
  --DW.dbo.ODL.Flag_2,
  --DW.dbo.ODL.Price_1,
  --DW.dbo.ODL.Price_2</CommandText>
      </Query>
