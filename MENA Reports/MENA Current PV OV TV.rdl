  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>dd1c5eed-56f8-40b4-a102-eb13d62f9b82</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PVDate">
            <Value>=Parameters!PVDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
       DST.ROMAN_NAME_1 NM1,
       TBI.DIST_ID,
       DST.COUNTRY_CODE,
       DST.MKTING_UNIT MARKETING_UNIT,
       SUM(TBI.VOL1) as PV,
       SUM(TBI.VOL3) as OV,
       SUM(TBI.VOL2) as TV,
       TBI.PV_DATE_DATE as PV_DATE,
       Paid_As_Rank_Desc.TITLE as PAID_RANK_DESC,
       TBI.PD_RNK PAID_RANK,
       MAX(CASE WHEN TBI.MKTING_UNIT=97 THEN 97 ELSE 0 END) TBI_MKTING_UNIT
FROM
  DW.dbo.TBI
  INNER JOIN DW.dbo.DST on DST.DIST_ID=TBI.DIST_ID
  LEFT OUTER JOIN DW.dbo.RANK  Paid_As_Rank_Desc ON ( TBI.PD_RNK=Paid_As_Rank_Desc.RANK and TBI.MKTING_UNIT=Paid_As_Rank_Desc.MARKET  )
WHERE
       CAST(TBI.PV_DATE_DATE AS DATE)  =  CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate), 0) AS DATE)
       AND DST.MKTING_UNIT=97 
   --and tbi.dist_id=22872297
GROUP BY
       DST.ROMAN_NAME_1 ,
       TBI.DIST_ID,
       DST.COUNTRY_CODE,
       DST.MKTING_UNIT,
       TBI.PV_DATE_DATE,
       Paid_As_Rank_Desc.TITLE,
       TBI.PD_RNK
HAVING 
       MAX(CASE WHEN TBI.MKTING_UNIT=97 THEN 97 ELSE 0 END)=97
UNION
SELECT 
       DST.ROMAN_NAME_1 NM1,
       TBI.DIST_ID,
       DST.COUNTRY_CODE,
       DST.MKTING_UNIT MARKETING_UNIT,
       SUM(TBI.VOL1) as PV,
       SUM(TBI.VOL3) as OV,
       SUM(TBI.VOL2) as TV,
       TBI.PV_DATE_DATE as PV_DATE,
       Paid_As_Rank_Desc.TITLE as PAID_RANK_DESC,
       TBI.PD_RNK PAID_RANK,
       MAX(CASE WHEN (TBI.MKTING_UNIT=49 AND TBI.COUNTRY_CODE IN ('BEL','NLD')) THEN 49 ELSE 0 END) TBI_MKTING_UNIT
FROM
  DW.dbo.TBI
  INNER JOIN DW.dbo.DST on DST.DIST_ID=TBI.DIST_ID
  LEFT OUTER JOIN DW.dbo.RANK  Paid_As_Rank_Desc ON ( TBI.PD_RNK=Paid_As_Rank_Desc.RANK and TBI.MKTING_UNIT=Paid_As_Rank_Desc.MARKET  )
WHERE
       CAST(TBI.PV_DATE_DATE AS DATE)  =  CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, @PVDate), 0) AS DATE)
       AND DST.MKTING_UNIT=49 AND TBI.COUNTRY_CODE IN ('BEL','NLD') 
   --and tbi.dist_id=22872297
GROUP BY
       DST.ROMAN_NAME_1 ,
       TBI.DIST_ID,
       DST.COUNTRY_CODE,
       DST.MKTING_UNIT,
       TBI.PV_DATE_DATE,
       Paid_As_Rank_Desc.TITLE,
       TBI.PD_RNK
HAVING 
       MAX(CASE WHEN (TBI.MKTING_UNIT=49 AND TBI.COUNTRY_CODE IN ('BEL','NLD')) THEN 49 ELSE 0 END)=49</CommandText>
      </Query>
