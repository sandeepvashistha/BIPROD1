  <DataSets>
    <DataSet Name="Allow_BackOrders">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>/*Select 
  Distinct INV.Item_Code, 
  ITD.Item_Name_1, 
  INV.Warehouse_Number, 
  (Case When INV.Flag_1 = 'T' Then 'DisAllow BackOrder' Else 'Allow BackOrder' End ) allow_Backorder
from 
  INV
  Inner Join ITD On ( INV.MITEM = ITD.MITEM )
where 
  INV.Uni_Mkting_unit = '63'*/</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="BackOrders">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>/*Select 
   Distinct INV.Item_Code, 
   ITD.Item_Name_1, 
   INV.Warehouse_Number, 
   (Case When INV.Flag_1 = 'T' Then 'DisAllow BackOrder' Else 'Allow BackOrder' End ) allow_Backorder,
   (Case When Item_NC_VW.[Item Status] = 0 THEN 'Active'	
             WHEN Item_NC_VW.[Item Status] = 1 THEN 'Discontinue'
             WHEN Item_NC_VW.[Item Status] = 2 THEN 'Inactive'
             WHEN Item_NC_VW.[Item Status] = 3 THEN 'Obsolete'
             WHEN Item_NC_VW.[Item Status] = 4 THEN 'Phantom'
             WHEN Item_NC_VW.[Item Status] = 5 THEN 'PreRelease'
             WHEN Item_NC_VW.[Item Status] = 6 THEN 'Reference' /*Else 'Non-Active'*/ End ) Active_Item
From 
   INV
   Inner Join ITD On ( INV.MITEM = ITD.MITEM )
   Left Outer Join Item_NC_VW On ( INV.Item_Code = Item_NC_VW.No_ COLLATE SQL_Latin1_General_CP1_CI_AS and Item_Nc_Vw.Uni_Company_Name in (/*'Australia','New Zealand',*/'Philippines'))
Where 
   INV.Uni_Mkting_unit = '63'
   And
   INV.Warehouse_Number &lt;&gt; '0'*/</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>Select Distinct
   INV.Item_Code, 
   ITD.Item_Name_1,
   INV.Uni_Mkting_unit, 
   INV.Warehouse_Number, 
   (Case When INV.Flag_1 = 'T' Then 'DisAllow BackOrder' Else 'Allow BackOrder' End ) allow_Backorder,
   (Case When Item_NC_VW.[Item Status] = 0 Then 'Active'	
         When Item_NC_VW.[Item Status] = 1 Then 'Discontinue'
         When Item_NC_VW.[Item Status] = 2 Then 'Inactive'
         When Item_NC_VW.[Item Status] = 3 Then 'Obsolete'
         When Item_NC_VW.[Item Status] = 4 Then 'Phantom'
         When Item_NC_VW.[Item Status] = 5 Then 'PreRelease'
         When Item_NC_VW.[Item Status] = 6 Then 'Reference' /*Else 'Non-Active'*/ End ) Active_Item,
   Cast(WHS.WH_Number As Varchar)+' - '+WHS.WH_Name Warehouse,
   Location_NC_VW.Code As Location,
   Cast(WHS.WH_Number As Varchar)+' - '+Location_NC_VW.Code Loc,
   --Count(ODH.Order_Number) as Order_Count,
   (Case When Count(ODH.Order_Number) &gt; 0 Then 'Active Warehouse' Else 'Inactive Warehouse' End ) Active_Warehouse
From 
   INV
   Inner Join ITD On ( INV.MITEM = ITD.MITEM )
   Left Outer Join Item_NC_VW On ( INV.Item_Code = Item_NC_VW.No_ COLLATE SQL_Latin1_General_CP1_CI_AS and Item_Nc_Vw.Uni_Company_Name in (/*'Australia','New Zealand',*/'Philippines'))
   Left Outer Join Location_NC_VW On ( Cast(Location_NC_VW.[Order Entry Warehouse Code] As Int) = INV.Warehouse_Number and Location_NC_VW.Uni_Company_Name in ('Australia','New Zealand','Philippines'))
   Inner Join WHS On ( WHS.Mkting_Unit = INV.Uni_Mkting_unit and WHS.WH_Number = INV.Warehouse_Number )
   Left Outer Join ODH on ( INV.Warehouse_Number = ODH.WareHouse And INV.Uni_Mkting_Unit = ODH.Uni_Mkting_Unit And ODH.status in (2,3,4,5) And ODH.odh_type not in ('C','R') And ODH.Post_date &gt; getdate()-30)
Where 
   INV.Uni_Mkting_unit = '63'
   And
   INV.Warehouse_Number &lt;&gt; '0'
Group by
   INV.Item_Code, 
   ITD.Item_Name_1,
   INV.Uni_Mkting_unit, 
   INV.Warehouse_Number, 
   (Case When INV.Flag_1 = 'T' Then 'DisAllow BackOrder' Else 'Allow BackOrder' End ),
   (Case When Item_NC_VW.[Item Status] = 0 Then 'Active'	
         When Item_NC_VW.[Item Status] = 1 Then 'Discontinue'
         When Item_NC_VW.[Item Status] = 2 Then 'Inactive'
         When Item_NC_VW.[Item Status] = 3 Then 'Obsolete'
         When Item_NC_VW.[Item Status] = 4 Then 'Phantom'
         When Item_NC_VW.[Item Status] = 5 Then 'PreRelease'
         When Item_NC_VW.[Item Status] = 6 Then 'Reference' /*Else 'Non-Active'*/ End ),
   Cast(WHS.WH_Number As Varchar)+' - '+WHS.WH_Name,
   Location_NC_VW.Code,
   Cast(WHS.WH_Number As Varchar)+' - '+Location_NC_VW.Code</CommandText>
      </Query>
