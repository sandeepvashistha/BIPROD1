  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartPvDate">
            <Value>=Parameters!StartPvDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndPvDate">
            <Value>=Parameters!EndPvDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DW.dbo.BWT_CURRENT.DIST_ID,
  DW.dbo.BWT_CURRENT.NM1,
  SAPDW_BTR_CURRENT.BONUS_TYPE,
  CONVERT(DATETIME,LTRIM(RTRIM(SAPDW_BTR_CURRENT.PV_DATE))+'01',103) As PV_Date,
  SAPDW_BTR_CURRENT.BONUS
FROM
  DW.dbo.BWT_CURRENT
  LEFT OUTER JOIN DW.dbo.BTR  SAPDW_BTR_CURRENT ON ( CONVERT(DATETIME,LTRIM(RTRIM(DW.dbo.BWT_Current.PV_DATE))+'01',103) = CONVERT(DATETIME,LTRIM(RTRIM(SAPDW_BTR_CURRENT.PV_DATE))+'01',103) 
  and DW.dbo.BWT_CURRENT.MDIST=SAPDW_BTR_CURRENT.MDIST )
WHERE
  ( ( ( DW.dbo.BWT_CURRENT.MARKETING_UNIT = 1  )  )
  OR  ( ( 1 = 1  )  )
  )  AND  
  (
   DW.dbo.BWT_CURRENT.COUNTRY_CODE  IN  ( 'PHL'  )
   AND
   SAPDW_BTR_CURRENT.SOLD_DIST_ID  =  SAPDW_BTR_CURRENT.DIST_ID
   AND
   SAPDW_BTR_CURRENT.BONUS_TYPE  IN  ( 22, 24  )
   AND
   SAPDW_BTR_CURRENT.BONUS  &gt;  0
   AND 
   CONVERT(DATETIME,LTRIM(RTRIM(DW.dbo.BWT_Current.PV_DATE))+'01',103) Between @StartPvDate And @EndPvDate
  )</CommandText>
      </Query>
