  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SQLPRODA">
      <DataSourceReference>SQLPRODA</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>11b5aeff-8710-40ca-85a2-a5703c18ae67</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>SELECT 
  A.MARKET_NUMBER,
  A.COUNTRY_CODE,
  A.BA_NAME,
  A.BA_NUMBER, 
  A.DATE_ENTRY,
  A.DISTRIBUTOR_STATUS_CODE,
  A.LAST_ACTIVITY,
  MAX(ODH.ORDER_NUMBER) AS ORDER_NUMBER,
  MAX(ODH.PRICE_2) AS PV
FROM
(
SELECT 
  T.UNI_MKTING_UNIT AS MARKET_NUMBER,
  T.COUNTRY_CODE,
  T.ROMAN_NAME_1 AS BA_NAME,
  T.DIST_ID AS BA_NUMBER, 
  T.ENTRY_DATE AS DATE_ENTRY,
  T.DIST_STATUS AS DISTRIBUTOR_STATUS_CODE,
  MAX(T.order_entry_date) AS LAST_ACTIVITY
  FROM (
SELECT
  DST.UNI_MKTING_UNIT,
  DST.COUNTRY_CODE,
  DST.DIST_ID,
  DST.ROMAN_NAME_1,
  DST.ENTRY_DATE,
  DST.DIST_STATUS,
  ISNULL(ODH.ENTRY_DATE,'1999-01-01') as order_entry_date
FROM
  DW.DBO.DST 
  LEFT OUTER JOIN DW.DBO.ODH ON
  (DST.dist_id=ODH.dist_id AND ODH.STATUS &lt;&gt; 9 AND ODH.ODH_TYPE &lt;&gt; 'C')
WHERE
   DST.UNI_MKTING_UNIT  =  63 AND DST.COUNTRY_CODE='PHL'
   AND   DST.DIST_STATUS  IN  ( 'A','B')
   AND   DST.ENTRY_DATE  &lt;  Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, GETDATE())), 0)
) T
GROUP BY 
  T.UNI_MKTING_UNIT,
  T.COUNTRY_CODE,
  T.DIST_ID,
  T.ROMAN_NAME_1,
  T.ENTRY_DATE,
  T.DIST_STATUS
HAVING MAX(T.order_entry_date) &lt; Dateadd(Month, Datediff(Month, 0, DATEADD(m, -12, GETDATE())), 0)
) A
LEFT OUTER JOIN DW.DBO.ODH ON (ODH.DIST_ID=A.BA_NUMBER AND ODH.ENTRY_DATE=A.LAST_ACTIVITY)
GROUP BY
  A.MARKET_NUMBER,
  A.COUNTRY_CODE,
  A.BA_NAME,
  A.BA_NUMBER, 
  A.DATE_ENTRY,
  A.DISTRIBUTOR_STATUS_CODE,
  A.LAST_ACTIVITY
ORDER BY
  A.LAST_ACTIVITY</CommandText>
      </Query>
