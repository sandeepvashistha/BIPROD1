  <DataSets>
    <DataSet Name="Record_Count_Query">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select 1/count(1) cnt
from
dw.dbo.odl
join dw.dbo.odh
on odl.morder = odh.morder
where [status] in (1,2,3,4)
and odh.odh_type not in ('C','R')
and odh.record_date &lt; getdate()-3
group by odl.mkting_unit, odl.order_number, record_date, odh.[status]
having sum(qty_ordered) = sum(qty_shipped)
</CommandText>
      </Query>
      <Fields>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_1">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select odl.mkting_unit, odl.order_number, record_date, odh.[status], sum(QTY_ORDERED) as qty_ordered, sum(qty_shipped) as qty_shipped
from dw.dbo.odl
join dw.dbo.odh
on odl.morder = odh.morder
where [status] in (1,2,3,4)
and odh.odh_type not in ('C','R')
and odh.record_date &lt; getdate()-3
group by odl.mkting_unit, odl.order_number, record_date, odh.[status]
having sum(qty_ordered) = sum(qty_shipped) 
order by odl.mkting_unit, odl.order_number, record_date</CommandText>
      </Query>
      <Fields>
        <Field Name="mkting_unit">
          <DataField>mkting_unit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="order_number">
          <DataField>order_number</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="record_date">
