  <DataSets>
    <DataSet Name="Query_Summary">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select a.uni_mkting_unit,a.country_code,a.entry_month,count(order_number) as order_count
from
(
select distinct odh.uni_mkting_unit,odh.country_code,odh.order_number,
DATEADD(MONTH, DATEDIFF(MONTH, 0, odh.entry_date), 0) entry_month
from dw.dbo.odh inner join dw.dbo.odl on odh.morder=odl.morder
where odh.status in (2,3,4,5) and odh_type&lt;&gt;'C' and odl.qty_bo_filled&gt;0
) a
group by 
a.uni_mkting_unit,a.country_code,a.entry_month
order by a.uni_mkting_unit,a.country_code,a.entry_month</CommandText>
      </Query>
      <Fields>
        <Field Name="uni_mkting_unit">
          <DataField>uni_mkting_unit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="country_code">
          <DataField>country_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="entry_month">
          <DataField>entry_month</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="order_count">
          <DataField>order_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Query_Summary_qty">
      <Query>
        <DataSourceName>SQLPRODA</DataSourceName>
        <CommandText>select odh.uni_mkting_unit,odh.country_code,DATEADD(MONTH, DATEDIFF(MONTH, 0, odh.entry_date), 0) entry_month,
odl.item_code,itd.item_name_1,sum(odl.qty_bo_filled) backorder_qty,count(odh.order_number) order_count,
(case when odl.flag_2 is null then 'Individual' when odl.flag_2='C' then 'Child' when odl.flag_2='M' then 'Parent' end) child_parent
from dw.dbo.odh inner join dw.dbo.odl on odh.morder=odl.morder
left outer join dw.dbo.itd on odl.mkting_unit=itd.uni_mkting_unit and odl.item_code=itd.item_code
where odh.status in (2,3,4,5) and odh_type&lt;&gt;'C' and odl.qty_bo_filled&gt;0
group by
odh.uni_mkting_unit,odh.country_code,DATEADD(MONTH, DATEDIFF(MONTH, 0, odh.entry_date), 0),odl.item_code,itd.item_name_1,
(case when odl.flag_2 is null then 'Individual' when odl.flag_2='C' then 'Child' when odl.flag_2='M' then 'Parent' end)
order by
odh.uni_mkting_unit,odh.country_code,DATEADD(MONTH, DATEDIFF(MONTH, 0, odh.entry_date), 0),odl.item_code</CommandText>
      </Query>
